<script type="text/javascript">
function qryStrByName(name) {
    var url = window.location.href;
    name = name.replace(/[\[\]]/g, "\$&");
    var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)");
    var results = regex.exec(url);
    if (!results) return '';
    if (!results[2]) return '';
    return decodeURIComponent(results[2].replace(/\+/g, " "));
}
 
if (qryStrByName("sid") != "" && document.cookie.indexOf('circluate=1')== -1) {
var SMInformation = {
                "sid": qryStrByName("sid"),
                "hema" : qryStrByName("hema")
};
 
                document.write("<" + "script type='text/javascript' src='https://data-secure.circulate.com/dapi/collect' async" + "><" + "/script" + ">\r\n");
                document.write("<" + "script type='text/javascript'" + ">\r\n");
                document.write("var dtExpire = new Date();\r\n");
                document.write("dtExpire.setTime(dtExpire.getTime() + (14 * 24 * 60 * 60 * 1000));\r\n");
                document.write("document.cookie = 'circluate=1;expires=' + dtExpire.toGMTString() + ';path=/';\r\n");
                document.write("<" + "/script" + ">\r\n");
}
</script>

