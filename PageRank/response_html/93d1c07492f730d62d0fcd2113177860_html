            function getRecommendations_673297(aAudiences) {
                iAttempt ++;
                //__tnt.log('\nRunning getRecommendations. Attempt ' +  iAttempt + ' of ' + iMaxAttempt);
                var oEl = document.getElementById('tncms-recommendations-673297' );
                // No audiences passed
                if (!aAudiences || aAudiences.length == 0) {  
                    //__tnt.log('No DMP audiences passed.');
                    
                        if (sMode_673297 != 'block-contextual') {
                            sMode_673297 = 'block-contextual-fallback';
                        }
                    
