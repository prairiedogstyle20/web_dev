        <script type="text/javascript">
        Etsy.performance = Etsy.performance || {};
        Etsy.performance.firstAnimationFrameFired = -1;
        // normalize requestAnimationFrame across user agents
        !function(){
            var vendors = ['ms', 'moz', 'webkit', 'o'];
