<script>
	//console.log("DC Interstitial: Creating Dialog");
	dc_popup_ad_dialog = jQuery('.dc-ad-popup');
	dc_popup_ad_dialog.dialog({
		'dialogClass': 'wp-dialog dc-popup',
		'modal': true,
		'autoOpen': false,
		'closeOnEscape': true,
		'draggable': false,
		'resizable': false,
		'create': function(event, ui) {
			jQuery(event.target).parent().css('position', 'fixed');
		},
		resizeStop: function(event, ui) {
			var position = [(Math.floor(ui.position.left) - $(window).scrollLeft()), (Math.floor(ui.position.top) - $(window).scrollTop())];
			jQuery(event.target).parent().css('position', 'fixed');
			$(dc_popup_ad_dialog).dialog('option', 'position', position);
		}
	});
	dc_popup_ad_dialog.css('overflow', 'hidden');
	
	googletag.cmd.push( function() {
		googletag.pubads().addEventListener('slotRenderEnded', function(event) {
			console.log("DC AD: slotRenderEnded for " + event.slot.getAdUnitPath());
			if(event.isEmpty) {
				console.log("DC AD: Empty!");
				return;
			}
			switch(event.slot.getAdUnitPath()) {
				case '/10519169/dailycaller_inline1':
					$('#dailycaller_inline1').css('margin-top', '20px');
					$('#dailycaller_inline1').css('display', 'block');
					break;	
				case '/10519169/dailycaller_inline2':
					$('#dailycaller_inline2').css('display', 'block');
					break;	
				case '/10519169/dailycaller_inline3':
					$('#dailycaller_inline3').css('display', 'block');
					break;					
				case '/10519169/dailycaller_right1':
					$('#dailycaller_right1').css('min-height', '1010px');
					$('#dailycaller_right1').css('margin-bottom', '40px');
					break;	
				case '/10519169/dailycaller_right2':
					$('#dailycaller_right2').css('min-height', '600px');				
					break;	
				case '/10519169/dailycaller_right3':
					$('#dailycaller_right3').css('min-height', '600px');				
					break;				
				case '/10519169/dailycaller_interstitial':
					console.log("DC Interstitial: Opening!");
					jQuery('#dc-fbpopup-dlg').remove();
					dc_popup_ad_dialog.dialog('option', 'width', event.size[0]);
					dc_popup_ad_dialog.dialog('option', 'height', event.size[1] + 36);
					dc_popup_ad_dialog.dialog('open');
					break;
				case '/10519169/dailycaller_skin':
					console.log('DC AD: SKIN Opening!');
					var content = $('#dailycaller_skin iframe').contents();
					var imgad = content.find(".img_ad[src!=''], .img_ad img[src!='']").attr('src');
					var imghref = content.find('a')[0].href;
					$('body').css('background-image', 'url(' + imgad + ')');
					$(document).on('click','body',function(event){
						console.log(event.target);
						if (event && event.target == document.body) {
							window.open(imghref);
						}
					});			
					console.log('DC AD: SKIN ' + imgad);
					console.log('DC AD: SKIN ' + imghref);
					break;	
			}		
		});
	});
