<!-- FEB 2019 -->
<script>
(function($){
  if($('body#cart').length != 0){
    var itemToDel = 0,
    vtr = 1;
    if(window.location.href.indexOf('del_item') != -1)
      itemToDel = window.location.href.split('del_item=')[1];


    setTimeout(function(){
      $('.item-remove').each(function(){
        if(vtr == itemToDel){

          $(this).find('div').click();
        }
        vtr++;
      });
    }, 1000);

  }

  if($('body#cart').length == 0){
    $('.sqs-cart-dropzone').after('<div class="new-button"><div class="yui3-widget sqs-widget sqs-pill-shopping-cart"><div class="sqs-pill-shopping-cart-content light"></div></div></div>');
  }

  $('.sqs-add-to-cart-button').attr('onclick', 'updateCartData()');


  var cpalStyle = '.color-pallette{margin:0;padding:0;list-style: none;text-align:center}'+
  '.color-pallette li{display:inline-block;padding:0 5px;}'+
  '.color-pallette li span{display:inline-block;width:15px;height:15px;border-radius:50%;border:2px solid transparent;}'+
  '.color-pallette li span.active{border-color:#fff;box-shadow:0 0 4px #000;}'+
  'a.product small{line-height:normal;width:100%;text-align:center;display:inline-block;}',
  ptitle = [];

  if($('#productList').length != 0){
    $('.product-title').each(function(){
      ptitle.push($(this).height());
    }).each(function(){
      $(this).height(Math.max.apply(Math,ptitle));
    });
    $('#productList').before('<style>'+cpalStyle+'</style>');
    $('#productList a').each(function(){
      var plink = $(this).attr('href'),
      colorList = '',
      itemImg = '',
      tp = $(this).find('.product-overlay'),
      ti = $(this).find('.intrinsic'),
      sizeoptions = '';
      $(this).attr('href', 'javascript:void(0);').find('.intrinsic, .product-title').attr('onclick',"window.location='"+plink+"'");
      $.ajax({
        url: plink
      }).done(function(data) {
        var ctr = 0,
        ictr = 0,
        optarg = $(data).find('select[data-variant-option-name="SIZE"] option');
        $(optarg).each(function(){
          if(this.value != ''){
            if(($(optarg).length - 1) == ctr)
              sizeoptions += this.value;
            else
              sizeoptions += this.value + ' - ';
          }
          ctr++;
        });
        colorList +='<ul class="color-pallette">';
        itemImg += '<div class="wrapper prod-list-custom"><div class="sqs-gallery-design-stacked">';
        $(data).find('#productThumbnails').find('.slide img').each(function(){
          var alt = $(this).attr('alt'),
          fp = $(this).attr('data-image-focal-point').split(','),
          dataSrc = $(this).attr('data-src');
          adCss = '';
          adCss2 = 'opacity: 0; z-index: 888;display:none;';
          adCss3 = '';

          if(ictr == 0){
            adCss = 'slide-active';
            adCss2 = 'opacity: 1; z-index: 889;display:table-cell;';
            adCss3 = 'active';
          }

          itemImg += '<div class="slide-'+ictr+' slide sqs-gallery-design-stacked-slide '+adCss+'" style="'+adCss2+'"><img alt="'+alt+'"  src="'+dataSrc+'?format=300w" style="position: relative;width:100%;"></div>';
          colorList += '<li><span class="'+adCss3+'" onclick="showImg('+ictr+',this)" style="background:url(' + dataSrc + '?format=100w) no-repeat '+(fp[0] * 100)+'% '+(fp[1]*100)+'%;"></span></li>';
          ictr++;
        });
        itemImg += '</div></div>';
        colorList +='</ul><small>Available in '+sizeoptions+'</small>';

        $(ti).html(itemImg);
        $(tp).append(colorList);
        updItmImgHght();
      });
    });
  }
  $('.new-button').on('click', function(){
    updateButton();
    cartItemDetails(1);
  });

  setTimeout(function(){
    $('.new-button .sqs-pill-shopping-cart-content.light').html($('div[data-test="continue-to-cart"]').html());
    //updateButton();
  }, 1500);


  var customCSS = '.side-cart{z-index:9999;width:320px;right:-620px;position:fixed;top:0;bottom:0;margin:0;pading:0;}'
  +'.side-cart .con{display:-webkit-box;display:-webkit-flex;display:-moz-flex;display:-ms-flexbox;display:flex;-webkit-box-direction:normal;-webkit-box-orient:vertical;-webkit-flex-direction:column;-moz-flex-direction:column;-ms-flex-direction:column;flex-direction:column;-ms-touch-action:manipulation;touch-action:manipulation;height:100%;position:relative;font-size:14px}'
  +'.sqs-cart-dropzone{display:none;}'
  +'.new-button{position:fixed;top: 100px;right: 20px;width: auto;max-width: 282px;z-index: 9998;}'
  +'.side-cart.open{-webkit-animation:slide 0.5s forwards;-webkit-animation-delay:2s;animation:slide 0.5s forwards;animation-delay:0.2s;}'
  +'@-webkit-keyframes slide{100%{right:0;}}@keyframes slide{100%{right: 0;}}'
  +'.side-cart header{color: #000;width: 100%;height: 47px;background-color: #eff0f1;position: relative;}'
  +'.side-cart header h2{font-size: 20px;margin: 15px 0px;width: 100%;text-align: center;display: inline-block;}'
  +'.side-cart header h2{font-size: 20px;margin: 15px 0px;width: 100%;text-align: center;display: inline-block;}'
  +'.side-cart header a{border: none;background: none;padding: 11px 20px 3px 9px;position: absolute; top: 0px;left: 0px;}'
  +'.side-cart ul{display:block;background:white;overflow-x:hidden;overflow-y:auto;padding:0;position:relative;text-transform:none;top:0px;width:100%;z-index:1;-webkit-box-flex:1;-webkit-flex:1;-moz-box-flex:1;-moz-flex:1;-ms-flex:1;flex:1;margin:0;}'
  +'.side-cart.loading::after{content:"";width:100%;height:100%;background:rgba(255,255,255,0.8) url(https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.9.0/ajax-loader.gif) center center no-repeat;top:0;left:0;position:absolute;z-index:999;}'
  +'.side-cart .cart-footer{text-align:right;background-color:#eff0f1;width:100%;z-index:1;-webkit-align-self:flex-end;-moz-align-self:flex-end;-ms-flex-item-align:end;align-self:flex-end;font-size:13px}'
  +'.side-cart ul .sc-prod-dets{width:62%;float:right;margin:0;padding:0 4%;font-size:14px;line-height:normal;}'
  +'.side-cart ul .sc-prod-dets strong{margin-bottom:10px;display:inline-block;}'
  +'.side-cart ul .sc-img{float:left;width:30%;}'
  +'.side-cart ul .sc-img img{height:auto;display:inline-block;width:100%;}'
  +'.bottom-content{text-align:right;padding:0.5em 1em 2.5em;}'
  +'.side-cart ul li{position:relative;width:100%;display:inline-block;list-style:none;padding:0.5em;border-bottom:1px solid #ccc;}'
  +'.dark-trans-bg{width:100vw;height:100vh;background:rgba(0,0,0,0.5);z-index:9998;position:fixed;}'
  +'.sqs-cart-dropzone::after{content:"";width:100%;height:100%;left:0;top:0;}'
  +'.close-btn{position:absolute;top:50%;right:20px;}'
  +'.prod-list-custom .sqs-gallery-design-stacked-slide{position:relative;}'
  +'.prod-list-custom .sqs-gallery-design-stacked{display:table;width:100%;height:100%;}'
  +'.prod-list-custom .sqs-gallery-design-stacked-slide{display:table-cell;vertical-align:middle;width:100%;}'
  +'.prod-list-custom .sqs-gallery-design-stacked-slide img{top:auto!important;right:auto!important;bottom:auto!important;left:auto!important;position:relative!important;width:100%!important;max-width:100%!important;height:auto!important}'
  +'.btn-checkout{padding:0.5em 1em;background-color:#f90;color:#000;font-weight:bold;display:inline-block;width:100%;text-align:center;text-transform:uppercase;}';

  $('body').prepend('<style>'+customCSS+'</style>');

})(jQuery);

function cartItemDetails(a){
  (function($){
    if($('.con-side-cart').length != 0)
      $('.con-side-cart').remove();

    $.ajax({
      url: "/cart",
      context: document.body
    }).done(function(data) {
      var cart_json = $.parseJSON($(data).find('script[type="application/json"]').html()),
      htmlContent = '<div class="con-side-cart"><div onclick="closeCart();" style="display:none;" class="dark-trans-bg">&nbsp;</div><div class="side-cart"><div class="con"><header class="cart-header"><h2>Your Cart</h2><a href="javascript:void(0)" onclick="closeCart()"><svg width="25" height="25" xmlns="http://www.w3.org/2000/svg" data-name="Layer 1" viewBox="0 0 156.4 156.4"><defs><style>.cls-1{fill:none;stroke:#000;stroke-linecap:round;stroke-linejoin:round;stroke-width:8px;}</style></defs><title>Cart Close Arrow</title><polyline class="cls-1" points="64.7 36.2 101.7 78.2 64.7 120.3"></polyline></svg></a></header><ul>';

      if(cart_json.cart.items.length != 0){
        cart_json.cart.items.forEach(function(currentValue, index, arr){
          htmlContent += listCartItems(currentValue,index);
        });
      }else
        htmlContent += '<li style="text-align:center">Cart Empty</li>';

      htmlContent += '</ul><div class="cart-footer"><div class="bottom-content">sub total: <strong>$'+cart_json.cart.subtotal.decimalValue+'</strong><br><a href="/checkout" class="btn-checkout">Checkout</a></div></div></div></div></div>';
      $('#siteWrapper').prepend(htmlContent);

      if(a == 1){
        $('.side-cart').addClass('open');
        $('.dark-trans-bg').show();
      }
    });
  })(jQuery);
}
function deleteItem(a,b){
  (function($){
    $('.side-cart').addClass('loading');
    targItm = a + 1;
    $('.load-iframe').remove();
    $('body').append('<iframe class="load-iframe" style="display:none;" src="/cart/?del_item='+targItm+'"></iframe>');
    $(b).closest('li').fadeOut();
    setTimeout(function(){
      getUpdatedCart();
      updateButton();
      $('.side-cart').removeClass('loading');
    }, 4000);
  })(jQuery);
}

function getUpdatedCart(){
  (function($){
    $.ajax({
      url: "/cart",
      context: document.body
    }).done(function(data) {
      var cart_json = $.parseJSON($(data).find('script[type="application/json"]').html()),
      htmlContent = '';
      if(cart_json.cart.items.length != 0){
        cart_json.cart.items.forEach(function(currentValue, index, arr){
          htmlContent += listCartItems(currentValue,index);
        });
      }else{
        htmlContent += '<li style="text-align:center">Cart Empty</li>';
      }
      $('.side-cart ul').html(htmlContent);
      $('.cart-footer .bottom-content strong').html(cart_json.cart.subtotal.decimalValue);
    });
  })(jQuery);
}

function updateButton(a,b){
  (function($){
    $.ajax({
      url: "/cart",
      context: document.body
    }).done(function(data) {
      var cart_json = $.parseJSON($(data).find('script[type="application/json"]').html()),
      cartLength = 0;
      cart_json.cart.items.forEach(function(cur){
        cartLength += cur.quantity;
      });
      $('.new-button .sqs-pill-shopping-cart-content.light .total-quantity').html(cartLength);
      $('.new-button .sqs-pill-shopping-cart-content.light .subtotal .price').html('$'+cart_json.cart.subtotal.decimalValue);
    });
  })(jQuery);
}
function closeCart(){
  (function($){
    $('.side-cart').removeClass('open');
    $('.dark-trans-bg').fadeOut();
  })(jQuery);
}
function updateCartData(){
  (function($){
    setTimeout(function(){
      updateButton();
    }, 2000);
  })(jQuery);
}
function listCartItems(currentValue,index){
  var o1 = currentValue.variantOptions[1],
  o0 = currentValue.variantOptions[0],
  o2 = currentValue.variantOptions[2],
  htmlContent;
  if(o1)
    o1 = o1.value;
  if(o2)
    o2 = 'Material:' + o2.value;
  else
    o2 = "";
  if(o0)
    o0 = o0.value;
  htmlContent = '<li><a class="close-btn" href="javascript:void(0);" onclick="deleteItem('+index+',this)">X</a><div class="sc-img"><img src="'+currentValue.image.url+'?format=100w" /></div> <p class="sc-prod-dets"><strong>'+currentValue.productName+'</strong><br>Price: $' +currentValue.unitPrice.decimalValue+'<br>'+o0+' - '+o1+'<br>Qty: '+currentValue.quantity+'<br>'+o2+'</p></li>';

  return htmlContent;
}

function showImg(a,b){
  (function($){
    var targ = $(b).closest('a.product');
    $(targ).find('.color-pallette span').each(function(){
      $(this).removeClass('active');
    });
    $(targ).find('.slide').each(function(){
      $(this).css({'opacity':0,'z-index':888}).hide();
    });
    $(targ).find('.slide-'+a).css({'opacity':1,'z-index':889,'display':'table-cell'});
    $(b).addClass('active');
  })(jQuery);
}
function updItmImgHght(){
  (function($){
    $('.prod-list-custom .sqs-gallery-design-stacked-slide').each(function(){
      $(this).height($(this).closest('.prod-list-custom').outerHeight());
      $(this).width($(this).closest('.prod-list-custom').outerWidth());
    });
  })(jQuery);
}

