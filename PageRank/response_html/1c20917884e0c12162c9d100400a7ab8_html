					<td><script language="JavaScript" type="text/JavaScript">
var nav4 = window.Event ? true : false;
function verif_enter(evt,num, form_busqueda)
{	
	document.form_busqueda.action = "resultados.php";

	var key = nav4 ? evt.which : evt.keyCode;	
	
	if (key==13)
	{
	Validar_palabra(form_busqueda);
	}
}

function Validar_palabra(form_busqueda)
{
	document.form_busqueda.action = "resultados.php";
	
	if (document.form_busqueda.palabra_busca.value == "")
	{ alert("Por favor ingrese la palabra a buscar"); document.form_busqueda.palabra_busca.focus(); return; }
	if (document.form_busqueda.palabra_busca.value.length<3){
	alert("La palabra a buscar debe tener mínimo 3 caracteres"); document.form_busqueda.palabra_busca.focus(); return; }

	document.form_busqueda.submit();
}
</script>
