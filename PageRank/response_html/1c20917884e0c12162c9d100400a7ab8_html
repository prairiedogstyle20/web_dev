//PARA EL TAMAÑO
var valor_actual=15;
function tamano(valor)
{
	var texto = document.getElementById('div_texto');
	valor     = parseFloat(valor);
	valor_actual = valor_actual+valor;

	if ((valor_actual>=10)&&(valor_actual<=30))
	{
		texto.style.fontSize = valor_actual; 
	}else{
		//alert(valor_actual);
		if (valor_actual==32)
		{
			valor_actual = 30;
		}else{
			valor_actual = 10;
		}
	}
	//alert(texto.style.fontSize);
}

function denunciar(id_comentario)
{
	if (confirm('¿Desea denunciar este comentario?')){
	
		document.formDenunciar.id_comentario.value=id_comentario;
		document.formDenunciar.submit();
	}
}

	function cuenta_pdf(id_sem)
	{
		ajaxa=nuevoAjax();
		ajaxa.open("GET", "gcw_includes/cuenta_pdf.php?id_sem="+id_sem,true);
		ajaxa.onreadystatechange=function() 
		{
				//cont_enc.innerHTML = ajax.responseText
		}
		ajaxa.send(null)	
	}
</script>
