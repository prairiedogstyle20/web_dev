			var url = jQuery(this).attr("shref") || 'https://www.redstate.com/alexparker/2019/01/28/nancy-pelosi-approval-rating-decrease-government-shutdown/';
			var title = jQuery(this).data("title") || 'Whether Or Not Trump Accepts Dem Budget Deal, Wall Construction Will Almost Certainly Move Forward';
