<script async>

var url = location.href;

if(document.getElementById("fb_cnt_text") != null || document.getElementById("fb_cnt_text_top") != null || document.getElementById("fb-btn") != null){

	$.ajax({
		type: 'GET',
		url: 'https://graph.facebook.com/?id=' + encodeURI(location.href) + '',
		dataType: 'jsonp',
		timeout: 30000,
        success: function(json){
			if('share' in json){
				if(document.getElementById("fb_cnt_text") != null){
					$("#fb_cnt_text").append(json.share.share_count);
				}
				if(document.getElementById("fb_cnt_text_top") != null){
					$("#fb_cnt_text_top").append(json.share.share_count);
				}
				<!-- ãªãã¥ã¼ã¢ã« -->
				if(document.getElementById("fb-btn") != null){
					if(json.share.share_count != 0){
						$("#fb-btn").append(json.share.share_count);
					}
				}
			}else{
				if(document.getElementById("fb_cnt_text") != null){
					$("#fb_cnt_text").append('0');
				}
				if(document.getElementById("fb_cnt_text_top") != null){
					$("#fb_cnt_text_top").append('0');
				}

			}
     	},
     	error: function () {
        	console.log("fb-error");
        }
	});
	
	if(document.getElementById("fb-link_b") != null){
		document.getElementById("fb-link_b").href="http://www.facebook.com/sharer.php?u="+encodeURI(location.href)+"&amp;t="+encodeURIComponent(document.title);
	}
	if(document.getElementById("fb-link") != null){
		document.getElementById("fb-link").href="http://www.facebook.com/sharer.php?u="+encodeURI(location.href)+"&amp;t="+encodeURIComponent(document.title);
	}
	<!-- ãªãã¥ã¼ã¢ã« -->
	if(document.getElementById("fb-btn") != null){
		document.getElementById("fb-btn").href="http://www.facebook.com/sharer.php?u="+encodeURI(location.href)+"&amp;t="+encodeURIComponent(document.title);
	}
	if(document.getElementById("fb-like") != null){
		document.getElementById("fb-like").href="http://www.facebook.com/sharer.php?u="+encodeURI(location.href)+"&amp;t="+encodeURIComponent(document.title);
	}
}





</script>
