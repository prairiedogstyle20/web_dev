            president = "obama" === "obama" ? [ "/extension/rasmussenreports/design/plain/images/trump.png", "/extension/rasmussenreports/design/plain/images/obama-test.png" ]
                      : "trump" === "obama" ? "/extension/rasmussenreports/design/plain/images/obama-test.png" 
                      : "trump" === "trump" ? "/extension/rasmussenreports/design/plain/images/trump.png" 
                      : "/extension/rasmussenreports/design/plain/images/obama-test.png"; 
        
        //options.chart.marginLeft = 75;
        options.chart.events = 
        {  
            load: function () 
            {

                if ( Array.isArray( president ) ) {

                  // Mr. Presidents
                  this.renderer.image(president[0], 0, 50, 43, 89).add();
                  this.renderer.image(president[1], 0, 139, 43, 89).add();

                } else {

                // Mr. President
                this.renderer.image(president, 0, 50, 43, 89).add();

                }

                // Mr. President's seal
                this.renderer.image('/extension/rasmussenreports/design/plain/images/Seal_Of_The_President_Of_The_United_States_Of_America.jpg', 237, 130, 175, 175).add();
                
                if (options.chart.cApprovalIndex)
                {
                    this.renderer.text(options.chart.cApprovalIndex, 370, 149)
                    .css(
                    {
                        color: '#000',
                        fontSize: '16px',
                        fontWeight: 'bold'
                    })
                    .add();
                }
            }
        };
        options.xAxis.labels.formatter = function()
        {
            if (this.isFirst || this.isLast || ($.inArray(this.value, this.axis.options.tickDates) != -1) )
            {
                return this.value;
            }
        };
        options.yAxis.labels.formatter = function() 
        {
            return this.value + '%';
        };

        
                    
                options.tooltip = {
                    formatter: function () {

                        var dateArrayDMY = this.x.split( '-' );
                        var currentYear = Number( dateArrayDMY[ 2 ] );
                        var comparisonYears = Number( options.comparison_years );
                        var newYear =  currentYear - comparisonYears;
                        var newYearString = newYear < 10
                            ? '0' + String( newYear )
                            : String( newYear );
                        var newDateString = String( dateArrayDMY[0] )
                            + '-'
                            + String( dateArrayDMY[1] )
                            + '-'
                            + newYearString;
                        var myTooltip = '';

                        myTooltip += '<b>' + this.x + '</b>';
                        myTooltip += '<br/>' + this.points[0].series.name + ': ' +
                            ( this.points[0].y === '0'
                                ? 'No Polling Data<br/>'
                                : this.points[0].y + '%' + '<br/>' );
                        myTooltip += '<b>' + newDateString + '</b>';

                        if ( this.points.length > 1 ) {

                          myTooltip += '<br/>' + this.points[1].series.name + ': ' +
                              ( this.points[1].y === '0'
                                  ? 'No Polling Data<br/>'
                                  : this.points[1].y + '%' + '<br/>' );

                        } else {
                          
                          myTooltip += '<br/>No polling data.<br/>';
                        
                        }

                        return myTooltip;

                    },
                    shared: true
                };
            
                

        options.plotOptions.series = { "connectNulls": false };

        presApprovalGraphs[ 'container_total_approve_2017-01-20_2020-01-31_obama' ] = new Highcharts.Chart( options );	
	});

</script>
