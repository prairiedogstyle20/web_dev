<script>
        jQuery(document).ready(function($){
            // Reset the meter if the data store is too old.
__tnt.metering.init.push(function(){
    __tnt.log("checking reset date");

    var expireDate = new Date(__tnt.metering.config.date);
    var lastDate = new Date(__tnt.recent.date.get());
    
    expireDate = expireDate.getTime();
    lastDate = lastDate.getTime();
    
    if(Number(lastDate)<Number(expireDate)){
        __tnt.log("resetting restrictions");
        var nextDate = new Date(expireDate+(__tnt.metering.config.days * 86400000));

        __tnt.recent.count.set(0);
        __tnt.recent.date.set(nextDate.getTime());

        __tnt.log(nextDate);
    }
});

// Set the 'read' variable if the current asset is in the recent assets list
__tnt.metering.init.push(function(){
    __tnt.log("checking whether user has read this before");
    
    var recent = __tnt.recent.assets.get();
    var uuid = __tnt.metering.config.uuid;
    
    var check = false;
    var i = 0;
    while(!check && i < recent.length){
        if(recent[i]==uuid){
            __tnt.log("found asset in recent");
            var check = true;
        }
        i++;
    }    
    
    if(check){
        __tnt.log("user has read this already");
        __tnt.metering.config.read = true;
    }else{
        __tnt.log("not read before");
    }
});
// if count is less than registration limit return true
__tnt.metering.access.c.push(function(){
    if(__tnt.recent.count.get()<__tnt.metering.config.reg_limit){
        return true;
    }
});
// check if the user is known and then test against the subscription limit
__tnt.metering.access.c.push(function(){
    if((__tnt.user.loggedIn)&&(__tnt.recent.count.get()<__tnt.metering.config.sub_limit)){
        return true;
    }
});
// if the user has read this asset before they can read the asset
__tnt.metering.access.c.push(function(){
    if(__tnt.metering.config.read){
        __tnt.log("user has previously viewed asset");
        return true;
    }
});
__tnt.metering.access.e.push(function(){
    __tnt.log("executing granted scripts");

    // Analytics logging goes here
    __tnt.metering.gtm(false);

    });
// increment counter on new assets
__tnt.metering.access.e.push(function(){
    // this will also need to check if the user has previously read the asset (by checking the swtich set by the init script.)
    if(!__tnt.metering.config.read){
        __tnt.log("incrementing counter");

        var count = __tnt.recent.count.get();
        count++;
        __tnt.recent.count.set(count);

        // fire an event that will allow a one time only message for a given count
        $(window).trigger('__tnt.metering.new');
    }else{
        $(window).trigger('__tnt.metering.read');
    }
});
// add unread assets to the recent asset list
__tnt.metering.access.e.push(function(){
    if(!__tnt.metering.config.read){
        __tnt.recent.assets.add(__tnt.metering.config.uuid);
    }
});
__tnt.metering.access.e.push(function(){$('.subscriber-hide').show();
});
__tnt.metering.access.e.push(function(){
    var count = __tnt.recent.count.get();
    var limit = __tnt.metering.config.sub_limit;
    var remaining = (limit - count);

    if(count > limit){count = limit;}
    if(remaining < 0){remaining = 0;}

    var messageClass = 'access';
    if(__tnt.metering.config.referred){ messageClass = 'referred'; }
    if(__tnt.metering.config.read){ messageClass = 'read'; }

    // initialize by finding generic message for this count
    message = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+']').first();

    // find messages at this count specifically for users who are not logged in
    if(!__tnt.user.loggedIn){
        var messageCheck = {length:0};
            messageCheck = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+'][data-registered=false]').first();
        if(messageCheck.length && message.attr('data-registered') == "false"){ message = messageCheck; }
    }

    // find messages at this count specifically for users who are logged in
    if(__tnt.user.loggedIn){
        var messageCheck = {length:0};
            messageCheck = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+'][data-registered=true]').first();
        if(messageCheck.length && message.attr('data-registered') == "true"){ message = messageCheck; }
    }

    // find messages specifically for display after the presentation of the wall
    try{
        if((typeof(message)=="undefined" || !message.length) && window.sessionStorage){
            if(sessionStorage.getItem('__tnt.metering.wall.viewed')=="1"){
                message = $('#template-metering').find('.template.meter.'+messageClass+'[data-registered=true][data-after-wall=true]').first();
            }
        }
    }catch(e){}

    // find any generic message that doesn't require a count or an after wall state
    if(typeof(message)=="undefined" || !message.length){
        message = $('#template-metering').find('.template.meter.'+messageClass+':not([data-count]):not([data-after-wall])').first();
    }

    if(typeof(message)!="undefined" && message.length){
        if(!$('#notification-area').length){
            $('#asset-content').find('.asset-body').append('<div id="notification-area"></div>');
        }
        __tnt.template(
            message[0],
            $('#notification-area')[0],
            true,
            [{
                'element': '.count',
                'html': String(count)
            },{
                'element': '.total',
                'html': String(limit)
            },{
                'element': '.remaining',
                'html': String(remaining)
            }],
            function(el){
                if(__tnt.user.authToken){
                    $(el).find('.logged-in').show();
                }else{
                    $(el).find('.not-logged-in').show();
                }
                $(el).show();
            }
        );
    }

    try{
        if(window.sessionStorage){
            sessionStorage.removeItem('__tnt.metering.wall.viewed');
        }
    }catch(e){}

});
__tnt.metering.access.e.push(function(){

});
__tnt.metering.access.d.push(function(){
    __tnt.log("executing denied scripts");
    // Analytics logging goes here 
    __tnt.metering.gtm(true);

    //DMP logging
    if(window._cc){
        __tnt.log("Logging DMP data")
        var paywallDmpData = 'Paywall : Viewed Paywall';
        // Call to cms.dmp.js_behavior_action is here
        
    }
});
__tnt.metering.access.d.push(function(){
    var asset = $('#asset-content').find('.asset-body');
    if(asset.find('.subscriber-only.encrypted-content').length){
        if(typeof(tncms)!="undefined"){
            tncms.unscramble = function(){};
        }
        
        asset.find('.share-container, .inline-asset, .subscriber-only.encrypted-content').remove();
    }

    $('.subscriber-hide, .subscriber-only').addClass('hide');
    $('.subscriber-denied').removeClass('hide');
    
    $('[data-subscription-required-remove]').remove();
    $('[data-subscription-required-class]').each(function(){
        $(this).attr('class',$(this).attr('data-subscription-required-class'));
    });
});
__tnt.metering.access.d.push(function(){
    var lastPreview = $("#asset-content").find(".asset-body").find(".subscriber-preview").last();
    var bgColor = '#ffffff'.replace('#','');
    var rValue = parseInt(bgColor.slice(0,2),16);
    var gValue = parseInt(bgColor.slice(2,4),16);
    var bValue = parseInt(bgColor.slice(4,6),16);
    var bgColorRGB = rValue+','+gValue+','+bValue;
    
    $(lastPreview)
        .css({'position':'relative'})
        .append('<div class="redacted-overlay" style="position:absolute;bottom:0;left:0;width:100%;height:'+$(lastPreview).height()+'px;background: linear-gradient(to bottom, rgba('+bgColorRGB+',0), rgba('+bgColorRGB+',255) 80%);"></div>')
        .after('<div class="subscription-required"></div>');
    
    var count = __tnt.recent.count.get();
    var limit = __tnt.metering.config.sub_limit;
    var remaining = (limit - count);
    
    if(count > limit){count = limit;}
    if(remaining < 0){remaining = 0;}
    
    var messageClass = 'subscription';
    if((count>=__tnt.metering.config.reg_limit)&&(count<__tnt.metering.config.sub_limit)){
        messageClass = 'registration';
    }
    
    var message = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+'][data-registered=false]').first();
    if(__tnt.user.loggedIn){ message = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+'][data-registered=true]').first(); }
    
    if(!message.length){ message = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+']').first(); }
    if(!message.length){ message = $('#template-metering').find('.template.meter.'+messageClass+':not([data-count])').first(); }

    if(message.length){
        var insertionPoint = $('#asset-content').find('.asset-body').find('.subscription-required');
        if(!insertionPoint.length){
            insertionPoint = $("#eedition-subscription");
        }
        if(!insertionPoint.length){
            if($('#asset-content').length){ if(!$('#asset-content').find('.asset-body').length){ $('#asset-content').prepend('<div class="asset-body"></div>'); } }
            $('#asset-content').find('.asset-body').prepend('<div class="subscriber-premium"><div class="subscription-required"></div></div>');
            insertionPoint = $('#asset-content').find('.asset-body').find('.subscription-required');
        }
        
        __tnt.template(
            message[0],
            insertionPoint[0],
            true,
            [{
                'element': '.count',
                'html': String(count)
            },{
                'element': '.total',
                'html': String(limit)
            },{
                'element': '.remaining',
                'html': String(remaining)
            }],
            function(element){
                $(element).find("a[data-toggle=collapse]").each(function(){
                    if($(this).attr("href")){
                        $(this).on('click',function(){
                            var href = $(this).attr("href");

                            $(href).collapse('toggle');
                        });
                    }
                });
                
                if(!(__tnt.metering.compatibility().status == 'FAIL: no metering in eedition')){
                    if($("#template-metering").find('.metering-modal').length){
                        $("#template-metering").find('.metering-modal').modal();
                        
                        $('#template-metering').find('.metering-modal').on('shown.bs.modal',function(){
                            $('.subscription-services').trigger('tncms-track-services-displayed');
                        });
                    }
                }
            }
        );
    }
    
    if(__tnt.user.authToken){
        $('#asset-content').find('.subscriber-premium').find('.subscription-required').find('.logged-in').show();
        $('#template-metering').find('.metering-modal').find('.logged-in').show();
        $('#eedition-subscription').find('.logged-in').show();
    }else{
        $('#asset-content').find('.subscriber-premium').find('.subscription-required').find('.not-logged-in').show();
        $('#template-metering').find('.metering-modal').find('.not-logged-in').show();
        $('#eedition-subscription').find('.not-logged-in').show();
    }
    
    if(__tnt.user.services){
        $('#asset-content').find('.subscriber-premium').find('.subscription-required').find('.has-services').show();
        $('#template-metering').find('.metering-modal').find('.has-services').show();
        $('#eedition-subscription').find('.has-services').show();
    }else{
        $('#asset-content').find('.subscriber-premium').find('.subscription-required').find('.no-services').show();
        $('#template-metering').find('.metering-modal').find('.no-services').show();
        $('#eedition-subscription').find('.no-services').show();
    }
        
    try{
        if(window.sessionStorage){
            sessionStorage.setItem('__tnt.metering.wall.viewed',1);
        }
    }catch(e){}
    
    //GTM
    $('.subscription-services').trigger('tncms-track-services-displayed');

});    
__tnt.metering.config.date = 1548997200000;

                __tnt.metering.config.sub_limit = 3;
                __tnt.metering.config.reg_limit = 3;

__tnt.metering.compatibility = function(){
    var compatible = true;
    var status = "";
    
    /* this checks for compatibility with DOM storage */
    try{
        window.sessionStorage.setItem('__tnt.compatibility',true);
    }catch(e){
        compatible = false;
        status = "FAIL: setItem unsupported";
    };

    try{
        window.sessionStorage.getItem('__tnt.compatibility');
    }catch(e){
        compatible = false;
        status = "FAIL: getItem unsupported";
    }
    return {'check':compatible,'status':status};
};
var bloxHasSubscription = false;
            var bloxServiceValidated = $.cookie('tncms-service-validated');

            /* if service not whitelisted check user service */
            if( bloxServiceValidated != 1 ){
                var bloxServiceIDs = [];
                
                    bloxServiceIDs.push(2769);
                
                    bloxServiceIDs.push(2768);
                
                    bloxServiceIDs.push(2801);
                
                    bloxServiceIDs.push(2784);
                
                if (__tnt.user.services){
                    var bloxUserServiceIDs = __tnt.user.services.replace('%2C',',').split(',');
                    $.each(bloxUserServiceIDs, function(index, value){
                        var serviceID = parseInt(value);
                        if( serviceID != 0 ){
                            if( $.inArray(serviceID, bloxServiceIDs) > -1 ){
                                bloxHasSubscription = true;
                                return false;
                            }
                        }else{
                            bloxHasSubscription = true;
                            return false;
                        }
                    });
                }
            /* service has been whitelisted */
            } else {
                bloxHasSubscription = true;
            }

            /* check for whitelists once per day */
            if( !bloxHasSubscription && !bloxServiceValidated && typeof userWhiteList != 'undefined' && $.isArray(userWhiteList) ) {
                $.getJSON("https://www.mdjonline.com/_services/v1/client_ip_info/",function(data){
                    /* create a range */
                    var dataRange = data.ip.split('.');
                    dataRange = dataRange[0]+'.'+dataRange[1]+'.'+dataRange[2];
                    if( jQuery.inArray(data.ip, userWhiteList) > -1 || jQuery.inArray(dataRange, userWhiteList) > -1 ){
                        /* white list machine */
                        $.cookie('tncms-service-validated', 1, { path:'/' });

                        bloxHasSubscription = true;
                    } else {
                        $.cookie('tncms-service-validated', 0, { path:'/', expires:1 });

                        __tnt.metering.check();
                    }
                });

            /* no access */
            }else if( !bloxHasSubscription ){
                __tnt.metering.check();
            }
            if(bloxHasSubscription){
                $('.subscriber-only.encrypted-content').each(function(){
                    $(this).html(tncms.unscramble($(this).text()));
                    $(this).show();
                });
                $('.subscriber-hide').show();
            }

            // show main content div
            $('#site-container').show();

            // hide loading spinner
            $('.subscriber-load').hide();
        });

    </script>

