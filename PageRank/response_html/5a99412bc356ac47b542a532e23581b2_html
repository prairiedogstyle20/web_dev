        $(document).ready(function() {
            // Adjust for super small regions
            var thisBlock = "#weather-container-603934 ";
            var containerWidth = $(thisBlock).width();
            var hideThese = $("#weather-container-603934 .header-weather-cols .col-sm-3.hidden-xs, #weather-container-603934 .header-weather-cols .col-sm-4.col-xs-4, #weather-container-603934 .header-weather-cols .col-sm-4.col-xs-4, #weather-container-603934 div.panel-heading > div > div > div.weather-index-wind.col-sm-3.hidden-xs.opacity-change,  #weather-container-603934 div.panel-heading > div > div > div.weather-index-precip.col-xs-2.opacity-change");
            var panelTime = $("#weather-container-603934 .weather-panel-time-details");
            var addCol = $("#weather-container-603934 div.row.header-weather-cols > .col-sm-2.col-xs-3, #weather-container-603934 .panel-heading .weather-index-time");
            if(containerWidth < 112){
                $(hideThese).hide();
                $(thisBlock + "div.panel-heading .col-sm-3").removeClass("col-sm-3").addClass("col-sm-5");
                $(panelTime).removeClass("col-sm-4").addClass("col-sm-12").css("border","none");
                $(thisBlock + ".weather-panel-details .col-sm-6").removeClass("col-sm-6").addClass("col-sm-12");
            } else if(containerWidth < 166){
                $(hideThese).hide();
                $(thisBlock + ".weather-index-time").removeClass("col-sm-2").addClass("col-sm-6");
                $(addCol).addClass("col-lg-3");
                $(thisBlock + ".panel-heading > div > div > .col-sm-3.col-xs-5").addClass("col-lg-5");
                $(panelTime).addClass("col-lg-12").css("border","none");
            };
        });
