<script type="text/javascript">

$("#newsletterform").find("form").submit(function(e){
                e.preventDefault();
            });


$("#submit").click(function (event) {
    event.preventDefault();
    var r = validateEmail();
	var formData = $("#newsletterform").find("form").serialize();
    var checkCaptcha = true;
    var captchaValid = false;
    if(typeof grecaptcha === 'undefined'){
        checkCaptcha = false;
    }
    if(checkCaptcha){
        var response = grecaptcha.getResponse();
        if(response.length == 0){
            captchaValid = false;
            //reCaptcha not verified
        }else{
            captchaValid = true;
        }
    }
        if(!checkCaptcha || (checkCaptcha && captchaValid)){
            if(r){
                 $.ajax({
                             type: 'POST',
                             url: 'https://cl.exct.net/subscribe.aspx' +'?lid='+ '20291287' ,
                             cache: false,
                             async : false,
                             data:  formData + "&SubAction=sub_add_update"+
                             "&MID=" + '84403'+
                             "&thx=https://www.nielsen.com/etc/designs/nielsenglobal/newsletterforms-exacttarget/success.js?callback=processData"+
                             "&err=https://www.nielsen.com/etc/designs/nielsenglobal/newsletterforms-exacttarget/failure.js?callback=processData", // Giving the query parms in the jquery ajax syntax takes care of the encoding
                             dataType: 'jsonp',
                             crossDomain : true,
                             beforeSend : function(xhr) {
                             xhr.setRequestHeader('Accept', "text/html; charset=utf-8");
                             },
                             complete: function (xhr, status) {
                             // $('html, body').animate({scrollTop:0}, 'slow');
                             }    
                        }); 
                }
		}else{
        alert("Please validate captcha");
        }
});
    function processData(json) {
        if(json.exacttargetsubmit=="success"){
             $('#modal').modal();
    	     $('#modal-newsletter-lead-gen').modal();
        }
         if(json.exacttargetsubmit=="failure"){
             $('#modal').modal();
    	     $('#modal-newsletter-error-lead-gen').modal();
             send_et_failure_email_notification();
        }
    }

     function  send_et_failure_email_notification(){
         /* Get data to send on email */
        var pagePath = window.location.href;
	    	var indtemp = pagePath.lastIndexOf("?");
	    	if(indtemp!=-1)
	    	       {
	    	              pagePath = pagePath.substring(0,indtemp);
	    	       } 
        var formFields = "";
        var formFieldSeparator = "\n\n";
        var formData = $("#newsletterform").find("form").serializeArray();
        for(var i=0;i<formData.length;i++){
            if(formFields==''){
                formFields = formData[i].name+" :  "+formData[i].value;
            }else{
                formFields = formFields+formFieldSeparator+formData[i].name+" :  "+formData[i].value;
            }

        }
         
         /* Get data to send on email */
           $.ajax({ 
               type: 'GET', 
               async:true,      
               url:'/bin/ExactTargetEmailNotification',
               data:{form_page_path:pagePath,
                     form_fields:formFields,
                     form_mail_browsername:browser,
                     form_mail_browserversion:version
                     },    
               success: function(msg){            	   
                   //do nothing
               }      
           });  
       }

    function validateEmail(){
        var emailAlertValue = 'Email format is incorrect';
        var r= true;
        // check email has a blank space
	                  var emailVal =  $("input[name='Email Address']").val();
	                 if (!(/^\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*$/.test(emailVal))){
	                      alert(emailAlertValue);
	                      r = false;
	                      $("input[name='Email Address']").focus();
	                  }
        return r;

    }
</script>
