<script type="text/javascript">
$(function(){
    $("[data-toggle=popover]").popover({
        html : true,
        content: function() {
          var content = $(this).attr("data-popover-content");
          return $(content).children(".popover-body").html();
        },
        title: function() {
          var title = $(this).attr("data-popover-content");
          return $(title).children(".popover-heading").html();
        },
        async:true
    });

   $(document).on('click',"#email-submit-email1",function () {
        var senderName =  $('.popover-content').find('#sender-name').val();
        var toEmail =  $('.popover-content').find('#to-address').val();
        if(toEmail.trim()!='' && senderName.trim()!='' ){
            send_email_notification(senderName,toEmail);
            $('[data-toggle="popover"]').popover('hide');
       }
   });

    $('body').on('click', function (e) {
        //did not click a popover toggle, or icon in popover toggle, or popover
        if ($(e.target).data('toggle') !== 'popover'
            && $(e.target).parents('[data-toggle="popover"]').length === 0
            && $(e.target).parents('.popover.in').length === 0) { 
            $('[data-toggle="popover"]').popover('hide');
        }
    });
    $( window ).resize(function() {
          $('[data-toggle="popover"]').popover('hide');
    });

    $('body').on('hidden.bs.popover', function (e) {
        if (typeof($(e.target).data("bs.popover")) != "undefined"){
        	$(e.target).data("bs.popover").inState.click = false;
        }
    });

    $(document).ready(function() {
      $(window).keydown(function(event){
        if(event.keyCode == 13) {
          event.preventDefault();
          return false;
        }
      });
	});
});

function  send_email_notification(senderName,toEmail){
         /* Get data to send on email */
        var pagePath = window.location.href;
        var indtemp = pagePath.lastIndexOf("?");
        if(indtemp!=-1)
        {
            pagePath = pagePath.substring(0,indtemp);
        } 
        var formFields = "";
        var formFieldSeparator = "\n\n";
    	var from = "From";
    	var emailLine1 ="Nielsen data and insights delivered straight to your inbox:";
    	var emailLine2 = pagePath;
    	var emailLine3 ="You're receiving this email because someone thought you'd like our data and research. This is a one-time email. Subscribe to our newsletter for more Nielsen news each week:";
    	var emailLine4 ="http://www.nielsen.com/us/en/newsletter.html";
 
         /* Get data to send on email */
           $.ajax({ 
               type: 'GET', 
               async:true,      
               url:'/bin/SendEmailShareMessage',
               data:{to_address:toEmail,
                     subject:from + " " + senderName + ": "+"Nearly 47 Million Viewers Watch President Trump’s State of the Union Address",
                     body:emailLine1+formFieldSeparator+"Nearly 47 Million Viewers Watch President Trump’s State of the Union Address"+"\n"+emailLine2+formFieldSeparator+emailLine3+"\n"+emailLine4
                     },    
               success: function(msg){            	   
                   //do nothing
               }      
           });  
       }
$('#email1').popover().click(function(e) { e.preventDefault();});
</script> 

