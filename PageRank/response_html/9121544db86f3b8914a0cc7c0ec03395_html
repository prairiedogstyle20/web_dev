    <script type="text/javascript">
        function setSidebarCallout(){
            var dir =  $('body').css("direction");
			var sidebar = $('.sidebarcallout');
			var sidebarContent = $(sidebar[0]).find('div.sidebarimgtext');
			if(sidebarContent.length>0){
				adjustSideBarCallout(sidebar,dir);
			}else{
				$('.span3.sidesection').hide();
			}
		}
    $( document ).ready(function() {
        setSidebarCallout();
         $( window ).resize(function() {
             setSidebarCallout();
         });
     });
function adjustSideBarCallout(sidebar,dir){
    var maincol = $('#mainsection');
    var maincolHt = $(maincol).height()/2;
    var maincolWt = $(maincol).width();
    

    $( sidebar ).offset({ top:$('#mainsection').offset().top+maincolHt});
    //$( sidebar ).offset({ left:$('#mainsection').offset().left+maincolWt});
    if(dir=='rtl'){
        $( sidebar ).parent().offset({ left:$('#mainsection').offset().left- $( sidebar ).width()});
    }else{
    	$( sidebar ).parent().offset({ left:$('#mainsection').offset().left+maincolWt});
    }

     adjustOverlaps(sidebar);
    }
function adjustOverlaps(sidebar){
     /* Sidebar callout should not overlap on fullwidthimage or partially width image(single stat) */
      var comp=[".fullwidthimage",".partiallywrappedimage .img-container img"];
        for(var j=0;j<comp.length;j++){
            var fullImg = $('.mainreportsection').find(comp[j]);
            for(var i=0;i<fullImg.length;i++){
            var sidebarTop =  $(sidebar).offset().top;
            var sidebarHt = $(sidebar).height();
            var sidebarBottom = sidebarTop + sidebarHt;

            var fullImgTop = $(fullImg[i]).offset().top;
            var fullImgHt = $(fullImg[i]).height();
            var fullImgBottom = fullImgTop + fullImgHt;
                if((sidebarTop>fullImgTop && sidebarTop < fullImgBottom) || (sidebarTop < fullImgTop && sidebarBottom > fullImgTop) ) {
                    $(sidebar).offset({ top: fullImgBottom});
                }
            }
            } 	
    /* Sidebar callout should not run over on download button or related insights section*/
      comp=[".ctaformrebrand .dl-report-box",".relatedlinkssection"];
        for(var j=0;j<comp.length;j++){
            var fullImg = $('.reportdetailrebrand').find(comp[j]);
            for(var i=0;i<fullImg.length;i++){
            var sidebarTop =  $(sidebar).offset().top;
            var sidebarHt = $(sidebar).height();
            var sidebarBottom = sidebarTop + sidebarHt;

            var fullImgTop = $(fullImg[i]).offset().top;
            var fullImgHt = $(fullImg[i]).height();
            var fullImgBottom = fullImgTop + fullImgHt;
                if((sidebarTop>fullImgTop && sidebarTop < fullImgBottom) || (sidebarTop < fullImgTop && sidebarBottom > fullImgTop) ) {
                    var reportParsys = $('.reportdetailrebrand').find('.reportrebrandparsys');
                    var reportParsysTop = $(reportParsys[0]).offset().top;
                    $(sidebar).offset({ top: reportParsysTop});
                }
            }
            }

        }
</script>


