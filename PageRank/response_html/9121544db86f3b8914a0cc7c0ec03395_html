  <script type="text/javascript">
	$( document ).ready(function() {
        /* Social Icons component on the top sticky nav*/
        var x = $('.report-category-right .social-buttons-top').wrap('<p/>').parent().html();
        $('.report-category-right .social-buttons-top').unwrap();
        $('.scroll-icons').html($(x));

        var x = $('.report-category-right .print-icon').wrap('<p/>').parent().html();
        $('.report-category-right .print-icon').unwrap();
        $('.scroll-icons').append($(x));

        if($(window).width()<768){
           	 var x = $('.report-category-right.bottom .email-icon').wrap('<p/>').parent().html();
      		 $('.report-category-right.bottom .email-icon').unwrap();
        	 $('.scroll-icons').append($(x));
        }else{
             var x = $('.report-category-right .email-icon').wrap('<p/>').parent().html();
       		 $('.report-category-right .email-icon').unwrap();
        	 $('.scroll-icons').append($(x));
        }

        /* Logo component on the top sticky nav*/

        var x = $('.logo .logo-mob').wrap('<p/>').parent().html();
        $('.logo .logo-mob').unwrap();
        $('.scroll-logo').html($(x));

         /* Title section on the top sticky nav*/

        var x = $('.page-heading').wrap('<p/>').parent().html();
        $('.page-heading').unwrap();
        $('.scroll-title').html($(x));

         /* Download section on the top sticky nav*/

        var x = $('.download-btn-top .genericctabutton.downloadbtn .genericctalinkbuttons').wrap('<p/>').parent().html();
        $('.download-btn-top .genericctabutton.downloadbtn .genericctalinkbuttons').unwrap();
        $('.scroll-download').html($(x));

         /* Newswire CTA - Contact Us section on the top sticky nav*/

        var x = $('.newswiredetailbuttonCTA .cta-contact').wrap('<p/>').parent().html();
        $('.newswiredetailbuttonCTA .cta-contact').unwrap();
        $('.scroll-contactus').html($(x));

        enableSocialIconLinks();

		
  		  $('.pagescrollstickynav').css('display','none');
		
    });
</script> 
<script type="text/javascript">
     
    $( document ).ready(function() {
        // var subpageHt = $('.report').height()
    var $w = $(window);
	var $circ = $('.animated-circle');
	var $progCount = $('.progress-count');
	var $prog2 = $('.progress-indicator-2');

	var wh, h, sHeight;

	function setSizes(){
		wh = $w.height();
		h = $('body').height();
		sHeight = h - wh;
	}

	setSizes();
    var perc = Math.max(0, Math.min(1, ($w.scrollTop()/sHeight)));
    updateProgress(perc);

	$w.on('scroll', function(){
		var perc = Math.max(0, Math.min(1, ($w.scrollTop()/sHeight)));
        setSizes();
		updateProgress(perc);
	}).on('resize', function(){
		setSizes();
		$w.trigger('scroll');
	});

	function updateProgress(perc){
		var circle_offset = 126 * perc;
		$circ.css({
			"stroke-dashoffset" : 126 - circle_offset
		});
		$progCount.html(Math.round(perc * 100) + "%");

		$prog2.css({width : perc*100 + '%'});
	}

}());
/* Helper functions to check and hide download img icon on mobile widths when form / thank you is opened */
$.fn.isInViewport = function() {
    var topStickyHt = $('.pagescrollstickynav').height();
    var socialStickyHt = $('.rightscroll').height();

    var elementTop = $(this).offset().top;
    var elementBottom = elementTop + $(this).outerHeight();

    var viewportTop = $(window).scrollTop() ;
    var viewportBottom = viewportTop + $(window).height() - socialStickyHt;

    return elementBottom - topStickyHt > viewportTop && elementTop < viewportBottom;
};
function showHideDownloadIcon(){
    var downloadBtn = $('.dl-report-box');
    if(downloadBtn.length>0){
        if ($('.dl-report-box').isInViewport() && $(window).width() < 767) {
            if($('#modal-lead-gen').hasClass("opened")){
                $('.scroll-download').hide();
            }else{
                $('.scroll-download').show();
            }
        } else {
            $('.scroll-download').show();
        }
    }
}
/* Helper functions to check and hide download img icon on mobile widths when form / thank you is opened */ 
   $(window).scroll(function() {
    var distanceFromTop = $(this).scrollTop();
    if (distanceFromTop >= $('.report').offset().top) {
        $('.pagescrollstickynav').show( 300, function() {
             $('.pagescrollstickynav').addClass("fixed");
  		});
    } else {
        $('.pagescrollstickynav').hide(300, function() {
       		 $('.pagescrollstickynav').removeClass("fixed");
        });
	}
       if ($(window).width() < 768) {
           showHideDownloadIcon();
       }
   });
   $(window).resize(function() {
        showHideDownloadIcon();
   });

</script>  </div>

