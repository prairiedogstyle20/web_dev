<script type="text/javascript">
$( window ).resize(function() {
    setBoxHt();
    setTagIconImgClass();
});
    function setTagIconImgClass(){
    var tagIcons = $('.boxlist .tagiconframe img');
    var tagNames = $('.boxlist .tagsection h5');
    var imgHt = $(tagIcons[0]).height();
    $('.boxlist .tagiconframe').removeClass("wrappedDiv");
    for(var i=0;i<tagIcons.length;i++){
         if($(tagIcons[i]).height()<=$(tagNames[i]).height() && $(tagIcons[i]).height()>0 ){
            $(tagIcons[i]).closest("div.tagiconframe").addClass("wrappedDiv");
         }
    }

}
function setBoxHt(){
        if ($(window).width() >= 768) {
            var maxHt=0;
            var i=0;            
            var boxes = $('.boxlist'); 
            $('.boxlist').height("auto");
            while(i<boxes.length){
                var j=i;
                var noOfBoxesinCurrRow=3;
                if(j+3>=boxes.length){
                    noOfBoxesinCurrRow = boxes.length - j;
                }
                for(var k=j;k<j+noOfBoxesinCurrRow;k++){
                    var ht = $(boxes[k]).height();
                    if(ht>maxHt){
                        maxHt = ht;
                    }
                }
                for(var k=j;k<j+noOfBoxesinCurrRow;k++){
                    $(boxes[k]).height(maxHt);
                }
                maxHt=0;
                i=i+noOfBoxesinCurrRow;
            }
         }

    }
    var relatedLinksPageUrl = "/content/corporate/us/en/snp-include-pages/snp-results-include" +".html";
    var readmoreVal = "Read More";
    var resultPagePath = "";
    var seoImage = "";
    var maxResults = 3;
    var relatedTag = "TV|audience measurement";
    var categoryTag = "Media";
    var allRelTags = "Related:TV|Related:audience measurement|Category:Media|";
     var excludePagePath = "/content/corporate/us/en/insights/news/2019/nearly-47-million-viewers-watch-president-trumps-state-of-the-union-address.html";
    if( excludePagePath.indexOf("/content/corporate") > -1){
      	excludePagePath = excludePagePath.substring(18,excludePagePath.length);
    }
    if(relatedLinksPageUrl!=""   && allRelTags!="") {
        //$(document).ready(function(){ 
         $.ajax({
	        	type: 'GET',
	        	url:  relatedLinksPageUrl,
	        	cache: false,
             data: { q:"*",q1:"Newswire|Report|Podcast|Case Study|Videos|Webinars",x1:"contenttypetag",q_6:"RelatedLinks",sp_ro:"tags:10",x2:"tags",q2:""+allRelTags,
                    sp_x_3:"url",sp_q_3:excludePagePath,sp_q_required_3:"-1",sp_p_3:"any",readmore:readmoreVal,pinPagePath:resultPagePath,pinSeoImage:seoImage,
                    maxResults:maxResults},
             	beforeSend : function(xhr) {
	            	xhr.setRequestHeader('Accept', "text/html; charset=utf-8");
                    $('.loading-image').show()
	        	},
                complete: function(){
                    $('.loading-image').remove();
                },
	        	success: function(result) {
                     var datahtml = result;
					datahtml = $(datahtml).find("#relatedlinks").html();
                      if(datahtml!=null){
                          $(".relatedarticleslinks").append(datahtml);
                          setBoxHt();
                          setTagIconImgClass();
                      }else{
                          $(".relatedarticleslinksnp").closest("div.row-fluid"). hide();
                      }
	      		},
	       		error: function() {
                    //alert("ajax call error");
	          	}
	      	});	
        //	});
    }else{
        $('.loading-image').remove();
        $(".relatedlinkssection").hide();
    }
</script>
 </div>

                   
                   
               </div>
           </div>         
          </div>
           
