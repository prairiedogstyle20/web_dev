<script type="text/javascript">

function get_splash() {
    $('html, body').animate({scrollTop:0}, 'slow');
    $('#modal').modal();
    $('#modal-glob-splash').modal();
}

function close_splash() {
    $('#modal-glob-splash').modal('hide');
    $('#modal').modal('hide');
}  
   
$.fn.exists = function () {
	return this.length !== 0;
}
    
$(document).ready(function(){  
	
    $('a.glob-splash-btn').click( function(e) {
       if($(this).hasClass('prevented')){
         e.preventDefault();
         if(!$(this).hasClass('empty')){
        	 get_splash();        	 
         }
       }
   	});
     
    //close win when close btn click
    $('.close-modal').click(function(event){
        event.preventDefault();
        $('#modal-glob-splash').modal('hide');
        $('#modal').modal('hide');
    });
    
    //close win when en link clicked from en homepage
    $('.en-close-modal').click(function(event){
        var thePath = window.location.pathname;
        var enPath = '/us/en.html';
        if (thePath.indexOf(enPath, thePath.length - enPath.length) !== -1) {
            event.preventDefault();
            $('#modal-glob-splash').modal('hide');
            $('#modal').modal('hide');
        }
    });
    
    //re-pos win at center
    var windowHeight = $(window).height();
    var modalPlacement = $(window).width();
    
    $('.modal').each(function(){
        /*$(this).children('.modal-body').height(windowHeight*.9);*/
        $(this).css('left', (.5*modalPlacement)-($(this).width()*.5));
    });
    
    //re-size when browser re-size
    $(window).resize(function(){
        windowHeight = $(window).height();
        modalPlacement = $(window).width();
        $('.modal').each(function(){
            /*$(this).children('.modal-body').height(windowHeight*.9);*/
            $(this).css('left', (.5*modalPlacement)-($(this).width()*.5));
        });
        if($(window).width() > 767){
            var tabCounter = $('.top10 .nav-tabs li').length;
            $('.top10 .nav-tabs li').width(($('.top10 .nav-tabs').width()/tabCounter)-.1);
        }
        else{
            $('.top10 .nav-tabs li').css('width','100%');
        }
    });
});
</script>





