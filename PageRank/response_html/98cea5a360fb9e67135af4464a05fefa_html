<a data-content='&lt;div class="container-fluid"&gt;&#x000A;  &lt;a class=&#x0027;confirm-close-popover dkons x&#x0027; href=&#x0027;#&#x0027;&gt;&lt;/a&gt;&#x000A;&#x000A;  &lt;form id="update-form" class="edit_story" action="/stories/1834461/append_liveupdate" accept-charset="UTF-8" method="post"&gt;&lt;input name="utf8" type="hidden" value="&amp;#x2713;" /&gt;&lt;input type="hidden" name="_method" value="patch" /&gt;&lt;input type="hidden" name="authenticity_token" value="Mk9O0Ty7bPU0x+1fSDZaDcPrnHYg2g4AP0X+GXztYKAKxfBjnHmR80gRPvSbtxzoTn7c9QXikfd6uENUCe5EkQ==" /&gt;&#x000A;    &lt;label for="story_Update"&gt;Update&lt;/label&gt;&#x000A;    &lt;div class=&#x0027;pop-over-cke-container&#x0027;&gt;&#x000A;      &lt;textarea class="form-control" placeholder="Write your update..." cols="20" rows="6" name="story[update_text]" id="story_update_text"&gt;&#x000A;&lt;/textarea&gt;&#x000A;    &lt;/div&gt;&#x000A;    &lt;button name="button" type="submit" id="update-button"&gt;Update this story&lt;/button&gt;&#x000A;&lt;/form&gt;&lt;/div&gt;&#x000A;&lt;script&gt;&#x000A;//&lt;![CDATA[&#x000A;&#x000A;  (function(){&#x000A;    $(&#x0027;#story_update_text&#x0027;).ckeInline();&#x000A;  })();&#x000A;&#x000A;  // this is garbage, needs much better&#x000A;  (function() {&#x000A;    var disabledButton = false;&#x000A;&#x000A;    $(&#x0027;#update-button&#x0027;).click(function(e){&#x000A;      e.preventDefault();&#x000A;&#x000A;      if (disabledButton) return;&#x000A;      disabledButton = true&#x000A;&#x000A;      $.ajax({&#x000A;        type: &#x0027;PUT&#x0027;,&#x000A;        url:  $(&#x0027;#update-form&#x0027;).attr(&#x0027;action&#x0027;),&#x000A;        data: $(&#x0027;#update-form&#x0027;).serialize(),&#x000A;        success: function() {&#x000A;&#x000A;          $(&#x0027;#update-toggle&#x0027;).popover(&#x0027;hide&#x0027;);&#x000A;&#x000A;          var cacheInvalidator = +(new Date())&#x000A;          var newUrl = location.origin + location.pathname + &#x0027;?t=&#x00