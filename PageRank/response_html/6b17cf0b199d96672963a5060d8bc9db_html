// Detect any ab-group set in the current DOM.
var parseABInfoTag = function() {
  var ab_info_tag = jQuery('[class^="ab-group--"],[class*=" ab-group--"]');
  if (ab_info_tag.length > 0) {
    var extractABValue = function(element, prefix) {
      return element
        .className
        .split(' ')
        .filter(function(v, i) {
          // Filter out classes not starting with prefix.
          return v.match(new RegExp("^" + prefix));
          })
        [0]
        .replace(prefix, '');
    };
    var ab_group = extractABValue(ab_info_tag[0], 'ab-group--');
    //var ab_experiment = extractABValue(ab_info_tag[0], 'ab-experiment--');

    s.prop21 = s.eVar14 = ab_group;
  }
};
