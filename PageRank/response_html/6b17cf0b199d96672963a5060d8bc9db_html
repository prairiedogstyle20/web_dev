<script>googletag.cmd.push(function() {
  googletag.pubads().enableAsyncRendering();
  googletag.pubads().enableSingleRequest();
  googletag.pubads().collapseEmptyDivs();
  googletag.pubads().addEventListener('slotRenderEnded', function(event) {

      var ad_tag_id = event.slot.getAdUnitPath().split('/');
      var ad_tag_id = ad_tag_id[ad_tag_id.length - 1];
      var magazine_sections = jQuery('.l-magazine-left:not(section)');            
      
      if ( ad_tag_id == 'x90_2' ) {
        if (event.isEmpty === true) {
          jQuery( '#' + event.slot.getSlotElementId() + '-wrapper' ).parents( 'div[data-ad-id=\'90xA\']' ).remove();
        }
        
        var ad = jQuery('#dfp-ad-x90_2');
        var speedBumpHeight = ad.parents('.speed-bump').outerHeight();
        ad.parents('.full-width-speed-bump').css({ 'height' : speedBumpHeight });
        
      }

      if (event.slot.getAdUnitPath() !== '' && event.isEmpty === false) {
        var width = window.innerWidth || document.documentElement.clientWidth || document.body.clientWidth;

        if (ad_tag_id == '300x_1' && (jQuery('.node-type-issue').length !== 0) && width >= 900) {
          jQuery(magazine_sections[0]).css('min-height', event.size[1]+14);
        }

        if (ad_tag_id == '300x_2' && (jQuery('.node-type-issue').length !== 0 && width >= 900)) {
          jQuery(magazine_sections[1]).css('min-height', event.size[1]+14);
        }

        if (ad_tag_id == '300x_3' && (jQuery('.node-type-issue').length !== 0 && width >= 900)) {
          jQuery(magazine_sections[2]).css('min-height', event.size[1]+14);
        }

        if (ad_tag_id == '300x_4' && (jQuery('.node-type-issue').length !== 0 && width >= 900)) {
          jQuery(magazine_sections[3]).css('min-height', event.size[1]+14);
        }

        if (ad_tag_id == '300x_5' && (jQuery('.node-type-issue').length !== 0) && width >= 900) {
          jQuery(magazine_sections[4]).css('min-height', event.size[1]+14);
        }

        // @todo: We need to refactor this to apply globally.
        // refs: #41996
        if (ad_tag_id == 'x90_nav' && (jQuery('.site-header--gradient').length !== 0)) {
          jQuery('body').addClass('header-ad-present');
        }

        if (ad_tag_id == 'sponsored_feature_audio' && (jQuery('.node-type-media').length !== 0)) {
          jQuery('#dfp-ad-88x31_sponsored_feature_audio-wrapper').append('<p class="article-header-tools-item__sponsor">Audio powered by </p>');
          jQuery('#dfp-ad-88x31_sponsored_feature_audio').css('float', 'right');
          jQuery('.audio__sponsor').css('display', 'block');
        }

        if (ad_tag_id == 'sponsored_feature_audio' && (jQuery('.node-type-article').length !== 0)) {
          jQuery('#dfp-ad-88x31_sponsored_feature_audio-wrapper').prepend('<p class="article-header-tools-item__sponsor">Audio powered by </p>');
          jQuery('#dfp-ad-88x31_sponsored_feature_audio').css('float', 'left');
          jQuery('.article-header-tools-item__sponsor').css('float', 'left');
          jQuery('.audio__sponsor').css('display', 'block');
        }

        if (event.slot.getAdUnitPath() == '/101014362/fa/sponsored_feature_anthology/1' && jQuery('#anthology_wrapper_text_1').length == 0) {
          jQuery('#dfp-ad-88x31_sponsored_feature_anthology_1-wrapper').prepend('<p>The&nbsp;Foreign&nbsp;Affairs&nbsp;Anthology&nbsp;Series is&nbsp;sponsored&nbsp;by</p>');
        }
        if (event.slot.getAdUnitPath() == '/101014362/fa/sponsored_feature_anthology/2' && jQuery('#anthology_wrapper_text_2').length == 0) {
          jQuery('#dfp-ad-88x31_sponsored_feature_anthology_2-wrapper').prepend('<p>The&nbsp;Foreign&nbsp;Affairs&nbsp;Anthology&nbsp;Series is&nbsp;sponsored&nbsp;by</p>');
        }

        if (ad_tag_id == 'sponsored_feature_timeline') {
          if (jQuery('#timeline_wrapper_text').length == 0) {
            jQuery('#dfp-ad-88x31_sponsored_feature_timeline-wrapper').prepend('<div id="timeline_wrapper_text" style="float: left; margin-right: 10px;">Powered by </div>');
          }
          jQuery('#dfp-ad-88x31_sponsored_feature_timeline').css('float', 'left');
        }

        if (ad_tag_id == 'sponsored_feature_video') {
          jQuery('.node-type-media #dfp-ad-88x31_sponsored_feature_video-wrapper').prepend('Video powered by ');
          jQuery('.node-type-media #dfp-ad-88x31_sponsored_feature_video').css('display', 'inline-block').css('margin-left', '3px');
        }

        if (ad_tag_id == 'sponsored_feature_video') {
          jQuery('.node-type-homepage #dfp-ad-88x31_sponsored_feature_video-wrapper').prepend('<div class="media-sponsored-ad--video--front">Sponsored by </div>');
          jQuery('.node-type-homepage #dfp-ad-88x31_sponsored_feature_video').css('display', 'inline-block').css('margin-left', '3px');
        }

        if (ad_tag_id == 'sponsored_feature_imagegallery') {
          jQuery('.node-type-homepage #dfp-ad-88x31_sponsored_feature_imagegallery-wrapper').prepend('<div class="media-sponsored-ad--gallery--front">Sponsored by </div>').css('float', 'right');
          jQuery('.node-type-homepage #dfp-ad-88x31_sponsored_feature_imagegallery').css('float', 'right');
        }
        if (ad_tag_id == 'sponsored_feature_imagegallery') {
          jQuery('.node-type-media #dfp-ad-88x31_sponsored_feature_imagegallery-wrapper').prepend('<div class="media-sponsored-ad--gallery">Sponsored by </div>').css('display', 'inline-block');
          jQuery('.node-type-media #dfp-ad-88x31_sponsored_feature_imagegallery').css('display', 'inline-block');
        }

        if (ad_tag_id == '88x31_1' && (jQuery('.node-type-graduate-school').length !== 0)) {
          jQuery('#dfp-ad-88x31_1-wrapper').append('<span class="detail__sponsor">Presented by </span>');
          jQuery('#dfp-ad-88x31_1').css('float', 'right');
        }

        if (ad_tag_id.indexOf('88x31') > -1 && (jQuery('.node-type-sponsored-section-landing').length !== 0)) {
          var ad_css_id = '#dfp-ad-' + ad_tag_id;
          var ad_css_wrapper_id = ad_css_id + '-wrapper';
          jQuery(ad_css_wrapper_id).prepend('Sponsored by ');
          jQuery(ad_css_id).css('float', 'right').css('position', 'relative').css('top', '-10px');
        }

        if (ad_tag_id == 'sponsored_content_front' && jQuery('.sponsored_content_processed').length === 0) {
          jQuery('#dfp-ad-88x31_sponsored_content_front-wrapper').prepend('<p class=\'sponsored_content_processed\'>Presented by</p>');
          //jQuery('#dfp-ad-sponsored_content_front').css('float', 'right').css('position', 'relative').css('top', '-10px');
        }

        if (ad_tag_id == '88x31_1' && (jQuery('.node-type-sponsored-country').length !== 0)) {
          jQuery('#dfp-ad-88x31_1-wrapper').append('<div class="sponsored_country_text">Presented by </div>');
          jQuery('#dfp-ad-88x31_1').css('float', 'right');
          jQuery('.sponsored_country_text').css('float', 'left').css('margin-right', '5px');
        }

        if (ad_tag_id == '88x31_1' && (jQuery('.node-type-sponsored-article').length !== 0)) {
          jQuery('.node-type-sponsored-article #dfp-ad-88x31_1-wrapper').prepend('Sponsor Content presented by ');
          jQuery('.node-type-sponsored-article #dfp-ad-88x31_1').css('display', 'inline-block').css('margin-left', '3px');
        }
      }
    });
  googletag.enableServices();});
</script>
