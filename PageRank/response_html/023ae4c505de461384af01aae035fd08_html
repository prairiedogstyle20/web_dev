$(window).scroll(function() {
if($(window).width() > 767){

if ($(this).scrollTop() > 115){
    $('.nation-navigation').addClass("navbar-fixed-top");
	$('.nation-home').hide();
	$('.sticky-logo').show();
  }
  else{
	$('.nation-navigation').removeClass("navbar-fixed-top");
	
   $('.nation-home').show();
	$('.sticky-logo').hide();
  }
  }
});

if ($(window).width() < 768){
	$('body').css('padding-top', '75px');
	$('.nation-navigation').addClass("navbar-fixed-top");
}

 })
 
