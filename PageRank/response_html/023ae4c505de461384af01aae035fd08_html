<script>
	$(window).scroll(function(){
		if ($(this).scrollTop() > 100) {
			$('.scrollToTop, .add-scroll').fadeIn();
		} else {
			$('.scrollToTop, .add-scroll').fadeOut();
		}
	});
	
	//Click event to scroll to top
	$('.scrollToTop, .add-scroll').click(function(){
		$('html, body').animate({scrollTop : 0},800);
		return false;
	});
	
