        cmg.adconf.targeting.environ = location.search.indexOf('testads') > 0 ? 'debug' : location.host.split('.')[0];
        cmg.adconf.targeting.mediatype = 'np-one';
        cmg.adconf.targeting.qa = getQueryParam('qa') || '';
    }());
</script>
