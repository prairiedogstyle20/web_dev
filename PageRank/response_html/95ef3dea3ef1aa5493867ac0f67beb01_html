<script type="text/javascript">
    $(function () {
        $('#packages').html("<div class='swm-left'><img class='swm-widget-loading-img' src='https://www.sportswatchmonitor.com/images/squares.svg' alt='Loading Packages...' title='Loading Packages...' />&nbsp;<span class='swm-widget-loading-text'>Loading Packages</span></div>");
        $.ajax({
            datatype: "text/plain",
            type: "GET",
            url: 'https://www.sportswatchmonitor.com/Widgets/Packages?handicapper=1393',
            cache: false,
            success: function (data) {
                $('#packages').html(data);
            }
        });


        $('#premiumPicks').html("<div class='swm-left'><img class='swm-widget-loading-img' src='https://www.sportswatchmonitor.com/images/squares.svg' alt='Loading Guaranteed Picks...' title='Loading Guaranteed Picks...' />&nbsp;<span class='swm-widget-loading-text'>Loading Guaranteed Picks</span></div>");
        $.ajax({
            datatype: "text/plain",
            type: "GET",
            url: 'https://www.sportswatchmonitor.com/Widgets/PremiumPicksWidgetV2?handicapper=Jack%20Jones',
            cache: false,
            success: function (data) {
                $('#premiumPicks').html(data);

                var l = '';

                if (l != '' && l != null) {
                    scrollToLeagueWidget(l, 'PremiumPick');
                    catchScrolling($(window));
                }
            }
        });


        $('#freePicks').html("<div class='swm-left'><img  class='swm-widget-loading-img' src='https://www.sportswatchmonitor.com/images/squares.svg' alt='Loading Free Picks...' title='Loading Free Picks...' />&nbsp;<span class='swm-widget-loading-text'>Loading Free Picks</span></div>");
        $.ajax({
            datatype: "text/plain",
            type: "GET",
            url: 'https://www.sportswatchmonitor.com/Widgets/FreePicks?handicapper=Jack%20Jones',
            cache: false,
            success: function (data) {
                $('#freePicks').html(data);
            }
        });

   

        $('#top5articles').html("<div class='swm-left'><img class='swm-widget-loading-img' src='https://www.sportswatchmonitor.com/images/squares.svg' alt='Loading Articles...' title='Loading Articles...' />&nbsp;<span class='swm-widget-loading-text'>Loading Articles</span></div>");
        $.ajax({
            datatype: "text/plain",
            type: "GET",
            url: 'https://www.sportswatchmonitor.com/Widgets/TopFiveArticlesCapper/Jack Jones',
            cache: false,
            success: function (data) {
                $('#top5articles').html(data);
            }
        });


        
    });
</script>

