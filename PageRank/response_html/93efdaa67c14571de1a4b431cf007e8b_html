<!-- DFP bidder skyline -->
<script async="async" type="text/javascript" src="https://modulous.huffpost.com/2017/09/07/1504799030-custom_prebid.new.js"></script>
<script type="text/javascript">
  window.dfpBidder = (function() {

    window.googletag = window.googletag || {};
    window.googletag.cmd = window.googletag.cmd || [];
    window.googletag.slots = window.googletag.slots || {};

    window.pbjs = window.pbjs || {};
    window.pbjs.que = window.pbjs.que || [];

    var status = 'pending';
    var prebidTimeout = getPrebidTimeout(500);
    var debug_prebid_enabled = !!getQuery("debug_prebid", false);

    var slot_size_config = {
      "habillage": [1800, 1000]
    };

    var nextone_config = {
      member: 8253,
      targetIdsToSendToNextone: ["banniere_haute", "pave_haut", "habillage", "inread", "cover"],
      prefix: {
        adUnitPath: "nxo",
        adUnitSeparator: "-",
        keyValueToPrebid: "nxo_lm_",
        keyValueToPrimary: "nxo_"
      },
      roadblockSignalTag: ["pave_haut"],
      testScenario: getQuery("test_scenario", "exclusivite")
    };

    function getQuery(name, def) {
      def = def || null;
      url = window.location.href;
      name = name.replace(/[\[\]]/g, "\\$&");
      var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)");
      var results = regex.exec(url);
      if (!results) return def;
      if (!results[2]) return def;
      return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    function log(m) {
      var _tmp = debug_prebid_enabled && window.console && window.console.log(m);
      return;
    }

    function getPrebidTimeout(def) {
      return parseInt(getQuery('prebid_timeout', def + ""), 10);
    }

    function generatePageConfig() {
      var targeting_keys = googletag.pubads().getTargetingKeys();
      var slots = googletag.pubads().getSlots();
      var page_config = {
        keywords: {},
        tags: []
      };

      var m, n;

      for (m = 0, n = targeting_keys.length; m < n; m++) {
        var k = targeting_keys[m];
        page_config.keywords[k] = googletag.pubads().getTargeting(k);
      }

      for (m = 0, n = slots.length; m < n; m++) {
        var slot = slots[m];
        var tag = {};
        tag.adUnitPath = slot.getAdUnitPath();
        tag.targetId = slot.getSlotElementId();

        tag.sizes = [];
        slot.getSizes().forEach(function(size) {
          if (typeof size === 'object') { 
            var sz = []; 
            sz.push(size.getWidth(), size.getHeight()); 
            tag.sizes.push(sz); 
          }
        });
        
        if (tag.sizes.length === 1) {
          tag.sizes = tag.sizes[0];
        }

        page_config.tags.push(tag);
      }

      return page_config;
    }

    function generatePrebidAdUnits(page_config, nextone_config) {

      function getTargetForPreBid(target_id) {
        for (var m = 0, n = nextone_config.targetIdsToSendToNextone.length; m < n; m++) {
          if (target_id.indexOf(nextone_config.targetIdsToSendToNextone[m]) !== -1) {
            return nextone_config.targetIdsToSendToNextone[m];
          }
        }
        return null;
      }

      function getTargetSize(slot, dflt) {
        return slot_size_config[slot] || dflt || [];
      }

      var ad_units = [],
        keywords = {};

      keywords["test-nxo_scenario"] = nextone_config.testScenario;
      var key;

      for (key in page_config.keywords) {
        keywords[nextone_config.prefix.keyValueToPrebid + key] = page_config.keywords[key];
      }

      for (var m = 0, n = page_config.tags.length; m < n; m++) {
        var item = page_config.tags[m];
        var prebid_target = getTargetForPreBid(item.targetId);
        if (!!prebid_target) {
          ad_unit = {
            code: item.targetId,
            sizes: getTargetSize(prebid_target, item.sizes),
            bids: [{
              bidder: 'appnexusAst',
              params: {
                member: nextone_config.member,
                invCode: nextone_config.prefix.adUnitPath + item.adUnitPath.replace(/\//g, nextone_config.prefix.adUnitSeparator),
                keywords: keywords
              }
            }]
          };
          ad_units.push(ad_unit);
        }
      }
      return ad_units;
    }

    function setDfpTargetingForNextone(nextone_config) {

      function targetMarkedForRoadBlock(target_id) {
        for (var m = 0, n = nextone_config.roadblockSignalTag.length; m < n; m++) {
          if (target_id.indexOf(nextone_config.roadblockSignalTag[m]) !== -1) {
            return true;
          }
        }
        return false;
      }

      function parseBidResponse(bid) {
        var adData = {
          "isRoadblock": false,
          "lineItemCode": ""
        };
        try {
          if ("mediaType" in bid && bid.mediaType == "video-outstream") {
            adMatch = bid.adResponse.ad.rtb.video.content.match(/bounce\?NXO_LINE_ITEM_CODE\=([^\]\<]*)/);
            if (adMatch != null) {
              adData.lineItemCode = adMatch[1];
            }
          } else if ("mediaType" in bid && bid.mediaType == "native") { //  LazyRefresh Format Native
            var imp_trackers = bid.native.impressionTrackers;
            for (var i = 0; i < imp_trackers.length; i++) {
              if (imp_trackers[i].indexOf("ib.adnxs.com/bounce?NXO_LINE_ITEM_CODE") != -1) {
                var lineitemcode = imp_trackers[i].substring(imp_trackers[i].indexOf("CODE=") + 5);
                adData.lineItemCode = lineitemcode;
                break;
              }
            }
          } else { //  LazyRefresh END Format Native
            adMatch = bid.ad.match(/\/\* NXO_START (\{.*?\}) NXO_END \*\//);
            if (adMatch != null) {
              var parsed = JSON.parse(adMatch[1]);
              for (var key in parsed) {
                adData[key] = parsed[key];
              }
            }
          }
        } catch (e) {
          log("Failed to decode creative", e);
        }
        return adData;
      }

      var responses = pbjs.getBidResponses();

      log('PREBID RESPONSES:');
      log(responses);

      var pageKeywords = {};

      pageKeywords[nextone_config.prefix.keyValueToPrimary + "roadblock"] = 0;
      pageKeywords[nextone_config.prefix.keyValueToPrimary + "r_line_item_code"] = "";

      for (var targetId in responses) {
        var bid = responses[targetId].bids[0];
        log('BID STATUS CODE for:' + targetId + " is " + bid.getStatusCode());
        if (bid.getStatusCode() == 1) {
          var adData = parseBidResponse(bid);
          if (targetMarkedForRoadBlock(targetId)) {
            log("IS ROADBLOCK? " + adData.isRoadblock);
            if (adData.isRoadblock) {
              pageKeywords[nextone_config.prefix.keyValueToPrimary + "roadblock"] = 1;
              pageKeywords[nextone_config.prefix.keyValueToPrimary + "r_line_item_code"] = adData.lineItemCode;
            }
          }

          var tagKeywords = {};
          for (var i = 0, j = bid.adserverTargeting.length; i < j; i++) {
            var key = bid.adserverTargeting[i];
            tagKeywords[nextone_config.prefix.keyValueToPrimary + key] = bid.adserverTargeting[key];
          }

          tagKeywords[nextone_config.prefix.keyValueToPrimary + "demand_source"] = bid.buyerMemberId == nextone_config.member ? "direct" : "rtb";
          tagKeywords[nextone_config.prefix.keyValueToPrimary + "creative_id"] = bid.creative_id;
          tagKeywords[nextone_config.prefix.keyValueToPrimary + "line_item_code"] = adData.lineItemCode;

          var slots = googletag.pubads().getSlots();
          for (var m = 0, n = slots.length; m < n; m++) {
            var slot = slots[m];
            if (slot.getAdUnitPath() == targetId || slot.getSlotElementId() == targetId) {
              var pb_keywords = pbjs.getAdserverTargetingForAdUnitCode(targetId);
              if (pb_keywords) {
                for (var key in pb_keywords) {
                  tagKeywords[nextone_config.prefix.keyValueToPrimary + key] = pb_keywords[key];
                }
              }
              for (var key in tagKeywords) {
                log('SET SLOT TARGETING: (' + key + " -> " + tagKeywords[key].toString() + ")");
                slot.setTargeting(key, tagKeywords[key].toString());
              }
            }
          }
        }
      }


      // Set page-level targeting again
      for (var key in pageKeywords) {
        log('SET PAGE TARGETING: (' + key + " -> " + pageKeywords[key].toString() + ")");
        googletag.pubads().setTargeting(key, pageKeywords[key].toString());
      }
    }

    function initDfpAdServer(nextone_config) {
      if (pbjs.requestSent) {
        return;
      }
      pbjs.requestSent = true;
      googletag.cmd.push(function() {
        pbjs.que.push(function() {
          setDfpTargetingForNextone(nextone_config);
          // LazyRefresh 0
          //googletag.pubads().refresh();
          dfpController.loadAndLazyRefreshWhenSlotInView();
          // END  LazyRefresh 0
          status = 'complete';
        });
      });
    }

    log('PREBID_TIMEOUT: ' + prebidTimeout);
    log('NEXTONE TEST SCENARIO: ' + nextone_config.testScenario);

    return {
      exec: function() {
        /* Script ias */
        log("Script ias exec");
        requestAds = function() {
          log("Display Ads function by Script ias requestAds");
          clearTimeout(iasTimeOut);
          googletag.cmd.push(function() {
            var page_config = generatePageConfig();
            log('PAGE_CONFIG:');
            log(page_config);
            var ad_units = generatePrebidAdUnits(page_config, nextone_config);
            log('PREBID_AD_UNITS:');
            log(ad_units);
            pbjs.que.push(function() {
              pbjs.addAdUnits(ad_units);
              var tmp1 = debug_prebid_enabled && console.time('requestBids');
              pbjs.requestBids({
                bidsBackHandler: function(bidResponses) {
                  var tmp2 = debug_prebid_enabled && console.timeEnd('requestBids');
                  initDfpAdServer(nextone_config);
                },
                timeout: prebidTimeout
              });
            });
          });
        };

        iasDataHandler = function(adSlotData) {
          log("Script ias DataHandler:", adSlotData);
          __iasPET.setTargetingForGPT();
          requestAds();
        };

        var iasTimeOut = setTimeout(function() {
          log("Script ias TimeOut");
          requestAds();
        }, IAS_PET_TIMEOUT);

        googletag.cmd.push(function() {
          var gptSlots = googletag.pubads().getSlots();
          var iasPETSlots = [];

          for (var i = 0; i < gptSlots.length; i++) {
            //  LazyRefresh iasPETSlots dfp_inread
            if (gptSlots[i].getSlotElementId() === "dfp_inread") {
              
              sizes = [[2, 2], [630, 355]];
              
            } else {
              var sizes = gptSlots[i].getSizes().map(function(size) {
                if (size.getWidth && size.getHeight)
                  return [size.getWidth(), size.getHeight()];
                else
                  return [1, 1];
              });
            }
            //  END LazyRefresh iasPETSlots dfp_inread

            iasPETSlots.push({
              adSlotId: gptSlots[i].getSlotElementId(),
              size: sizes,
              adUnitPath: gptSlots[i].getAdUnitPath()
            });
          }

          log("Script ias PETSlots:", iasPETSlots);
          __iasPET.queue.push({
            adSlots: iasPETSlots,
            dataHandler: iasDataHandler
          });
        });
        /* Script ias */
        return;
      },
      completed: function() {
        return status === 'complete';
      }
    };
  })();
</script>
<!-- DFP bidder skyline ends -->
<!-- HEAD CODE -->
<script type="text/javascript" src="https://modulous.huffpost.com/2018/07/10/1531203820-e41fdd7-dfp-controller-ver-0.1.min.js"></script>
<script type="text/javascript">
    (function(){
        dfpController.setRefreshTimer(30000);
        dfpController.setRefreshOffset(250);

        dfpController.enableDebugMode();
        dfpController.loadGPTScript();

        // Set targeting
        (function(){
            var targeting = {
                rub: [],
                keywords: []
            };

            
            
            
            targeting.rub.push("bpage");
            var pageTags = "@gamp,donald-trump,etats-unis,international,shutdown";
            // Discuss with France AdOps in case they want @sponsor tags removed
            //pageTags = (pageTags + ",").replace(/(@aol-amp-render*|@sponsor*)\S+?,/g, "").replace(/\s+/g, "");
            //pageTags = pageTags.substr(0, pageTags.length-1);
            targeting.keywords = pageTags.split(",")
            
            
            

            

            var navtype = 0;
            if (typeof performance !== "undefined" && typeof performance.navigation !== "undefined" ) {
                navtype = performance.navigation.type;
            } else {
                if (document.referrer === window.location.href) navtype = 1;
            }
            // add a custom targeting to DFP call to pass the navigation type
            targeting.nt = "" + navtype;

            //temp
            function get_url_query(variable) {
                var query = window.location.search.substring(1);
                var vars = query.split('&');
                for (var i = 0; i < vars.length; i++) {
                    var pair = vars[i].split('=');
                    if (decodeURIComponent(pair[0]) == variable) {
                        return decodeURIComponent(pair[1]);
                    }
                }
                return '';
            }
            var dfptest = get_url_query('dfptest');
            if (dfptest !== '') {
                targeting.test = dfptest;
            }

            googletag.cmd.push(function() {
                googletag.pubads().enableAsyncRendering();
                googletag.pubads().collapseEmptyDivs();
                googletag.pubads().enableSingleRequest();
                for (var k in targeting) {
                    dfpController.log("Set Targeting: " + k + " -> " + targeting[k]);
                    googletag.pubads().setTargeting(k, targeting[k]);
                }
                googletag.enableServices();
                try {nvg41021.dfpnvg();}catch(e){}
            });

        })();

