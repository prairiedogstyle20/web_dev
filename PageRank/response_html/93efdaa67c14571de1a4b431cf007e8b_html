<script type="text/javascript">
    (function($) {
        $(document).ready(function () {
            var cookieName = 'fr_gdpr_consent_shown';
            var cookieShown = Cookies.get(cookieName);

            if (typeof cookieShown === 'undefined') {
                var $gdprConsent = window.popup.open({
                    id: 'fr-gdpr-consent',
                    content: "    <div class=\"popup-window__block gdpr-consent__logo\">\n        <img alt=\"Le Huffington Post\" src=\"https:\/\/amp.assets.huffpost.com\/assets-d928ff808e216b4bc55f710a0e7ea053\/images\/logos\/HuffPost.svg\">\n    <\/div>\n\n    <div class=\"popup-window__block\">\n        Notre politique de protection de vos donn\u00e9es personnelles \u00e9volue conform\u00e9ment au nouveau R\u00e8glement G\u00e9n\u00e9ral sur la Protection des Donn\u00e9es (RGPD) adopt\u00e9 par le Parlement europ\u00e9en et entrant en vigueur le 25 mai 2018.\n    <\/div>\n\n    <div class=\"popup-window__block\">\n        <a href=\"\/p\/charte-de-confidentialite\" class=\"popup-window__button gdpr-consent__understand\">\n            Prenez le temps de vous informer !\n        <\/a>\n\n        <small>\n            <a href=\"#\" class=\"popup-window__cancel\">\n                Plus tard\n            <\/a>\n        <\/small>\n    <\/div>\n",
                });

                $gdprConsent.on('close', function() {
                    Cookies.set(cookieName, 1, { expires: 30 });
                });                
                
                $('#fr-gdpr-consent').find('.popup-window__cancel').on('click', function(e) {
                    e.preventDefault();
                    e.stopPropagation();

                    window.popup.close('fr-gdpr-consent');
                });
                

                $('#fr-gdpr-consent').find('.gdpr-consent__understand').on('click', function(e) {
                    e.preventDefault();
                    e.stopPropagation();

                    window.popup.close('fr-gdpr-consent');
                    openPrivacyPage($(this).attr('href'));
                });

                var $consentClose = $('<div style="text-align: center; margin-top: 2rem; font-size: 1.1rem;"><strong><a style="padding: 5px; border: 2px solid #0dbe98;" href="#" class="popup-window__cancel">X&nbsp; Fermer</a></strong></div>');

                function openPrivacyPage(url) {
                    $.ajax(url, { method: 'GET' })
                        .done(function(data) {
                            var $entry = $(data).find('.entry');
                            $entry.append($consentClose);

                            var $pagePopup = window.popup.open({
                                id: 'privacy-policy',
                                content: $entry.html(),
                            });

                            $pagePopup.on('close', function() {
                                window.popup.closeAllPopups();
                            });
                        })
                        .fail(function(xhr) {
                            console.error(xhr);
                        });
                }
            }
        });
    })(jQuery);
</script>

                            

        
        
