    <script>
      $(document).ready(function() {
        /*document.getElementById('footballLink').addEventListener('click', function(e) {
             window.trackEvent('Fantasy Football Link DTI Desktop', {
                'action': 'User clicks the fantasy football link on Desktop header'
            });
      });*/

        $('.sticky-nav-header').html($('.masthead-header').html());
        $(window).scroll(function() {
          var winTop = $(window).scrollTop();
          if (winTop >= $('.masthead').height()) {
            $('.sticky-nav').css('top', 0);
          } else {
            $('.sticky-nav').css('top', '-130px');
          }
        });

        var isWebView =
          document.cookie.indexOf('isNativeAndroid') !== -1 ||
          document.cookie.indexOf('isNativeiOS') !== -1;
        if (isWebView) {
          $('#responsive-header').hide();
          $('#template-body-wrapper').css('margin-top', '0px');
        }

        var deeplinks = $("a[href^='https://deeplink.me']");
        var isUK = document.cookie.indexOf('GB-DK') !== -1;

        if (isUK) {
          for (var i = 0; i < deeplinks.length; i++) {
            deeplinks[i].href = deeplinks[i].href.replace('.com', '.co.uk');
          }
        }

        var promoCodeLinks = document.querySelectorAll('.promo-code-link');
        var signUpLinks = document.querySelectorAll('.sign-up-link');
        var facebookRegisterLinks = document.querySelectorAll(
          '.facebook-register-link',
        );

        //if the user is logged in
        if (vs.userId !== null) {
          _.forEach(promoCodeLinks, function(elem) {
            elem.parentNode.removeChild(elem);
          });

          _.forEach(signUpLinks, function(elem) {
            $(elem)
              .unbind()
              .on('click', function() {
                window.location.assign('/lobby');
              });
          });

          _.forEach(facebookRegisterLinks, function(elem) {
            $(elem)
              .unbind()
              .on('click', function() {
                window.location.assign('/lobby');
              });
          });

          $('#sign-in-link')
            .unbind()
            .on('click', function() {
              window.location.assign('/lobby');
            });
        }

        //if the user is not logged in
        else {
          if (vs.isMobile) {
            _.forEach(promoCodeLinks, function(elem) {
              $(elem)
                .unbind()
                .on('click', function() {
                  window.location.assign(
                    '/account/sitelogin/true?#promos&returnUrl=/account/depositnewvisitorinterstitial',
                  );
                });
            });

            _.forEach(facebookRegisterLinks, function(elem) {
              $(elem)
                .unbind()
                .on('click', function() {
                  window.location.assign(
                    '/account/sitelogin/true?returnUrl=/account/depositnewvisitorinterstitial',
                  );
                });
            });

            _.forEach(signUpLinks, function(elem) {
              $(elem)
                .unbind()
                .on('click', function() {
                  window.location.assign(
                    '/account/sitelogin/true?returnUrl=/account/depositnewvisitorinterstitial',
                  );
                });
            });

            $('#sign-in-link')
              .unbind()
              .on('click', function() {
                window.location.assign(
                  '/account/sitelogin/false?returnUrl=/account/depositnewvisitorinterstitial',
                );
              });
          } else {
            _.forEach(promoCodeLinks, function(elem) {
              $(elem)
                .unbind()
                .on('click', function() {
                  var mountElement = document.querySelector(
                    '#dkjs-dkauth-modal',
                  );
                  var properties = {
                    captchaKey: window.vs.recaptchaSiteKey,
                    gbosConfig: {
                      reverseDOBInput:
                        window.mvcVars.siteRegionConfig.reverseDOBInput,
                      allowPromoCode:
                        window.mvcVars.siteRegionConfig.allowPromoCode,
                    },
                    showPromo: true,
                    showFacebook: window.vs.showFacebookLogin,
                    registrationPixel:
                      window.pixel && window.pixel.fireRegistration,
                    modal: true,
                    geoCheck: function(onComplete) {
                      window.GC.verifyLocation(null, onComplete);
                    },
                    onRequestClose: function() {
                      dkjs.DKAuth.unmountDKAuth(mountElement);
                    },
                    trackEvent: window.trackEvent ? window.trackEvent : null,
                    trackSource:
                      'DKAuth_Modal_DTI_2016_PROMO_' + this.dataset.position,
                    defaultView: 'REGISTRATION',
                    host: window.mvcVars.apiHost,
                    tldHost: window.vs.locale.UK
                      ? window.vs.usSiteUrl
                      : window.vs.ukSiteUrl,
                    autoFocus: true,
                    successfulRegistration: function() {
                      window.GC.verifyLocation(null, function() {
                        window.location.assign(
                          '/account/depositnewvisitorinterstitial',
                        );
                      });
                    },
                    successfulLogin: function() {
                      window.GC.verifyLocation(null, function() {
                        window.location.assign('/lobby');
                      });
                    },
                    returnUrl: null,
                  };
                  dkjs.DKAuth.DKAuthStandalone({
                    mountElement: mountElement,
                    properties: properties,
                  });
                });
            });

            _.forEach(signUpLinks, function(elem) {
