                onSubmitRegistrationSuccess: function () {
                    window.location = "/account/depositnewvisitorinterstitial?ftue=1";
                }
            });

            $("a[data-lp-reg-promo=1]").signinRegModal({
                modalOptions: defaultFancyBox,
                popContainerSelector: "#signin-popup",
                isRegistrationPop: true,
                isInlineValidationEnabled: false,
