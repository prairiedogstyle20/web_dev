	var c = "mrpushbanner";

		if (canGetPushes(osxVersion)) {
			if ('safari' in window) {
				isSafari = true;
				if ('pushNotification' in window.safari) {
					permissionData = window.safari.pushNotification.permission(websitePushId);
					permission = safariPNUserGrantCallback(permissionData);

					if (permission == "default") {
						permCheckId = initialCheck();
					} else if (permission == "granted") {
						postCheckId = postCheck();
					} else if (permission == "denied") {
						postCheckId = postCheck();
						var cookie = readCookie(c);

						if (cookie && cookie == "hide") {
							clearInterval(postCheckId);
							$("#mr-pushbanner").css("display", "none");
						} else {
							$("#mr-pushbanner").css("display", "block");
							$("#mr-pushresubscribe").css("display", "block");
						}
					}
				}
			}

		$("#close-notif-banner").click(function(){
                	clearInterval(permCheckId);
                	clearInterval(postCheckId);
                	$("#mr-pushbanner").css("display", "none");
                	drop_cookie("hide");
        	});

		/*
        	$("#mr-pushresubscribe").click(function(e){
                	e.preventDefault();
                	safariRequestPerm();
        	});
		*/
	}

	function drop_cookie(value){
        	var days, date, expires;

                if(value == "show"){
                	value = ";";
			days = -1;
		}else{
			days = 30;
		}

		date = new Date();
		date.setTime(date.getTime()+(days*24*60*60*1000));
		expires = "; expires="+date.toGMTString();
		if(isSafari = true){
			document.cookie = "mrpushbanner=" + value+expires + "; domain=www.macrumors.com; path=/";
		}else{
			document.cookie = "mrpushbanner=" + value+expires + "; domain=.macrumors.com; path=/";
		}
	}
</script>
