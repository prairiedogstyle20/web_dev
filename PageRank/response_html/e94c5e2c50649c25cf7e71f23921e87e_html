<script>
  $(function() {
    $('.search .icon').on('click', function() {
      $('.social').toggleClass('hide');
      $('.primary > ul').toggleClass('hide remove');
      $('.primary .search').toggleClass('show');

      $('.search input').focus();
    });

	// 2018-11-09 - CJB - new menuing

	// ---- determine current page
	var strCurrentPage = null;
	if (window.location.pathname == '/') strCurrentPage = 'front';
	if (window.location.pathname == '/mac/') strCurrentPage = 'front';
	if (window.location.pathname == '/iphone/') strCurrentPage = 'front';
	if (window.location.pathname.match(/\/how-to\//)) strCurrentPage = 'how-to';
	if (window.location.pathname.match(/\/review\//)) strCurrentPage = 'review';
	if (window.location.pathname.match(/\/roundup\//)) strCurrentPage = 'roundups';
	if (window.location.hostname.match(/buyersguide/)) strCurrentPage = 'buyersguide';

	switch (strCurrentPage)
	{
		case 'front':
		default:
			$('li#front a.parent').addClass('active');
			break;

		case 'how-to':
			$('li.how-to a').addClass('active');
			break;

		case 'review':
			$('li.reviews a').addClass('active');
			break;

		case 'roundups':
		case 'buyersguide':
			break;
	}

	// ---- Front - begin
	$('#header li#front a.parent').on('click', function(e) {
		e.preventDefault();

		$('li#front ul#nav-dropdown').toggle();

		if (strCurrentPage != 'roundups')
	    	$('li#roundups a.parent').removeClass('active');
		$('li#roundups ul#nav-dropdown').hide();

		if($('ul#nav-dropdown').is(':visible')) {
       		$('li#front a.parent').addClass('active');
		} else {
			if (strCurrentPage != 'front')
				$('li#front a.parent').removeClass('active');
		}
	});

	// ---- Front - end

	// ---- Roundups - begin
	$('#header li#roundups a.parent').on('click', function(e) {
		e.preventDefault();

		$('li#roundups ul#nav-dropdown').toggle();

		if (strCurrentPage != 'front')
			$('li#front a.parent').removeClass('active');
		$('li#front ul#nav-dropdown').hide();

		$.each($('#nav-dropdown img.lazyb'), function (i, ele) {
			var $ele = $(ele);
			var contentSrc = $ele.attr('content-src');
			if (contentSrc) {
				$ele.attr('src', contentSrc);
				}
			})

		if($('ul#nav-dropdown').is(':visible')) {
			$('li#roundups a.parent').addClass('active');
		} else {
			if (strCurrentPage != 'roundups')
				$('li#roundups a.parent').removeClass('active');
		}
    });
	// ---- Roundups - end
    
  });
</script>

