/*DL format Cricket*/
var DLText = "", DLStatus = "", DLMatchId = "";
function loadDL() {
var url = 'https://cdn.ndtv.com/common/onemin/cricketfeed.json';
$.ajax({
url: url,
dataType: "jsonp",
callbackParameter: "callback",
jsonpCallback: "mycallback",
success: function (data) {
DLText = data.mainText;
DLStatus = data.status;
DLMatchId = data.titleText;
if (DLStatus == "1" && DLMatchId == 'sasl02132019186659') {
$('h1').after('<div style="font-size:16px;">( ' + DLText + ' )</div>');
}
},
error: function (data) { //return false;
}
});
}
loadDL();
</script>
 

