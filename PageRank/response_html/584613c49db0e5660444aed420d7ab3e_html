    var prebidjson = {"/1068322/NDTV_Sports_MatchCentre_728x90": {"code": "adslot728x90ATF","sizes": [728, 90],"bids": [{"bidder":"appnexus","params":{"placementId":"11534570"}},{"bidder":"rubicon","params":{"accountId":"11990","siteId":"42594","zoneId":"666202"}},{"bidder":"pubmatic","params":{"publisherId":"155977","adSlot":"ndtv_sports_728x90_prebid@728x90"}}]},"/1068322/NDTV_Sports_MatchCentre_300x250":{"code": "adslot300x250ATF","sizes": [300, 250],"bids": [{"bidder":"appnexus","params":{"placementId":"11534586"}},{"bidder":"rubicon","params":{"accountId":"11990","siteId":"42594","zoneId":"666202"}},{"bidder":"pubmatic","params":{"publisherId":"155977","adSlot":"ndtv_sports_300x250_prebid@300x250"}}]},"/1068322/NDTV_Sports_ROS_728x90_BTF":{"code": "adslot728x90BTF","sizes": [728, 90],"bids":[{"bidder":"appnexus","params":{"placementId":"11534595"}},{"bidder":"rubicon","params":{"accountId":"11990","siteId":"42594","zoneId":"666202"}},{"bidder":"pubmatic","params":{"publisherId":"155977","adSlot":"ndtv_sports_728x90_prebid@728x90"}}]}};
    var _prebidAllAdUnits = prebidjson;



    


    var adUnitsPrebid = Object.keys(_prebidAllAdUnits).map(function (key) {
        return _prebidAllAdUnits[key];
    });

    var isPrebidActive = adUnitsPrebid.length ? true : false;

    if (isPrebidActive) {
        googletag.cmd.push(function () {
            googletag.pubads().disableInitialLoad();
        });

        var pb_buckets = { "buckets": [{ "precision": 2, "min": 0, "max": 201, "increment": 0.67 }, { "precision": 2, "min": 201, "max": 536, "increment": 3.35 }, { "precision": 2, "min": 536, "max": 1340, "increment": 33.5 }] };

        pbjs.que.push(function () {

            pbjs.addAdUnits(adUnitsPrebid);
            pbjs.setConfig({ priceGranularity: pb_buckets });
            pbjs.setConfig({ bidderSequence: 'random' });
            pbjs.bidderSettings = {
                rubicon: {
                    bidCpmAdjustment: function (bidCpm, bid) {
                        var currencyRate = 67;
                        return bidCpm * currencyRate;
                    }
                },
                appnexus: {
                    bidCpmAdjustment: function (bidCpm, bid) {
                        var currencyRate = 67;
                        return bidCpm * currencyRate;
                    }
                },
                pubmatic: {
                    bidCpmAdjustment: function (bidCpm, bid) {
                        var currencyRate = 67;
                        return bidCpm * currencyRate * 0.92;
                    }
                }
            };
            pbjs.requestBids({
                bidsBackHandler: sendAdserverRequest
            });
        });

        function sendAdserverRequest() {
            if (pbjs.adserverRequestSent) {
                return;
            }
            pbjs.adserverRequestSent = true;
            googletag.cmd.push(function () {
                pbjs.que.push(function () {
                    pbjs.setTargetingForGPTAsync();
                    googletag.pubads().refresh();
                });
            });
        }

        setTimeout(function () {
            sendAdserverRequest();
        }, PREBID_TIMEOUT);
    }

