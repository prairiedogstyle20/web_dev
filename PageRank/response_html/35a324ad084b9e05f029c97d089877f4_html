               dataType: "text",
               cache: false,
               success : function (data) {
                if (data) {
                    $("#hdr-ll-cjci .song-ticker").html(data);
                    $(".time").css('display','none');

                    if ($('#hdr-ll-cjci p.artist').textWidth() + $('#hdr-ll-cjci .title').textWidth() >= '90') {
