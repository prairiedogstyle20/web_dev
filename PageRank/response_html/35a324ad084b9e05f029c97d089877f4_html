<script>
(function($){
    $('document').ready(function() {
        $.ajax({
            url : "/stream/recentplayed.php?s=CIRX&g=ROCK&l=1&nolist=true&ts="+ Date.now() +"",
               dataType: "text",
               cache: false,
               success : function (data) {
                if (data) {
                    $("#hdr-ll-cirx .song-ticker").html(data);
                    $(".time").css('display','none');

                    if ($('#hdr-ll-cirx p.artist').textWidth() + $('#hdr-ll-cirx .title').textWidth() >= '90') {
                        $('#hdr-ll-cirx .song-ticker').addClass('marquee');
                    }
                    //if ($('#hdr-ll-cirx .title').textWidth() >= '80') {
                    //  $('#hdr-ll-cirx .title').addClass('marquee');
                    //}

                }
            }
        });
    });
})(jQuery);
</script>

