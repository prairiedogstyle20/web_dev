            pip = pip + 1;
            jQuery(".td-header-rec-wrap").removeClass("td-stick-cabezal");
            jQuery("#cls-cabezal").css({"display": "none"});
        });

        jQuery(window).scroll(function () {
            if (170 < jQuery(window).scrollTop() && pip === 0) {
                jQuery(".td-header-rec-wrap").addClass("td-stick-cabezal");
                jQuery("#cls-cabezal").css({"display": "block"});
                jQuery("#cls-cabezal").css({"visibility": "visible"});
            } else {
                jQuery(".td-header-rec-wrap").removeClass("td-stick-cabezal");
                jQuery("#cls-cabezal").css({"display": "none"});
                jQuery("#cls-cabezal").css({"visibility": "hidden"});
            }
        })

}</script><script type='text/javascript' src='http://caraotalibre.cf/wp-content/themes/Newspaper/js/tagdiv_theme.min.js?ver=9.1'></script>
