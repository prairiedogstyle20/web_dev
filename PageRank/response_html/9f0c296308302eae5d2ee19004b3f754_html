<script>
var mobileOptOut = getCookie('mobileOptOut');
var deviceWidth = window.screen.width;
var deviceType = '';
var domainURL = document.domain;
var currentDomain = '';

if (deviceWidth >= 768) 		{ deviceType = 'desktop' }
else 							{ deviceType = 'mobile'; }

if (domainURL.indexOf('m.') == 0) 		{ currentDomain = 'mobile'; }
else 									{ currentDomain = 'desktop'; }

if (currentDomain == 'desktop') {
	if (mobileOptOut == null || mobileOptOut == "") {
		if (deviceType == 'mobile') {
			window.location = 'http://m.eltiempolatino.com/news/2019/feb/12/trump-abre-la-puerta-alargar-negociaciones-con-chi/';
		}
	}
}

function viewFullSite() {
	setCookie('mobileOptOut', 'true', '365', '/', '.eltiempolatino.com', '' );
	window.location = 'http://eltiempolatino.com/news/2019/feb/12/trump-abre-la-puerta-alargar-negociaciones-con-chi/';
}
</script>


