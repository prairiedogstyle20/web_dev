&q;ntv-contentd&q;,&q;params&q;:{&q;strnativekey&q;:&q;6kez5JUH4RdqUg4xdkwSNADy&q;}},&q;spon-logo&q;:{&q;slot_id&q;:&q;spon-logo&q;,&q;sizes&q;:[[120,40],[300,170],[300,100],[1,1]]},&q;mob-logo&q;:{&q;slot_id&q;:&q;mob-logo&q;,&q;sizes&q;:[[300,50],[300,75],[1,1],&q;fluid&q;]},&q;inread&q;:{&q;slot_id&q;:&q;inread&q;,&q;progressive&q;:true,&q;sizes&q;:[[600,575],[528,297],[1,1],&q;fluid&q;]},&q;topx&q;:{&q;slot_id&q;:&q;topx&q;,&q;sizes&q;:[[728,90],[970,250],[970,90],[970,66],[1,1],&q;fluid&q;],&q;criteoZoneId&q;:1168047,&q;incremental&q;:true},&q;artbottom&q;:{&q;slot_id&q;:&q;artbottom&q;,&q;sizes&q;:[[1,1],&q;fluid&q;]},&q;hero&q;:{&q;slot_id&q;:&q;hero&q;,&q;sizes&q;:[[1,1],[7,1]]}},&q;params&q;:{&q;author&q;:&q;danalexander&q;,&q;bbgterm&q;:false,&q;channel&q;:[&q;business&q;,&q;industry&q;,&q;lists&q;],&q;coverstory&q;:null,&q;displaychannel&q;:&q;wealth&q;,&q;displaysection&q;:&q;&q;,&q;ep&q;:&q;true&q;,&q;fl&q;:null,&q;fvid&q;:&q;&q;,&q;ht&q;:&q;TrumpsAmerica&q;,&q;id&q;:&q;blogAndPostId/blog/post/2282-3354&q;,&q;img&q;:null,&q;login&q;:false,&q;ns&q;:null,&q;section&q;:[&q;business:washington&q;,&q;industry:policy&q;,&q;lists:theworldsbillionaires&q;,&q;lists:worldsmostpowerfulpeople&q;],&q;specialslot&q;:&q;&q;,&q;templatetype&q;:&q;standard&q;,&q;type&q;:&q;forbesstaff&q;},&q;isEurope&q;:false},&q;hasWpCookies&q;:false,&q;/forbesapi/promote/get.json?location=header&a;type=map&a;all=true&q;:{&q;promotedContent&q;:{&q;headersubnavdogelink&q;:{&q;channel&q;:&q;channel_0&q;,&q;section&q;:&q;section_0&q;,&q;location&q;:&q;headersubnavdogelink&q;,&q;contentPositions&q;:[{&q;position&q;:1,&q;type&q;:&q;link &q;,&q;title&q;:&q;The Little Black Book of Billionaire Secrets&q;,&q;uri&q;:&q;http://bit.ly/2fTiz7H&q;,&q;hideDescription&q;:false,&q;fullImage&q;:false,&q;sponsored&q;:false,&q;removeTopPadding&q;:false,&q;removeBottomPadding&q;:false}],&q;more&q;:false,&q;enableAds&q;:false,&q;removeBVPrepend&q;:false,&q;brandvoiceHeader&q;:false,&q;removePadding&q;:fals