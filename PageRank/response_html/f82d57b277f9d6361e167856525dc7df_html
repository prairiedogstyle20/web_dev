                .asset-paging .direction {
                    color: #48c4d3;
                }
            </style><div class="asset-paging-container hidden-print">
            <div id="asset-paging-b490d764-2fcd-11e9-9de4-8ffa89068236" class="asset-paging">
                <div id="paging-prev-b490d764-2fcd-11e9-9de4-8ffa89068236" class="paging-link prev offset"></div>
                <div id="paging-next-b490d764-2fcd-11e9-9de4-8ffa89068236" class="paging-link next offset"></div>
