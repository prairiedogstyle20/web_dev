                        <script>
                            jQuery(document).ready(function($){
                                $('#followed-notification-modal-34f4908c-8b22-11e2-8a2a-001a4bcf6878-form').submit(function(e) {
                                    e.preventDefault();
                                    var $followBtn = $(this).find('.follow-btn');
                                    var sAction = $(this).attr('action') + '&title=' + $(this).find('.input-notification-name').val();
                                    $followBtn.find('.tnt-svg').show();
                                    $followBtn.prop('disabled', true);

                                    function submit_finished($alert){
                                        setTimeout(function(){
                                            $followBtn.find('.tnt-svg').hide();
                                            $alert.show().delay(2500).fadeOut(400);
                                        },2000);
                                    }

                                    $.ajax({
                                        type: 'POST',
                                        url: sAction,
                                        data: $(this).serialize(),
                                        success: function(oData){
                                            
                                                // Track follow event
                                                __tnt.trackEvent({
                                                    'network': 'Site',
                                                    'socialAction': 'Follow',
                                                    'url': 'https://www.kmaland.com/users/profile/34f4908c-8b22-11e2-8a2a-001a4bcf6878'
                                                });
                                            
                                            submit_finished( $('#followed-notification-modal-34f4908c-8b22-11e2-8a2a-001a4bcf6878-user .alert-success') );
                                        },
                                        error: function(oData){
                                            submit_finished( $('#followed-notification-modal-34f4908c-8b22-11e2-8a2a-001a4bcf6878-user .alert-danger') );
                                        }
                                    });
                                });
                            });
                        </script>
