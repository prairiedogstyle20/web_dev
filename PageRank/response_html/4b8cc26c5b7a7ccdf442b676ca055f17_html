function loadComments258309() {
  var xhttp = new XMLHttpRequest();
  var u = encodeURIComponent(document.location);
  xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
        document.getElementById("commentsBox258309").innerHTML = xhttp.responseText;
        reloadCaptcha258309();
    }
  };
  xhttp.open("GET", "//d1.niezalezna.pl/ajax/comments-short.php?u=" + Base64.encode(u) + "&id=258309", true);
  xhttp.withCredentials = true;
  xhttp.send();
} 

