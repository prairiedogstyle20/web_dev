function loadComments260621() {
  var xhttp = new XMLHttpRequest();
  var u = encodeURIComponent(document.location);
  xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
        document.getElementById("commentsBox260621").innerHTML = xhttp.responseText;
        reloadCaptcha260621();
    }
  };
  xhttp.open("GET", "//d1.niezalezna.pl/ajax/comments-short.php?u=" + Base64.encode(u) + "&id=260621", true);
  xhttp.withCredentials = true;
  xhttp.send();
} 

function reloadCaptcha260621() {
    if (captchaLoad) {
        grecaptcha.render( 'reCaptcha260621', {
            'sitekey' : '6Ld_xisUAAAAAB08kkTdcUR-e98vC6epUADYJEcg'
        });     
    } else {
        setTimeout(function(){
            reloadCaptcha260621();
        },1000);
    }
}

loadComments260621();

function commentPlus(id) {
  var xhttp = new XMLHttpRequest();
  var u = encodeURIComponent(document.location);
  xhttp.onreadystatechange = function() {
    if (xhttp.readyState == 4 && xhttp.status == 200) {
     if (xhttp.responseText=='fail'){
        //infoboxShow('Już oceniłeś ten komentarz.');
     } else {
        //infoboxShow('Do Twojego konta dodaliśmy 1 R za ocenę komentarza');
        document.getElementById('comment-up-'+id).innerHTML = xhttp.responseText;   
     }
    }
  };
  xhttp.open("GET", "//d1.niezalezna.pl/ajax/ajax-komentarze-ocena.php?u=" + Base64.encode(u) + "&id="+id+"&a=plus", true);
  xhttp.withCredentials = true;
  xhttp.send();
}

