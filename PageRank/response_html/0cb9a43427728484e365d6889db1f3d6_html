function janrainSignOut() {
	janrain.capture.ui.endCaptureSession();
}
(function() {
	if ( typeof window.janrain !== 'object' ) {
		window.janrain = {};
	}
	window.janrain.settings = {};
	window.janrain.settings.capture = {};

	// capture settings
