                <script>
                    function follow(modelid){
                        var modelid;
                        $('#follow' + modelid).remove();
                        $('#followsection' + modelid).load('index.php?m=entertainment&d=follow&id=' + modelid + '&ajaxload=1&theonepage=1');
                    }

                    function unfollow(modelidunf){
                        var modelidunf;
                        $('#unfollow' + modelidunf).remove();
                        $('#followsection' + modelidunf).load('index.php?m=entertainment&d=unfollow&id=' + modelidunf + '&ajaxload=1&theonepage=1');
                    }

                    function like(itemid){
                        var itemid;

                        $('#likes' + itemid).addClass('liked');
                        $('#likes' + itemid).attr('onclick', 'unlike(' + itemid + ')');
                        $('#likescount' + itemid).load('index.php?m=entertainment&d=like&id=' + itemid + '&ajaxload=1');
                        $('#unlikescount' + itemid).load('index.php?m=entertainment&d=undislike&id=' + itemid + '&ajaxload=1');
                        $('#unfollow' + itemid).removeClass();
                        $('#unfollow' + itemid).attr('onclick', 'dislike(' + itemid + ')');
                    }
                    function unlike(itemremoveid){
                        var itemremoveid;
                        $('#likes' + itemremoveid).removeClass();
                        $('#likes' + itemremoveid).attr('onclick', 'like(' + itemremoveid + ')');
                        $('#likescount' + itemremoveid).load('index.php?m=entertainment&d=unlike&id=' + itemremoveid + '&ajaxload=1');
                    }

                    function dislike(itemid){
                        var itemid;
                        $('#unfollow' + itemid).addClass('unliked');
                        $('#unfollow' + itemid).attr('onclick', 'undislike(' + itemid + ')');
                        $('#unlikescount' + itemid).load('index.php?m=entertainment&d=dislike&id=' + itemid + '&ajaxload=1');
                        $('#likescount' + itemid).load('index.php?m=entertainment&d=unlike&id=' + itemid + '&ajaxload=1');
                        $('#likes' + itemid).removeClass();
                        $('#likes' + itemid).attr('onclick', 'like(' + itemid + ')');

                    }
                    function undislike(itemremoveid){
                        var itemremoveid;
                        $('#unfollow' + itemremoveid).removeClass();
                        $('#unfollow' + itemremoveid).attr('onclick', 'dislike(' + itemremoveid + ')');
                        $('#unlikescount' + itemremoveid).load('index.php?m=entertainment&d=undislike&id=' + itemremoveid + '&ajaxload=1');
                    }
                    
                </script>

            

