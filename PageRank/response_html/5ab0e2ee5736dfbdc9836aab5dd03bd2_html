    <script>
      $("#full-menu-trigger").ready(function() {
        var timer;
        var mTrigger = [$("#full-menu-trigger"), $("#full-menu")];
        $.each(mTrigger, function() {
          $(this).mouseleave(function() {
            timer = setTimeout(function() {
              $("#full-menu").slideUp(200);
            }, 200);
          });
          $(this).mouseenter(function() {
            clearInterval(timer);
            $("#full-menu").slideDown(200);
          });
        });
      });
      
      $("#touch-menu-trigger").click(function() {
        $("#full-menu").slideToggle(200, function() {
          if($(".top-search").css("display") == "block") {
            $(".top-search").slideUp(200);
          }
        });
      });
      
      $(".search-button").click(function() {
        $(".top-search").slideToggle(200, function() {
          if($(".top-search").css("display") == "block") {
            $("#full-menu").slideUp(200);
            $(".top-search").find("input").focus();
          }
        });
      });
      
      $(".top-search").find(".close-search").click(function() {
        $(".top-search").slideUp(200);
      });
      
      
      var hasStickyTopHeader = true;
    </script>
