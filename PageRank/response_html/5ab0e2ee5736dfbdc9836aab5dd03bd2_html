          <script>
            function PopupCenter(url, title, w, h) {
              // Fixes dual-screen position                         Most browsers      Firefox
              var dualScreenLeft = window.screenLeft != undefined ? window.screenLeft : window.screenX;
              var dualScreenTop = window.screenTop != undefined ? window.screenTop : window.screenY;
            
              var width = window.innerWidth ? window.innerWidth : document.documentElement.clientWidth ? document.documentElement.clientWidth : screen.width;
              var height = window.innerHeight ? window.innerHeight : document.documentElement.clientHeight ? document.documentElement.clientHeight : screen.height;
            
              var left = ((width / 2) - (w / 2)) + dualScreenLeft;
              var top = ((height / 2) - (h / 2)) + dualScreenTop;
              var newWindow = window.open(url, title, 'scrollbars=yes, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);
            
              // Puts focus on the newWindow
              if (window.focus) {
                  newWindow.focus();
              }
            }
          </script>
        </div>
      </div>
    </div>
    <script>
      function PopupCenter(url, title, w, h) {
        // Fixes dual-screen position                         Most browsers      Firefox
        var dualScreenLeft = window.screenLeft != undefined ? window.screenLeft : window.screenX;
        var dualScreenTop = window.screenTop != undefined ? window.screenTop : window.screenY;
      
        var width = window.innerWidth ? window.innerWidth : document.documentElement.clientWidth ? document.documentElement.clientWidth : screen.width;
        var height = window.innerHeight ? window.innerHeight : document.documentElement.clientHeight ? document.documentElement.clientHeight : screen.height;
      
        var left = ((width / 2) - (w / 2)) + dualScreenLeft;
        var top = ((height / 2) - (h / 2)) + dualScreenTop;
        var newWindow = window.open(url, title, 'scrollbars=yes, width=' + w + ', height=' + h + ', top=' + top + ', left=' + left);
      
        // Puts focus on the newWindow
        if (window.focus) {
            newWindow.focus();
        }
      }
      var env = "https://api.elfinanciero.com.mx/public"
        $.ajax({
          url: env+'/notas/breaking-news',
          method: 'GET',
          dataType: 'JSON',
        }).done(function(response){
          var data = response.data[0]
          var slug = '/'+data.slug;
          var facebookSlug = 'https://www.facebook.com/sharer/sharer.php?u=https://elfinanciero.com.mx' + slug;
          var twitterSlug = 'https://twitter.com/intent/tweet?url=https://elfinanciero.com.mx' + slug + '&text=' + encodeURIComponent(data.title) + '&via=ElFinanciero_Mx';
          var whatsSlug = 'whatsapp://send?text=https://elfinanciero.com.mx' + slug;
          $(".header-breaking-news").addClass("there-is-breaking");
          $(".header-breaking-news .column.is-11 .slug").html(data.title);
          $("a[href='breaking']").attr('href', slug);
          $("a[href='twitter']").attr('href', twitterSlug);
          $("a[href='facebook']").attr('href', facebookSlug);
          $("a[href='whatsapp']").attr('href', whatsSlug);
        }).fail(function(response){
          console.log();
        });
      
    </script>
    <div class="header-breaking-news">
      <div class="container">
        <div class="columns is-multiline is-mobile">
          <div class="column is-11"><a class="slug" href="breaking">Breaking News</a><a class="break-twitter social" href="twitter" target="_blank"><i class="fab fa-twitter"></i></a><a class="break-facebook social" href="facebook" target="_blank"><i class="fab fa-facebook-square"></i></a><a class="break-whatsapp social is-hidden-tablet" href="whatsapp" target="_blank"><i class="fab fa-whatsapp-square"></i></a></div>
          <div class="column is-1 has-text-right"><a class="close-bn"><i class="fas fa-times"></i></a>
            <script>
              $(".close-bn").click(function() {
              $(".header-breaking-news").slideUp(200);
              });
              
              
              
            </script>
          </div>
        </div>
      </div>
