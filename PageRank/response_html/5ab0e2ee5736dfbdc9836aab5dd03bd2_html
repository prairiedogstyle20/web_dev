    <script>
      $(document).ready(function() {
        // Check for true or false hasStickyTopHeader; in Note scroll it's false
        if(hasStickyTopHeader) {
          var headTime = 1000;
          setTimeout(function() {
            var stickyHeader = UIkit.sticky("#top-header");
          }, headTime);
        };
        $(".adaptive-iframe").on('load', function () {
          $(this).iFrameResize({
            checkOrigin:false,
            heightCalculationMethod: 'taggedElement'
          });
        });
      
        // Drop Caps
        var dropcap = $(".drop-cap");
        $.each(dropcap, function(c) {
          window.Dropcap.layout(this, 3);
          this.style.fontFamily = "Geomanist";
          this.style.fontWeight = "300";
          this.style.paddingRight = "0.75rem";
          this.style.letterSpacing = "-0.25rem";
          dropcap.textContent = c;
        });
      
        $(document).find(".section.is-article").each(function() {
          // Video main element to sidebar
          if($(this).find(".page__main-element").find(".video-holder").length > 0) {
            $(this).find(".sidebar__inner").prepend("<div class='side-video-holder is-16-9'></div>");
            var videoHolder = $(this).find(".video-holder").first();
            var sideVideoHolder = $(this).find(".side-video-holder");
            var videoPlayer = videoHolder.find(".video-player").first();
            var video = videoPlayer.find("video").first();
            var isFS;
            var state;
            var whereAmI = "b";
            var viewportWidth;
            $(window).bind("scroll", function() {
              viewportWidth = $(window).width();
              if(viewportWidth >= 1008) {
                if($(this).scrollTop() >= videoHolder.offset().top + (videoHolder.outerHeight() / 2) && document.webkitCurrentFullScreenElement == null) {
                  if(whereAmI != "s") {
                    sideVideoHolder.slideToggle();
                    state = video[0].paused;
                    videoPlayer.animate({
                      top: -videoHolder.outerHeight() / 2,
                      opacity: 0
                    }, function() {
                      videoPlayer.prependTo(sideVideoHolder).animate({
                      top: 0,
                      opacity: 1
                      }, 200, function() {
                        if(!state) {
                          video[0].play();
                        }
                      });
                    });
                    whereAmI = "s";
                  }
                } else if($(this).scrollTop() < videoHolder.offset().top + (videoHolder.outerHeight() / 2) && document.webkitCurrentFullScreenElement == null){
                  if(whereAmI != "b") {
                    state = video[0].paused;
                    videoPlayer.animate({
                      top: -sideVideoHolder.outerHeight() / 2,
                      opacity: 0
                    }, function() {
                      videoPlayer.prependTo(videoHolder).animate({
                      top: 0,
                      opacity: 1
                      }, 200, function() {
                        if(!state) {
                          video[0].play();
                        }
                      });
                    });
                    sideVideoHolder.slideToggle();
                    whereAmI = "b";
                  }
                }
              }
            });
          }
        });
      });
    </script>
    <script>
      $('.search-field').on('keypress', function (e) {
        var self=$(this);
        if (e.which === 13) {
          var value = self.val();
          window.location = '/search/'+value;
        }
      });
    </script>
