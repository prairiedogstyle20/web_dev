    <script>var pageId = 617996;
      var cat = "Mundo";
      
      $(function () {
        var _vastDFP = function(URL_VAST){
          var url = URL_VAST;
          $.each($('.section.is-article' + '#' + pageId).find('video'), function(index, value){
            var thisID = $(this).attr('id');
            var player1 = new VAST.player(thisID, url);
            player1.init();
          });
        }
      
        var vastPl = VAST.initTAG( cat );
        _vastDFP( vastPl );
      
        $('.vjs-progress-control.vjs-control').removeClass('vjs-progress-control vjs-control');
        var EF = window.EF;
        hasStickyTopHeader = false;
      
        // -----------------------------------------------
        // Begin full menu and search
        // -----------------------------------------------
      
        var articleFullMenu = $(".article-top-header").find(".full-menu");
        $("#article-full-menu-trigger").ready(function() {
          var timer;
          var mTrigger = [$("#article-full-menu-trigger"), articleFullMenu];
          $.each(mTrigger, function() {
            $(this).mouseleave(function() {
              timer = setTimeout(function() {
                articleFullMenu.slideUp(200);
              }, 200);
            });
            $(this).mouseenter(function() {
              clearInterval(timer);
              articleFullMenu.slideDown(200);
            });
          });
        });
      
        $("#article-touch-menu-trigger").click(function() {
          articleFullMenu.slideToggle(200, function() {
            if($(".article-top-search").css("display") == "block") {
              $(".article-top-search").slideUp(200);
            }
          });
        });
      
        $(".article-search-button").click(function() {
          $(".article-top-search").slideToggle(200, function() {
            if($(".article-top-search").css("display") == "block") {
              articleFullMenu.slideUp(200);
              $(".article-top-search").find("input").focus();
            }
          });
        });
        $(".article-top-search").find(".close-search").click(function() {
          $(".article-top-search").slideUp(200);
        });
        // --------------------------------------------------
        // ^^^^^^^ End full menu and search ^^^^^
        // --------------------------------------------------
      
        var sectionSlug = "mundo";
        var lastNote = "mercados/el-dolar-avanza-frente-al-peso-tras-testimonio-de-powell-en-el-senado-de-eu";
        var categorySection = "Mundo".toUpperCase();
        var noteIndex = 0;
        var isLoading = false
        var isMobile = EF.client.isMobile
      
        //Get array with notes for scrolling
        var scrollInfiniteNotes = ["mundo/imagenes-de-venezolanos-comiendo-basura-molestaron-a-maduro-jorge-ramos","mundo/univison-acusa-retencion-de-periodistas-que-entrevistaban-a-maduro-en-venezuela","mundo/espana-es-el-pais-mas-sano-del-mundo-y-mexico","economia/asi-es-como-pemex-se-convirtio-en-la-petrolera-mas-endeudada-del-mundo","economia/aumento-en-gasto-publico-e-impulso-a-pemex-ponen-en-riesgo-calificacion-crediticia-de-mexico-moodys","mundo/venezuela-deporta-a-periodistas-de-univision-encabezados-por-jorge-ramos","mundo/con-barricadas-y-enmascarados-simpatizantes-de-maduro-agreden-a-civiles-en-frontera-con-colombia","nacional/comision-de-puntos-constitucionales-aprueba-dictamen-sobre-la-guardia-nacional","mercados/el-dolar-avanza-frente-al-peso-tras-testimonio-de-powell-en-el-senado-de-eu"]
        var totalNotes = scrollInfiniteNotes.length
        var $placeholder
        var site = "https://elfinanciero.com.mx/";
      
        //Full Width FB Comments
        $(function(){
          $.each( $('.fb-comments'), function(){
            $(this).find('iframe').css('width','100%');
          })
        });
      
        var showLoading = function() {
          $('.article-break').last().after('<div class="load-placeholder has-text-centered" style="background: white;">')
          $placeholder = $('.load-placeholder')
          $placeholder.html('<img src="/resources/images/loading.svg">');
        }
      
        EF.loadedNotes = 0
        EF.totalNotes = totalNotes
        var showNote = function (note) {
          EF.loadedNotes++
          $placeholder.replaceWith(note)
          isLoading = false
          window.ads && window.ads.ready()
          EF.ads.loadInread(sectionSlug)
          return note
        }
      
        var comscoreTrack = function (note) {
          var name = $(note)
            .data('slug')
            .replace(/\//gi, '.')
             autor = $(note)
            .data('autor')
          self.COMSCORE && COMSCORE.beacon({
            'c1': '2',
            'c2': '10181342',
            'ns_site': 'elfinanciero',
            'autor': autor,
            'name': name,
            'scroll': noteIndex
          });
          var rand = Math.floor(Math.random() * 10000)
          $.get('/comscore_view.txt?' + rand)
        }
      
        var getNoteFromScroll = function() {
          if (noteIndex < totalNotes && sectionSlug !== "sponsor") {
            showLoading()
            var query = [
              '/'+scrollInfiniteNotes[(noteIndex++)]+'?_format=compact',
              'lastNote='+lastNote,
              'sectionZero='+sectionSlug,
              'pos='+(noteIndex+1)
            ]
            $.get(query.join('&'))
              .then(showNote)
              .then(comscoreTrack)
              .fail(function (error){
                console.error('Error al cargar nota', error);
              });
          } else {
            console.log('Ultima nota');
          }
        }
      
        var $header = $('#article-head')
        var $articleHeader = $('.article-top-header')
        $(window).on('scroll resize', function () {
      
          // infinite scroll
          if ((window.innerHeight + window.scrollY ) >= (document.body.offsetHeight - 500) && !isLoading){
            isLoading = true
            getNoteFromScroll()
          }
      
          var $headerOffset = $header.offset()
          $('.section').each(function () {
            var $this = $(this)
      
            // Changeable box banner in sidebar
            if (!isMobile) {
              var railCol = $this.find(".sidebar");
              var trainCol = railCol.find(".sidebar__inner");
              var railHeight = railCol.height();
              var trainColDist = trainCol.offset().top - railCol.offset().top;
      
              // Test ad position
              if (trainColDist < railHeight / 2) {
                trainCol.find("button[data-uk-slideshow-item='0']").click();
              } else {
                trainCol.find("button[data-uk-slideshow-item='1']").click();
              }
            }
      
            // Test section change
            var $thisOffset = $this.offset()
            if ($headerOffset.top >= $thisOffset.top && $headerOffset.top <= $this.next().offset().top) {
              var newDescr = $this.find('h1').text();
              var newUrl = $this.data('slug');
              var oldDescr = $header.text();
      
              if (newDescr !== oldDescr) {
      
                window.history.replaceState(null, null, '/' + newUrl);
      
                $articleHeader
                  .find('.noteShareFacebook').attr({'href':'javascript:void(0);','onclick':'window.open("https://www.facebook.com/sharer/sharer.php?u='+site+newUrl+'","sharer","toolbar=0,status=0,width=548,height=325")'}).end()
                  .find('.noteShareTwitter').attr({"target":"_blank", "href":"https://twitter.com/intent/tweet?url="+site+newUrl+"&text="+encodeURIComponent(newDescr)}).end()
                  .find('.noteShareLinkedin').attr({'href':'javascript:void(0);','onclick':'window.open("https://www.linkedin.com/shareArticle?mini=true&url='+site+newUrl+'","sharer","toolbar=0,status=0,width=548,height=325")'}).end()
                  .find('.noteShareGooglePlus').attr({'href':'javascript:void(0);','onclick':'window.open("https://plus.google.com/share?url='+site+newUrl+'","sharer","toolbar=0,status=0,width=450,height=525")'}).end()
                  .find('.noteShareWhatsapp').attr('href', "whatsapp://send?text=https://elfinanciero.com.mx/" + newUrl)
      
      
                if (!isMobile) {
                  $header
                    .html(newDescr)
                    .css('opacity', 0.4)
                    .animate({ 'opacity': '1' }, 200);
                }
      
                document.title = newDescr;
                return;
              }
            }
          });
        });
      
        // load inreads
        EF.ads.loadInread(sectionSlug)
      });
    </script>
