                                if (add)
                                {
                                    if (classIndex === -1)
                                    {
                                        return classNameList + " " + className;
                                    }
                                }
                                else if (classIndex >= 0)
                                {
                                    return classNameList.replace(className, "");
                                }
                                return classNameList;
                            }

