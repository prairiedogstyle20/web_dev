                    found = !!(target.segments.find(function (segment) {
                        return aamArray.indexOf(segment) !== -1;
                    }));
                }
                
                window._sp_.mms.setTargeting(target.key, found);
            });
        }
    });

    
    window._sp_.bootstrap('//7q1z79gxsi.global.ssl.fastly.net/7q1z79gxsi-m.js');
    window._sp_.bootstrap('//7q1z79gxsi.global.ssl.fastly.net/7q1z79gxsi-c.js');
})();
