<script type="text/javascript">
function url_redirect() {
  var search_type   = document.forms.form_search.elements.search_category.value;
  var search_word   = encodeURI(document.forms.form_search.elements.MT.value);
  var input_encode  = encodeURI(document.forms.form_search.elements.IE.value);
  var output_encode = encodeURI(document.forms.form_search.elements.OE.value);
  var redirect_url  = '';

  if (search_type === '1') {
      redirect_url = 'https://search.goo.ne.jp/web.jsp?MT=' + search_word + '&STYPE=web&IE=UTF-8&rank=0&from=news_tp&PT=news_tp';
  } else {
      redirect_url = 'https://search.goo.ne.jp/web.jsp?MT=' + search_word + '&mode=2&IE=' + input_encode + '&OE=' + output_encode + '&from=news_header';
  }
  document.location = redirect_url;
}
</script>
