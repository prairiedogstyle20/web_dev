var chosenVariation = cxApi.chooseVariation();

var pageVariations = [
    function(){

      jQuery('#wpfront-notification-bar .wpfront-button').on('click', function(e) {
        e.preventDefault();
        ga('send','event','Slidedown','click','Petition CTA', {
            hitCallback: function() {
                window.location.href="https://liveaction.org/petition/?utm_source=liveactionnews&utm_medium=referral&utm_campaign=defundpetition&utm_content=slidedown";
            }
        });
      });

    },
    function(){

        // Hide Slide down for Variation 1
        jQuery( '<style>#wpfront-notification-bar { display: none !important; }</style>' ).appendTo('head');

        // Step 1 append Modal
        jQuery('<div class="na-modal" style="display: none;"><h4>Hold on…</h4><p>Before you go, would you <strong>sign the petition to help end taxpayer funding of the nation’s largest abortion provider?</strong> Planned Parenthood performs almost 900 abortions a day. We must put a stop to this.</p><a href="https://liveaction.org/petition/?utm_source=liveactionnews&utm_medium=referral&utm_campaign=defundpetition&utm_content=exit_intent" class="na-modal-btn">Sign the Petition Today</a><a href="#" class="na-modal-close"><i class="fa fa-times" aria-hidden="true"></i></a></div><div class="na-cover" style="display: none;"></div>').appendTo('body');

        // Step 2 initialize event handlers
        jQuery('.na-modal-close').on('click', function(e) {
            e.preventDefault();
            jQuery('.na-modal, .na-cover').fadeOut('fast');
        });

        jQuery('.na-modal-btn').on('click', function(e) {
            e.preventDefault();
            ga('send','event','Exit Modal','click','Petition CTA', {
                hitCallback: function() {
                    window.location.href="https://liveaction.org/petition/?utm_source=liveactionnews&utm_medium=referral&utm_campaign=defundpetition&utm_content=exit_intent";
                }
            });
        });

        // Step 3 Enable Exit Intent
        var modal = ouibounce(jQuery('.na-modal')[0], {
            cookieName:'na-exit',
            callback : function(){ jQuery('.na-cover').fadeIn('fast'); },
        });

    }
];

if (document.readyState === "interactive" || document.readyState === "complete") {
    pageVariations[chosenVariation]()
} else {
    document.addEventListener("DOMContentLoaded", pageVariations[chosenVariation]());
}

</script>

