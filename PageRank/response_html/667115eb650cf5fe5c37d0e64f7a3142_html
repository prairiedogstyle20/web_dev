</div><div class="custom-sidebar widget_text" id="text-73"><div class="custom-sidebar-title-wrapper"><h3 class="custom-sidebar-title gdl-border-x bottom">Scripts</h3><i class="icon-double-angle-right"></i><div class="clear"></div></div> <div class="textwidget"><script type='text/javascript'>
	(function($) {

		console.log( 'mashable header == ' + status );
	
		if( status == 'on' ){
		
			console.log('on');
			$('#mash-banner').show();
	
			if ( main_background != '' ){
				$('#mash-banner').css('background','url("' + main_background + '") no-repeat');
			}
			
			/* set side story 1 info */
			
			if( side_story_1_title != '' && side_story_1_url != '' ){
				$('.side-stories .s2 h1').html( side_story_1_header )
				$('.side-stories .s2 h2').html( '<a href="' + side_story_1_url + '">' + side_story_1_title + '</a>' );
			}
			
			/* set side story 2 info */
			
			if( side_story_2_title != '' && side_story_2_url != '' ){
				$('.side-stories .s3 h1').html( side_story_2_header )
				$('.side-stories .s3 h2').html( '<a href="' + side_story_2_url + '">' + side_story_2_title + '</a>' );
			}
			
			/* set side story 3 info */
			
			if( side_story_3_title != '' && side_story_3_url != '' ){
				$('.side-stories .s4 h1').html( side_story_3_header )
				$('.side-stories .s4 h2').html( '<a href="' + side_story_3_url + '">' + side_story_3_title + '</a>' );
			}
			
			/* set main story info */
			
			if( main_story_title != '' && main_story_url != '' ){
				$('.main-story .s1 h1').html( main_story_header );
				$('.main-story .s1 h2').html( '<a href="' + main_story_url + '">' + main_story_title + '</a>' );
			}
			
		}
		
		console.log('off');
		
	})( jQuery );
</script></div>
