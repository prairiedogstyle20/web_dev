    <script>
        var loaderCheckFinished = setInterval(function () {
            var deferBlock = document.getElementsByClassName("defer-block");
            if ((deferBlock.length > 0 && window.getComputedStyle(deferBlock[0])["display"] === "block") || deferBlock.length === 0) {
                // Stops the load spinner once the deferBlock CSS (in grid) is loaded.
                var hf = document.getElementById('hide-footer');
                if (hf !== null) {
                    hf.style.display = "block";
                }
                clearInterval(loaderCheckFinished);
            }
        }, 100);
    </script>

    

