    <script id="creInit">
        (function(jq){
            var downloadScript = function(url, callback) {
                jq.ajax({
                    url: url,
                    dataType: 'script',
                    cache: true, // otherwise will get fresh copy every page load
                    success: callback
                })
            }

            var renderSwoosh = function(opcUrl, meteredCount, maxCount) {
                var ajaxUrl = opcUrl + '/assets/js/swoosh.js';
                var mode = 'WWW';
                downloadScript(ajaxUrl, function() {
                    // Wait for the document to be ready before the swoosh is rendered
                    jq(document).ready(function() {
                        opcSwoosh(jq, jq("#sueddeutsche"), meteredCount, maxCount, opcUrl, mode);
                    });
                });
            }

            var creListener = function(response){
                if (typeof(dataLayer) !== 'undefined') {
                    dataLayer.push({'event': 'c1TrackingResponse', 'c1TrackingResponse': response});
                }

                                    var data = response.data;

                                            // Render Swoosh
                        if (data.type == 'reminder') {
                            var read = Math.min(data.max_count, data.count);
                            renderSwoosh("https://offers.sueddeutsche.de", read, data.max_count);
                        }
                    
                    if (data.user_group_actions && data.user_group_actions.cms_snippet && data.user_group_actions.cms_snippet.parameter) {
                        var snippet = data.user_group_actions.cms_snippet.parameter;
                        if (SDE.C1 && SDE.C1.UserGroupActionProcessor) {
                            SDE.C1.UserGroupActionProcessor.processSnippet(snippet);
                        }
                    }
                            };

            downloadScript('https://www.sueddeutsche.de/cre-1.0/tracking/tracking.js', function() {
                if (typeof(cre_client) != "undefined") {
                    cre_client.set_page_view();
                                            cre_client.set_site('sz.de');
                                            cre_client.set_cms_id('1.4326472');
                                            cre_client.set_content_id('\/politik\/trump-mauer-mexiko-1.4326472');
                                            cre_client.set_heading('Trump im Rumpelstilzchen-Modus');
                                            cre_client.set_entitlement_id('live_szde');
                                            cre_client.set_origin('web');
                                            cre_client.set_service_id('szde');
                                            cre_client.set_channel('2.236');
                                            cre_client.set_entitlement('metered');
                                            cre_client.set_doc_type('article');
                                            cre_client.set_tag('Politik_USA');
                                            cre_client.set_kicker('Rede in El Paso');
                                        cre_client.addListener(creListener);
                    cre_client.request();
                }
            });
        })(SDE.$);
    </script>
  
  
      

      
  <script>
    /**
     * Object holding information regarding user settings on SZ.de
     */
    SdeSettings =  {};

    /** Bit mask to signal that ad tracking has been disabled */
    SdeSettings._AD_TRACKING_DISABLED_MASK = 1;
    /** Bit mask to signal that video tracking has been disabled */
    SdeSettings._VIDEO_TRACKING_DISABLED_MASK = 2;

    /**
     * Private method reading the value of the cookie 'SdeDisabled'
     */
    SdeSettings._getSdeDisabledValue = function () {
        var value = document.cookie.replace(/(?:(?:^|.*;\s*)SdeDisabled\s*\=\s*([^;]*).*$)|^.*$/, "$1");
        value = parseInt(value, 10);
        return isNaN(value) ? 0 : value;
    };

    /**
     * Private method setting the value of the cookie 'SdeDisabled'
     */
    SdeSettings._setSdeDisabledValue = function (value) {
        var expiryDate = new Date();
        expiryDate.setFullYear(expiryDate.getFullYear() + 10);
        document.cookie = "SdeDisabled=" + value + ";expires=" + expiryDate + ";domain=.sueddeutsche.de;path=/";
    };

    /**
     * Private method checking the value of the cookie 'SdeDisabled' against the given mask.
     */
    SdeSettings._isDisabled = function (mask) {
        return Boolean(SdeSettings._getSdeDisabledValue() & mask);
    };

    /**
     * Private method adding the given mask to the value of the 'SdeDisabled'
     */
    SdeSettings._disable = function (mask) {
        if (!SdeSettings._isDisabled(mask)) {
            SdeSettings._setSdeDisabledValue(SdeSettings._getSdeDisabledValue() + mask);
        }
    };

    /**
     * Private method removing the given mask from the value of the 'SdeDisabled'
     */
    SdeSettings._enable = function (mask) {
        if (SdeSettings._isDisabled(mask)) {
            SdeSettings._setSdeDisabledValue(SdeSettings._getSdeDisabledValue() - mask);
        }
    };

    /**
     * Returns true if ad tracking has been disabled by the user
     */
    SdeSettings.isAdTrackingDisabled = function () {
        return SdeSettings._isDisabled(SdeSettings._AD_TRACKING_DISABLED_MASK);
    };

    /**
     * Returns true if video tracking has been disabled by the user
     */
    SdeSettings.isVideoTrackingDisabled = function () {
        return SdeSettings._isDisabled(SdeSettings._VIDEO_TRACKING_DISABLED_MASK);
    };
</script>

                    


