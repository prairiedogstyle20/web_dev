	<script type="text/javascript">
		 function oc(a) 
		 {
		 	var o = {};
		 	for(var i=0;i<a.length;i++)
		 	{
		 		o[a[i]]='';
		 	}
		 	return o;
		 }
		 $(function() 
		 {
		 	$('a').click(function() 
		 	{
		 			if($(this).attr("href") && !$(this).hasClass("newtab_ignore"))
		 			{
		 				domain = $(this).attr("href").match(/:\/\/(.[^/]+)/);
			
		 				if ( !domain || domain[1].match(/c-span.org/) || $(this).parent('.social-login').length > 0)
		 				{
							return true;
		 				}
		 				else
		 				{ 
		 					window.open($(this).attr("href"));
		 					return false;
		 				}
		 			}
		 	});
		 });

		// ALLOW CROSS-SUBDOMAIN AJAX
		//document.domain = "c-span.org";
	</script>
