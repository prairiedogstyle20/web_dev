	function shareGDM(Network){
	var shareURL;
	var URL = "https://www.witn.com/content/sports/Roy-stops-by-South-Central-East-Duplin-tops-James-Kenan-505756861.html";
	var Title = "Roy stops by South Central, East Duplin tops James Kenan";
		ga('send', 'event', 'Shared', 505756861, Network);
		if(Network == "Facebook")
	{shareURL = "https://www.facebook.com/sharer/sharer.php?u="+URL;}
	if(Network == "Twitter")
	{shareURL = "https://twitter.com/intent/tweet?text="+Title+"&url="+URL;}
	if(Network == "GooglePlus")
	{shareURL = "https://plus.google.com/share?url="+URL;}
	if(Network == "LinkedIn")
	{shareURL = "https://www.linkedin.com/shareArticle?mini=true&url="+URL+"&title="+Title;}
	window.open(shareURL,"_blank","width=650, height=525");
	}
	function shareGDMEmail(){
		ga('send', 'event', 'Shared', 505756861, 'Email');
		window.location.href = "mailto:?subject=Roy stops by South Central, East Duplin tops James Kenan&body=https://www.witn.com/content/sports/Roy-stops-by-South-Central-East-Duplin-tops-James-Kenan-505756861.html";
	}
