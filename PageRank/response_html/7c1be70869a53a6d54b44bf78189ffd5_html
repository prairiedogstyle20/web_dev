<div id="top">Наверх</div>

<script type="text/javascript" src="/themes/js/ajax.js?ver=1551205640"></script>

<script src="/themes/2017/js/j.toggler.js?ver=1551205640"></script>
<script src="/themes/2017/js/jquery.cookie.js?ver=1551205640"></script>
<script type="text/javascript" src="/themes/frontend/portlet/moment.min.js?ver=1551205640"></script>
<script type="text/javascript" src="/themes/frontend/portlet/jquery.daterangepicker.js?ver=1551205640"></script>
<script async src="//platform.twitter.com/widgets.js?ver=1551205640" charset="utf-8"></script>
<script>
	$(function () {

		var top_show = 150;
		var delay = 1000;
		$(document).ready(function () {
			$(window).scroll(function () {
				if ($(this).scrollTop() > top_show) $('#top').fadeIn();
				else $('#top').fadeOut();
			});
			$('#top').click(function () {

				$('body, html').animate({
					scrollTop: 0
				}, delay);
			});
		});

		var $lis = $('.infoboard_ li.j_toggler');

		$lis.click(function () {
			$('.content_infoboard_ .infb').removeClass('act');
			$('.menu_infoboard_ li.j_toggler').removeClass('act');
			$($(this).attr('data-toggle')).addClass('act');
		});


		var $lis2 = $('.stories_ li.j_toggler');

		$lis2.click(function () {
			$('.stories_item').removeClass('act');
			$('.stories_ li.j_toggler').removeClass('act');
			$($(this).attr('data-toggle')).addClass('act');
		});


		$('body').on('click', 'input.search-but', function (e) {
			e.preventDefault;
			if ($(this).parents('form').find('input[value=video]').is(':checked')) {
				var form = $(this).parents('form');
				var search_string = form.find('input[name=searchm]').val();
				document.location.href = '/video?q=' + search_string;
			} else {
				$(this).parents('form').submit();
			}
			return false;
		});

		$('body').on('click', 'input[name=search_button]', function (e) {
			e.preventDefault;
			if ($(this).parents('form').find('input[value=video]').is(':checked')) {
				var form = $(this).parents('form');
				var search_string = form.find('input[name=search]').val();
				document.location.href = '/video?q=' + search_string;
			} else {
				$(this).parents('form').submit();
			}
			return false;
		});

		$('#date_from').dateRangePicker({
			autoClose: true,
			format: 'DD.MM.YYYY',
			singleDate: true,
			showShortcuts: false
		});

		$('#date_to').dateRangePicker({
			autoClose: true,
			format: 'DD.MM.YYYY',
			singleDate: true,
			showShortcuts: false
		});
	});
</script>

