<script type="text/javascript">
$(window).load(function(){
$('.news_read_txt p img').each(function() {
	
	
	
	if ($(this).attr('alt')!='') {
		
		if (typeof($(this).attr('alt')) != 'undefined' && $(this).attr('alt') != null)
		{
			$(this).wrap('<div class="nwsimgwthcpt" />'); 
			$(this).after( "<div class='img-title'>" + $(this).attr('alt') + "</div>" ); 
		}
		
			
	}
	

});
}); 
    $(function () {
        		


        $('span.nb-err').click(function (e) {
            $('div.mns_1_win').css("display", "block");
        });
        $('u.close').click(function (e) {
            $('div.mns_1_win').hide();
        });

        $('.nb-prin').printPage();

        $('div.mns_1_win form button[type=submit]').click(function (e) {
            e.preventDefault();
            $.ajax({
                type: 'POST',
                url: '/frontend/news/ErrorOnFullPost',
                data: {text: $('div.mns_1_win form textarea[name=text]').val(), link: document.URL},
                success: function (data) {
                    $('div.mns_1_win form').hide();
                    $('div.thanks_message_err').show();
                }
            });
            return false;
        });
        $('div.mns_1_win u.close-thks-msg').click(function (e) {
            e.preventDefault();
            $('div.mns_1_win').hide();
            $('div.mns_1_win form textarea[name=text]').val('');
            $('div.thanks_message_err').hide();
            $('div.mns_1_win form').show();
            return false;
        });

        $('.support').click(function () {
            comment = $(this).parents().eq(5);
            parent = comment.find('.parent').val();
            $.post('/ajax/commentSupport', {id_comment: parent}, function (data) {
                var resp = $.parseJSON(data);
                if (resp.error !== undefined) {
                    alert(resp.error);
                } else if (resp.success !== undefined) {
                    points = comment.find('.cr1').html();
                    comment.find('.cr1').html(parseInt(points) + 1);
                    alert(resp.success);
                } else {
                    alert("Некорректный запрос");
                }
            });
            return false;
        });
        $('.complain').click(function () {
            comment = $(this).parents().eq(5);
            //alert(comment.html());
            parent = comment.find('.parent').val();
            $.post('/ajax/commentComplain', {id_comment: parent}, function (data) {
                var resp = $.parseJSON(data);
                if (resp.error !== undefined) {
                    alert(resp.error);
                } else if (resp.success !== undefined) {
                    alert(resp.success);
                } else {
                    alert("Некорректный запрос");
                }
            });
            return false;
        });

        // вставка формы ответа для комментариев
        $('.ct_answer').click(function (a) {
			
			if ($('.comments_form_').hasClass('nologin_') ) {
				window.location.href = "https://www.sports.kz/registration";
			}
			else
			{
				$('#comments2').not('#comments2:first').remove();
				comment = $(this).parents().eq(2);

				if ( $(this).parents().eq(2).find('#comments2').length<=0 ) {
					if ($("#comments-box").size()) {
						textarea = $('#comments-box #comments').clone(true);
					} else {
						textarea = $('#comments2').clone(true);

					}

					textarea.find('h3').html("Ответ для " + $('.nick', comment).val());
					textarea.find('.cancel').removeClass('none');
					textarea.appendTo(comment);					 
				 } 
				
				

				a.preventDefault();
			}

        });


        $("#comments-box").find('form').submit(function () {
            var f = $(this).serialize()
            $.post($(this).attr('action'), f, function (res) {
                if (res.success) {
                    alert(res.success);
                } else {
                    alert(res.error);
                }
            }, 'json');
            return false;

        })


        $('.cancel-answer').click(function () {
            $(this).parents("#comments2").remove();
        });

        $('#nogood,#low,#good').click(function (a) {

            if ($(this).parents('.unregcontent').size() == 0) {
                id_news = $(this).attr('rel');
                vote = $(this).attr('id');
                var url = '/ajax/voteNews';
                if ($(this).parent().is('#tv_vote_btn')) {
                    url = '/ajax/voteTvNews';
                } else if ($(this).parent().is('#auth_vote_btn')) {
                    url = '/ajax/voteAuthorNews';
                } else if ($(this).parent().is('#auth_video_vote_btn')) {
                    url = '/ajax/voteAuthorVideo';
                } else if ($(this).parent().is('#auth_fotogallery_vote_btn')) {
                    url = '/ajax/voteAuthorFotogallery';
                } else if ($(this).parent().is('#auth_rating_btn')) {
                    url = '/ajax/voteAuthorRating';
                } else if ($(this).parent().is('#auth_rumors_vote_btn')) {
                    url = '/ajax/voteAuthorRumors';
                } else if ($(this).parent().is('#sochi_fotogallery_vote_btn')) {
                    url = '/ajax/voteFotogallery';
                } else if ($(this).parent().is('#rio_fotogallery_vote_btn')) {
                    url = '/ajax/voteFotogallery';
                } else if ($(this).parent().is('#sochi_video_vote_btn')) {
                    url = '/ajax/voteSochiVideo';
                } else if ($(this).parent().is('#rio_video_vote_btn')) {
                    url = '/ajax/voteRioVideo';
                }

                $.get(url, {i: id_news, v: vote}, function (data) {
                    var resp = jQuery.parseJSON(data);
                    if (typeof resp == 'object') {
                        if (resp.error) {
                            alert(resp.error);
                        } else if (resp.success) {
                            alert(resp.success);
                        }
                    } else if (response !== false) {
                        //$('#votes').html();
                    } else {
                        alert("Некорректный запрос");
                    }

                });
            }
            a.preventDefault();
        });

        $("[data-fancybox]").fancybox();

        $('.ocomm').click(function () {
            $('html, body').animate({ scrollTop: $('#comments').offset().top }, 500);
        });
    });
</script>


