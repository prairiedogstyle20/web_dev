		$(function () {
			


			showPanel();

			function showPanel() {
				if ($.cookie('showPanel') == "1") {
					$('.infoboard_').addClass('match_hide');
				}
				else {
					$('.infoboard_').removeClass('match_hide');
				}
			}

			$(".infoboard_close").click(function () {
				if ($.cookie('showPanel') == "1") {
					$.cookie("showPanel", null);
				}
				else {
					$.cookie("showPanel", "1");
				}

			});

		});
	
	</script>


	
	
