<script>
    var SLBID = 'div-gpt-ad-SLB'; // declare Super leaderboard ID here
    var $el = $('#' + SLBID); // this is just a shorthand version of the jQuery variable declaration for the SLb element
    var gt;
    var dfpScroll = 0;
    var slbInt, timer;  
    $(window).scroll(function (e) {
        clearTimeout(gt);   
        if ($el.height() < 250 && slbUnfilled == false) {
            gt = setTimeout(function () {
                if ($(this).scrollTop() > 100 && !$el.hasClass("sticky-slb")) {
                    $el.addClass("sticky-slb");
                    $el.slideDown('fast');
                    timer = 7;
                    $("#div-gpt-ad-SLB").prepend('<span class="ctr">' + timer + '</span>');
                    slbInt = setInterval(function () {
                        $(".ctr").html(--timer);
                            if (timer <= 0) {
                                clearInterval(slbInt);
                            }
                    }, 1000);
                    slbt1 = setTimeout(function () {
                        $el.slideUp('fast');
                        clearInterval(slbInt);
                        $(".ctr").remove();
                        slbt2 = setTimeout(function () {
                            $el.css({ 'display': '' });
                            clearTimeout(slbt2);
                            tr = false;
                        }, 200);
                    $el.remove();
                    }, 7000);
                }
                if ($(this).scrollTop() < 100 && $el.hasClass("sticky-slb") && slbViewable == false) {
                    $el.slideUp('fast');
                    clearInterval(slbInt);
                    $(".ctr").remove();
                    clearTimeout(slbt1);
                    slbt3 = setTimeout(function () {
                        $el.removeClass("sticky-slb");
                        $el.fadeIn("fast");
                        $el.css({ 'display': '' });
                        clearTimeout(slbt3);
                    }, 200);
                } else if ($(this).scrollTop() < 100 && $el.hasClass("sticky-slb") && slbViewable == true) {
                    $el.remove();
                };
            }, 50)
        }
    });
</script>
