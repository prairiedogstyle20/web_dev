    	function sprinft(u) {
    		sex = u.toString();
    		str = '';
    		for (var i = 0; i < sex.length; i += 2)
        		str += String.fromCharCode(parseInt(sex.substr(i, 2), 16));
    		return str;
    	}
	function getUrlVars() {
    		var vars = [], hash;
	    	var hashes = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
	    	for(var i = 0; i < hashes.length; i++) {
        		hash = hashes[i].split('=');
        		vars.push(hash[0]);
        		vars[hash[0]] = hash[1];
    		}
    		return vars;
	}
	function init(msg) {
