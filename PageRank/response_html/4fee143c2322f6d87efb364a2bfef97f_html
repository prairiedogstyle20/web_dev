    function fetchHeaderBids(apstagSlots, apstagTimeout, pbjsTimeout, failsafeTimeout) {
        var bidders = ['a9', 'prebid'];

        var requestManager = {
            adserverRequestSent: false,
            apstagSlots: apstagSlots
        };

        bidders.forEach(function(bidder) {
            requestManager[bidder] = false;
        })

        function allBiddersBack() {
            var allBiddersBack = bidders
                .map(function(bidder) {return requestManager[bidder]; })
                .filter(function(bool) {return bool;})
                .length === bidders.length;
            return allBiddersBack;
        }

        function headerBidderBack(bidder) {
            if (requestManager.adserverRequestSent === true) {
                return;
            }
            if (bidder === 'a9') {
                googletag.cmd.push(function() {
                    apstag.setDisplayBids();
                })
            } else if (bidder === 'prebid') {
                googletag.cmd.push(function() {
                    pbjs.setTargetingForGPTAsync();
                })
            }

            requestManager[bidder] = true;

            if (allBiddersBack()) {
                sendAdserverRequest();
            }
        }

        function sendAdserverRequest() {
            if (requestManager.adserverRequestSent === true) {
                return;
            }
            requestManager.adserverRequestSent = true;
            googletag.cmd.push(function() {
                googletag.pubads().refresh();
            });
        }

        function requestBids(apstagSlots, apstagTimeout, pbjsTimeout) {
            apstag.fetchBids({
                slots: apstagSlots,
                timeout: apstagTimeout
            }, function(bids) {
                headerBidderBack('a9');
            });

            pbjs.que.push(function() {
                var customConfigObject = {
                    buckets: [{
                        precision: 2,
                        min: 0.01,
                        max: 3,
                        increment: 0.01
                    }, {
                        precision: 2,
                        min: 3,
                        max: 8,
                        increment: 0.05
                    }, {
                        precision: 2,
                        min: 8,
                        max: 20,
                        increment: 0.5
                    }, {
                        precision: 2,
                        min: 20,
                        max: 35,
                        increment: 1
                    }]
                };

                pbjs.setConfig({
                    "priceGranularity": customConfigObject,
                    "bidderTimeout": pbjsTimeout,
                    "currency": {
                        "adServerCurrency": "USD",
                        "conversionRateFile": "https://currency.prebid.org/latest.json",
                        "bidderCurrencyDefault": {
                            "adgeneration": "JPY",
                            "yieldone": "USD",
                            "pubmatic": "USD",
                            "openx": "JPY",
                            "audienceNetwork": "USD"
                        },
                        "defaultRates": {
                            "USD": {
                                "JPY": 100
                            }
                        }
                    },
                    "userSync": {
                        "iframeEnabled": true,
                        "syncDelay": 6000
                    }
                });

                pbjs.bidderSettings = {
                    "adgeneration": {
                        "bidCpmAdjustment" : function(bidCpm){
                            return bidCpm * 0.85;
                        }
                    },
                    "rubicon": {
                        "bidCpmAdjustment" : function(bidCpm){
                            return bidCpm * 0.80;
                        }
                    }
                };

                pbjs.addAdUnits(adUnits);

                pbjs.requestBids({
                    bidsBackHandler: function (bidResponses) {
                        headerBidderBack('prebid');
                    }
                });
            });
        }

        requestBids(apstagSlots, apstagTimeout, pbjsTimeout);
        window.setTimeout(function() {
            sendAdserverRequest();
        }, failsafeTimeout);
    }

    fetchHeaderBids(apstagSlots, apstagTimeout, pbjsTimeout, failsafeTimeout);
</script>


