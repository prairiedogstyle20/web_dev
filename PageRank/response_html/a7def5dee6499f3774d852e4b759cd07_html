document.cookie = "__adblocker=; expires=Thu, 01 Jan 1970 00:00:00 GMT; path=/";
var setNptTechAdblockerCookie = function(adblocker) {
    var d = new Date();
    d.setTime(d.getTime() + 60 * 60 * 24 * 2 * 1000);
    document.cookie = "__adblocker=" + (adblocker ? "true" : "false") + "; expires=" + d.toUTCString() + "; path=/";
}
var script = document.createElement("script");
script.setAttribute("async", true);
script.setAttribute("src", "//www.npttech.com/advertising.js");
script.setAttribute("onerror", "setNptTechAdblockerCookie(true);");
document.getElementsByTagName("head")[0].appendChild(script);
</script>

<script>
tp = window.tp || [];

// Tags
var pianoTags = [];

pianoTags.push('BlockArticleModule'.toLowerCase());




tp.push(["setCustomVariable", "content_is_editorial_opinion", "no"]);





















var md = new MobileDetect(window.navigator.userAgent);
if (md.phone() !== null) {
    pianoTags.push('userPhone');
    pianoTags.push('user_phone');
    tp.push(["setCustomVariable", "user_device_type", "mobile"]);
} else if (md.tablet() !== null) {
    pianoTags.push('userTablet');
    pianoTags.push('user_tablet');
    tp.push(["setCustomVariable", "user_device_type", "tablet"]);
} else {
    pianoTags.push('user_desktop');
    pianoTags.push('userDesktop');
    tp.push(["setCustomVariable", "user_device_type", "desktop"]);
}

tp.push(['setTags', pianoTags]);

// Piano Variables



tp.push(["setContentIsNative", false]);


// Custom Variables
tp.push(["setCustomVariable", "site_module", "BlockArticleModule".toLowerCase()]);
tp.push(["setCustomVariable", "site_platform", 'martini']);














tp.push(["setCustomVariable", "user_level", "annonymous"]);
pianoTags.push('user_level_anonymous');




// Site Sections
$.each(document.location.href.split('/').slice(3), function (i, v) {
    if (v.match(/^[A-Za-z0-9_]+$/)) {
        if (i === 0) {
            tp.push(["setCustomVariable", "site_section", v]);
            tp.push(["setContentSection", v]);
        } else {
            tp.push(["setCustomVariable", "site_section" + i, v]);
        }
    }
});

// Article Subjects
var articleSubjects = [];



tp.push(["setCustomVariable", "content_subjects", articleSubjects.join(',')]);

//Adblocker Detection
if ($.cookie('nqab') === null) {
    tp.push(["setCustomVariable", "user_adblocker_sp", "unknown"]);
} else if ($.cookie('nqab') === "true") {
    tp.push(["setCustomVariable", "user_adblocker_sp", "yes"]);
} else {
    tp.push(["setCustomVariable", "user_adblocker_sp", "no"]);
}

// Init Piano
tp.push(["setDebug", true]);
tp.push(["setUserRef", ""]);

tp.push(["addHandler", "meterActive", function(meterData){
   console.log("TP: Martini: You've seen " + meterData.views
       + " out of " + meterData.maxViews
       + " free articles. You have " + meterData.viewsLeft
       + " articles left."
   );
}]);
tp.push(["addHandler", "loginRequired", function(params) {
    // If they're already signed in, we don't need to do anything special.
    if (tp.user.isUserValid()) {
        return;
    }

    document.cookie = '__pianoParams=' + JSON.stringify(params) + '; path=/; domain=.heraldscotland.com';

    var ref = "subo";
    if (false) {
        var from = "ad-light";
    } else {
        var from = 'user_subscription';
        if (location.pathname === '/subscribe/gift/') {
            from = 'gift';
        } else if (location.pathname === '/subscribe/gift/redeem/') {
            from = 'gift-redeem';
        }
    }



    location.href = '/my/account/log-in/?ref=subo&from=' + from;
}]);

tp.push( [ "addHandler", "showTemplate", function (templateParams) {
    $('#piano-message').show();
    $('#piano-top-message').show();
}]);

tp.push( [ "addHandler", "showOffer", function (offerParams) {
    window.shownOffer = true;
    if (offerParams.containerSelector === '#piano-message') {
        $('#piano-message').show();
    }

    if (offerParams.containerSelector === '#piano-top-message') {
        $('#piano-top-message').show();
    }

}]);

tp.push(["addHandler", "checkoutComplete", function(conversion){
    var s = window.s;
    s.events = s.apl(s.events, "event4", ",", 1); // Set the subscription counter event.
    s.events = s.apl(s.events, "purchase", ",", 1); // Set the purchase event.
    s.products = "Subscription : Metered Access;" + conversion.termId + ";1;"+conversion.chargeAmount;
    s.purchaseID = conversion.termConversionId;
    s.pageName = '(www) /subscribe/complete/';
    s.tl();
}]);

tp.push(["addHandler", "checkoutClose", function(conversion){
   $.cookie('pianoConverted', '1', {path: '/', domain: window.utag_data.cookie_domain});
}]);

tp.push(["addHandler", "meterExpired", function(meterData){
    var cVal = window.location.href;
    $.cookie('meterExpire', cVal, {path: '/', domain: window.utag_data.cookie_domain});
}]);

tp.push(["init", function() {

    // If the user is logged in and they have a cookie which suggests they were trying to buy something, attempt to
    // put the user back where they were so they can continue.
    if (tp.user.isUserValid()) {
        var paramsCookie = tp.util.findCookieByName('__pianoParams');
        var params = false;
        try {
            // Try to parse stored JSON data and remove the cookie.
            params = JSON.parse(paramsCookie);
            document.cookie = '__pianoParams=null; expires=-1; path=/; domain=.heraldscotland.com';
        } catch (e) {
            params = false;
        }

        // If the "params" object is valid, start the checkout process.
        if (params) {
            tp.offer.startCheckout(params);
        }
    }

    // any additional functionality you need to execute after
    // the tinypass javascript loads
}]);
