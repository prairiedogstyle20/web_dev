                            $("#gallery-items-52d73b8e-2f2e-11e9-b0b2-6391dc08a23c .owl-stage-outer").height( $('#carousel-52d73b8e-2f2e-11e9-b0b2-6391dc08a23c .owl-item.active').height() );
                        }, 300);
                    }
                 
                 
                        try {
            
            var bOnlyDFP = false, bIsOAS = false;
            if( typeof gptAdSlots != 'undefined' ){
                        googletag.cmd.push(function(){
                            googletag.pubads().clear();
                            googletag.pubads().refresh();
                        });}
                /* reloadByIds will return false/0 if the ad slot called for hasn't actually been loaded to the page. */
