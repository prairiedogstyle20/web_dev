                                // Display asset
                                var sHTML_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = '';
                                if (this.content.includes('engagement-asset') && iDisplayCount_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c < iMaxDisplay_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c) {
                                    iDisplayCount_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c ++;
                                    sHTML_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c += '<div class="engagement-item item-' + iDisplayCount_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c + ' original">' + this.content + '</div>';
                                }
                            }
                        } else {
                            if (this.id == sOriginID_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c) {
                                // Origin found. Begin displaying assets
                                bFoundOrigin_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = true;
                            } else {
                                // Origin found is false. Skip asset
                                // Fail-safe in case origin is not present in set. This can be removed when origin is fixed.
                                if (bFirstRun_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c == true) {
                                    // Stored first id
                                    sFirstID_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = this.id;
                                    bFirstRun_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = false;
                                } else if (this.id == sFirstID_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c) {
                                    // We've started again. Force origin
                                    sOriginID_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = sFirstID_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c;
                                    bFoundOrigin_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = true;
                                } // end Fail-safe
                            }
                        }

                        // Append engagement assets to container
                        oEngagementContainer_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c.append(sHTML_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c);

                        // Stop traversing asset array
                        if (iDisplayCount_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c >= iMaxDisplay_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c) {
                            bStop_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = true;
                            return false;
                        }
                    });

                    // Include block_id on newly added list items
                    oEngagementContainer_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c.find('.engagement-item.original').each(function() {
                        var sHref_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = scrubURL($(this).find("a.centered-content-link").attr("href"));

                        if(sHref_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c!="javascript:void(0)"){

                        // Add content discovery tracking
                        
                        sHref_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c += '#tncms-source=endcard-gallery';
                        

                        

                        // Add to image and headlines links
                        $(this).removeClass('original').find("a.centered-content-link").attr("href", sHref_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c);
                        }
                    });

                    // Check next URL
                    if (sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c && bStop_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c == false) {
                        // if origin hasn't been found yet and we hit next_url. Trigger the set to pull in again.
                        if (bFoundOrigin_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c == false) {
                            // No origin yet. Call the populate function
                            __tnt.engagement.assets["52d73b8e-2f2e-11e9-b0b2-6391dc08a23c"].populate(sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c);
                        } else {
                            // Append sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c to engagement set
                            oEngagementContainer_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c.append('<div id="engagement-more" data-next-url="' + sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c + '"></div>');

                            if (iDisplayCount_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c < iMaxDisplay_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c) {
                                // Get next url and request more assets
                                __tnt.engagement.assets["52d73b8e-2f2e-11e9-b0b2-6391dc08a23c"].populate(oEngagementMore_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c.data('next-url'));
                            }

                        }
                    }

                    // Add total count to container
                    if (bStop_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c == true) {
                        // Add total count to container
                        oEngagementContainer_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c.attr("data-engagement-total", iDisplayCount_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c);
                        
                        // Remove engagement loading spinner
                        oEngagementSpinner_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c.remove();
                        
                        return false;
                    }

                } else {
                    // Empty asset set returned. Go back in for a real set
                    if(__tnt.engagement.assets["52d73b8e-2f2e-11e9-b0b2-6391dc08a23c"].attempts<2){
                    if(sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c==null||sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c==""){
                        sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c = "/sports/photos-taylor-vs-elwood-boys-bb/collection_52d73b8e-2f2e-11e9-b0b2-6391dc08a23c.html";
                    }

                    __tnt.engagement.assets["52d73b8e-2f2e-11e9-b0b2-6391dc08a23c"].populate(sNextUrl_52d73b8e_2f2e_11e9_b0b2_6391dc08a23c);

                    __tnt.engagement.assets["52d73b8e-2f2e-11e9-b0b2-6391dc08a23c"].attempts++;
                    }
                }
            });
        }
        
        }

        // Rewrite URL for preview
        // TODO move to one location
        function scrubURL(sURL) {
            if (typeof sURL != 'undefined'){
                return sURL;
            }
        }
    </script>
</div>
                        </div>
                    </div>
            </div>
        </div></div>
                </div>
                
                
                
                
                    
