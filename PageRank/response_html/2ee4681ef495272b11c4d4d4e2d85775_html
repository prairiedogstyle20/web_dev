            *[data-fragment-name].highlighted-fragment {
                box-shadow:
                    0px 0px 19px 5px rgba(255,0,255,0.7), 
                    0px 0px 19px 5px rgba(255,0,255,0.7) inset; 
            }
        </style>
        <script>
            window.HS_FRAGMENTS = {
                highlightOn: function() {
                    document.querySelectorAll('[data-fragment-name][data-fragment-id]').forEach(function(node) {
                        node.classList.add('highlighted-fragment');
                    });
                },
                highlightOff: function() {
                    document.querySelectorAll('.highlighted-fragment').forEach(function(node) {
                        node.classList.remove('highlighted-fragment');
                    });
                },
                report: function() {
                    console.dir(document.querySelectorAll('[data-fragment-name]'));
                },
                reportFallbacks: function() {
                    console.dir(document.querySelectorAll('[data-fragment-name]:not([data-fragment-id])'));
                },
                reportSuccesses: function() {
                    console.dir(document.querySelectorAll('[data-fragment-name][data-fragment-id]'));
                }
            };
        </script>
    </head>

