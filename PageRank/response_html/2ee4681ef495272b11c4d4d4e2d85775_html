            <script type="text/javascript">
	var googletag = googletag || {};
	googletag.cmd = googletag.cmd || [];

	function loadGoogleAdsScripts() {
		var gads = document.createElement('script');
		gads.async = true;
		gads.type = 'text/javascript';
		var useSSL = 'https:' == document.location.protocol;
		gads.src = (useSSL ? 'https:' : 'http:') +
			'//www.googletagservices.com/tag/js/gpt.js';
		var node = document.getElementsByTagName('script')[0];
		node.parentNode.insertBefore(gads, node);
	}

	var match = new RegExp('/hs-gdpr-optin15022019v2=(.+?)(?=\b)/');
	var gdprCookie = document.cookie.match(match);
	var gdprCookieValue = gdprCookie ? parseInt(gdprCookie[1]) : 0;
	var isGDPRSubjectAndAdsEnabled = !!(document.querySelector('meta[name=gdpr-subject]')) && !!(document.querySelector('meta[name=gdpr-ads-enabled]'));	
	var requestNonPersonalizedAds = (isGDPRSubjectAndAdsEnabled && gdprCookieValue != 1) ? 1 : 0;

	if(isGDPRSubjectAndAdsEnabled) { 
		loadGoogleAdsScripts();
	} else {
		window.delayedScriptLoader.register_script(loadGoogleAdsScripts);
	}


	function getUrlParameter(name) {
		name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
		var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
		var results = regex.exec(location.search);
		return results === null ? null : decodeURIComponent(results[1].replace(/\+/g, ' '));
	};

	var customTargetingValue = getUrlParameter('preview_id');

	window.adViewport = window.matchMedia("(min-width: 640px)").matches ? 'desktop' : 'mobile';
	window._ad_slots = {};
	window._dfpUnitPrefix = '/1020073/';
	window._nativeAdListener = {
		_queue: {},
		addData: function(frameId, slotData) {
			if (this._queue[frameId] !== undefined) {
				return this._queue[frameId].onReceive(slotData);
			}

			this._addToQueue(frameId, { data: slotData });
		},
		addListener: function(frameId, onReceive) {
			if (this._queue[frameId] !== undefined) {
				return onReceive(this._queue[frameId].data);
			}

			this._addToQueue(frameId, { onReceive: onReceive });
		},
		_addToQueue: function(frameId, obj) {
			this._queue[frameId] = obj;
		}
	};

	window._defineAndDisplayNewAdSlot = function(slotName, type, opts) {
		opts.onRenderEnded = opts.onRenderEnded || function() {};
		opts.onSlot = opts.onSlot || function() {};

		var videoOnly = opts.videoOnly !== undefined && opts.videoOnly;

		if (typeof window.adPositions[type] === "undefined") {
			console.error(type, 'is not defined yet');
			return;
		}

		var adPosition = window.adPositions[type];
		window.adPositions[slotName] = adPosition;
		var adSlot = window.adPositions[slotName]['anyViewport'] || window.adPositions[slotName][window.adViewport];
		
		var renderEnded = adSlot.renderEnded || function() {};

		googletag.pubads().addEventListener('slotRenderEnded', function(event) {
			if (event.slot.getSlotElementId() === slotName) {
				renderEnded(event);
				opts.onRenderEnded(event);
			}
		});

		var slot = window._defineAdSlot(adSlot, slotName, videoOnly);
		opts.onSlot(slot);

		googletag.cmd.push(function() {
			googletag.display(slotName);
			window._ad_slots[slotName] = slot;
		});
	};

	window.responsiveAdSizes = [
		[1, 1],
		[1, 2],
		[5, 1],
		[400, 565],
		[720, 405],
		[2400, 600],
		[2400, 601],
		[2400, 602],
		[2400, 603]
	];

	window.adUnits = {
		'hs-desktop-banner-top': {
			sizes: [[728, 90], [970, 250], [2400, 600]]
		},
		'hs-desktop-banner-middle': {
			sizes: [[728, 90], [970, 250], [2400, 600]]
		},
		'hs-desktop-banner-bottom': {
			sizes: [[728, 90], [970, 250], [2400, 600]]
		},
		'hs-mobile-banner-top': {
			sizes: [[320, 50], [2400, 600]]
		},
		'hs-mobile-banner-middle': {
			sizes: [[320, 50], [300, 250], [400, 565], [720, 405], [2400, 600]]
		},
		'hs-mobile-banner-bottom': {
			sizes: [[320, 50], [300, 250], [400, 565], [720, 405], [2400, 600]]
		},
		'hs-desktop-double-mpu-top': {
			sizes: [[300, 600]]
		},
		'hs-desktop-inpost-top': {
			sizes: [[300, 250], [720, 405]]
		},
		'hs-desktop-inpost-bottom': {
			sizes: [[300, 250], [720, 405]]
		},
		'hs-mobile-inpost-top': {
			sizes: [[300, 250], [400, 565], [720, 405]]
		},
		'hs-mobile-inpost-bottom': {
			sizes: [[300, 250], [400, 565], [720, 405]]
		},
		'hs-mobile-inpost-bottom-video-only': {
			unitName: 'hs-mobile-inpost-bottom',
			sizes: [[720, 405]]
		},
		'hs-desktop-overlay': {
			sizes: [[1, 1]]
		},
		'hs-mobile-overlay': {
			sizes: [[1, 2]]
		},
		'hs-in-stream': {
			sizes: [[300, 250], [320, 50], [728, 90], [970, 90], [970, 250], [970, 300]]
		},
		'hs-native-priority-post': {
			sizes: [[5, 1]]
		},
		'hs-native-related-post': {
			sizes: [[5, 1]]
		}
	};

	function _onRenderLeaderboardTop(data) {
		if (data.isEmpty) {
			return;
		}

		var width = data.size[0];
		var height = data.size[1];
		var isFullWidth = (width === 2400 && height === 600);
		var leaderboardTop = document.getElementById('leaderboard-top');
		
		// set to correct sizes after ad is loaded
		var leaderboardTopIframe = leaderboardTop.querySelector('iframe');
		if (leaderboardTopIframe !== null) {
			leaderboardTopIframe.height = height;
			leaderboardTopIframe.width = width;
		}

		if (isFullWidth) {
			var vpWidth = Math.max(document.documentElement.clientWidth, window.innerWidth || 0);
			var ratio = height / width;
			var scaledAdHeight = vpWidth * ratio;
			leaderboardTopIframe.height = Math.round(scaledAdHeight);
			leaderboardTop.style.paddingTop = '0';
			leaderboardTop.style.paddingBottom = '0';
		} else {
			leaderboardTop.style.minHeight = height + 'px';
		}
	}

	window.adPositions = {
		'leaderboard-top': {
			desktop: {
				unit: 'hs-desktop-banner-top',
				renderEnded: function(data) {
					_onRenderLeaderboardTop(data);
					window.HS.vent.trigger('rendered:desktop-banner-top', data);
				},
			},
			mobile: {
				unit: 'hs-mobile-banner-top',
				renderEnded: function(data) {
					_onRenderLeaderboardTop(data);
				}
			}
		},
		'leaderboard-middle': {
			desktop: {
				unit: 'hs-desktop-banner-middle',
				renderEnded: function(event) {
										var
					ad_el = document.getElementById('leaderboard-middle'),
					nl_el = document.getElementById('newsletter-interstitial');
					if (event.isEmpty) {
						ad_el.parentNode.removeChild(ad_el);
					} else {
						nl_el.parentNode.removeChild(nl_el);
					}
									}
			},
			mobile: {
				unit: 'hs-mobile-banner-middle'
			},
			lazy: false
		},
		'leaderboard-bottom': {
			desktop: {
				unit: 'hs-desktop-banner-bottom'
			},
			mobile: {
				unit: 'hs-mobile-banner-bottom'
			},
			lazy: false
		},
		'mpu': {
			desktop: {
				unit: 'hs-desktop-double-mpu-top',
				renderEnded: function(data) {
					window.HS.vent.trigger('rendered:mpu-top', data);
				}
			},
			lazy: false
		},
		'popover': {
			desktop: {
				unit: 'hs-desktop-overlay'
			},
			mobile: {
				unit: 'hs-mobile-overlay'
			},
			collapseEmptyDiv: true
		},
		'stream': {
			desktop: {
				unit: 'hs-in-stream'
			},
			mobile: {
				unit: 'hs-in-stream'
			},
			collapseEmptyDiv: true
		},
		'in-post-top': {
			desktop: {
				unit: 'hs-desktop-inpost-top'
			},
			mobile: {
				unit: 'hs-mobile-inpost-top'
			},
			collapseEmptyDiv: true
		},
		'in-post-bottom': {
			desktop: {
				unit: 'hs-desktop-inpost-bottom'
			},
			mobile: {
				unit: 'hs-mobile-inpost-bottom'
			},
			collapseEmptyDiv: true
		},
		'in-post-bottom-video-only': {
			anyViewport: {
				unit: 'hs-mobile-inpost-bottom-video-only'
			},
			collapseEmptyDiv: true
		},
		'native-priority-post': {
			anyViewport: {
				unit: 'hs-native-priority-post'
			},
			collapseEmptyDiv: true
		},
		'native-related-post': {
			anyViewport: {
				unit: 'hs-native-related-post'
			},
			collapseEmptyDiv: true
		}
	};

	window._isAdSizeReponsive = function(size) {
		for (var i = 0; i < window.responsiveAdSizes.length; i++) {
			if (
				window.responsiveAdSizes[i][0] === size[0] &&
				window.responsiveAdSizes[i][1] === size[1]
			) {
				return true;
			}
		}
		return false;
	};

	window._getSizeMapping = function(sizes) {
		var responsiveSizes = sizes.filter(_isAdSizeReponsive);
		var nonResponsiveSizes = sizes.filter(function(s) { return !_isAdSizeReponsive(s) });

		var sizeMapping = [{minWidth: 0}];

		var getItemIndex = function(arr, objKey, objVal) {
			for (var i; i < arr.length; i++) {
				if (arr[i][objKey] !== undefined && arr[i][objKey] === objVal) {
					return i;
				}
			}
			return -1;
		};

		sizeMapping = nonResponsiveSizes.reduce(function(mapping, size) {
			if (getItemIndex(mapping, 'minWidth', size[0]) < 0) {
				mapping.push({minWidth: size[0]});
			}
			return mapping;
		}, sizeMapping);

		sizeMapping = sizeMapping.map(function(map) {
			map.sizes = responsiveSizes
				.concat(
					nonResponsiveSizes.filter(function(size) {
						return size[0] <= map.minWidth;
					})
				)
				.sort(function(a, b) {
					return a[0] === b[0] && a[1] > b[1] || a[0] > b[0];
				});
			return map;
		});

		return sizeMapping.sort(function(a, b) {
			return a.minWidth < b.minWidth;
		});
	};

	window._buildSizeMapping = function(sizeMapping) {
		var s = googletag.sizeMapping();
		for (var i = 0; i < sizeMapping.length; i++) {
			s.addSize([sizeMapping[i].minWidth, 0], sizeMapping[i].sizes);
		}
		return s.build();
	};

	window._defineAdSlot = function(unitSettings, positionContainerId) {
		var unit = window.adUnits[unitSettings.unit];
		var sizeMapping = _getSizeMapping(unit.sizes);

		var adUnitName = unit.unitName !== undefined ? unit.unitName : unitSettings.unit;

		var slot = window.googletag
			.defineSlot(
				_dfpUnitPrefix + adUnitName,
				unit.sizes,
				positionContainerId
			);

		if (sizeMapping.length > 1) {
			slot.defineSizeMapping(
				_buildSizeMapping(sizeMapping)
			)
		}

		slot.addService(googletag.pubads());

		return slot;
	};

	window._defineAdPosition = function(positionId) {
		var adPosition = window.adPositions[positionId];

		if (typeof adPosition[window.adViewport] === "undefined") {
			return false;
		}

		var unitSettings = adPosition[window.adViewport];

		var slot = window._defineAdSlot(unitSettings, positionId);

		if (unitSettings.renderEnded !== undefined) {
			slot.renderEnded = unitSettings.renderEnded;

			googletag.pubads().addEventListener('slotRenderEnded', function(event) {
				if (event.slot.getSlotElementId() === positionId) {
					unitSettings.renderEnded(event);
				}
			});
		}

		if (unitSettings.loaded !== undefined) {
			googletag.pubads().addEventListener('slotOnload', function(data) {
				if (slot === data.slot) {
					unitSettings.loaded(data);
				}
			});
		}

		if (adPosition.collapseEmptyDiv !== undefined && adPosition.collapseEmptyDiv) {
			slot.setCollapseEmptyDiv(true);
		}

		window.adPositions[positionId].slot = slot;
	};

	window._displayAdPosition = function(positionId) {
		if (typeof window.adPositions[positionId] === "undefined") {
			console.error(positionId, 'is not defined yet')	;
			return;
		}

		var adPosition = window.adPositions[positionId];

		if (typeof adPosition[window.adViewport] === "undefined") {
			return false;
		}


		googletag.cmd.push(function() {
			googletag.display(positionId);
			if (positionId === 'leaderboard-top') {
				// set ad iframe to width and height of zero
				// this is before ad data is loaded
				var ad = document.querySelector('#' + positionId + ' iframe');
				if (ad !== null) {
					ad.height = 0;
					ad.width = 0;
				}
			}
		});
	};

	window._initAdPosition = function(positionId) {
		if (typeof window.adPositions[positionId] === "undefined") {
			console.error(positionId, 'is not defined yet')	;
			return;
		}

		var adPosition = window.adPositions[positionId];

		if (adPosition.lazy === undefined || !adPosition.lazy) {
			window._displayAdPosition(positionId);
		}
	}

	googletag.cmd.push(function() {

		var pubadsService = googletag.pubads();

		for (var adPosKey in window.adPositions) {
			window._defineAdPosition(adPosKey);
		}

		
		pubadsService.setTargeting("env", "production");
		pubadsService.setTargeting("frontpage", "0");
		pubadsService.setTargeting("nsfw", "0");
		pubadsService.setTargeting("adult_content", "0");
		pubadsService.setTargeting("categories", ["style"]);
		pubadsService.setTargeting("tags", ["the-north-face"]);
		pubadsService.setTargeting("author", "Heather Snowden");
		pubadsService.setTargeting("post_id", "1831633");
		pubadsService.setTargeting("vertical", "style");

		if (customTargetingValue) {
			pubadsService.setTargeting('preview_id', customTargetingValue);
		}

		pubadsService.setRequestNonPersonalizedAds(requestNonPersonalizedAds);
		pubadsService.enableSingleRequest();
		pubadsService.enableAsyncRendering();
		googletag.enableServices();
	});
</script>

