    $('#site-search-576088-btn').on(__tnt.client.clickEvent, function(){
        $('#site-search-576088').submit();
        return false;
    });

    $('#site-search-576088').on('submit', function(){
        // Filter query
        var sQueryFiltered = $("#site-search-576088 input[name=q]").val().replace(/\?/g, '');
    $("#site-search-576088 input[name=q]").val( sQueryFiltered );

        if( $('#site-search-576088-term').val() ){
            return true;
        }else{
            return false;
        }
