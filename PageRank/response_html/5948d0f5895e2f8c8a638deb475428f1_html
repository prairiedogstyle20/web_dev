                        <script>
                            jQuery(document).ready(function($){
                                $('#followed-notification-modal-ed54fc6a-4657-11df-a84b-0017a4a78c22-form').submit(function(e) {
                                    e.preventDefault();
                                    var $followBtn = $(this).find('.follow-btn');
                                    var sAction = $(this).attr('action') + '&title=' + $(this).find('.input-notification-name').val();
                                    $followBtn.find('.tnt-svg').show();
                                    $followBtn.prop('disabled', true);

                                    function submit_finished($alert){
                                        setTimeout(function(){
                                            $followBtn.find('.tnt-svg').hide();
                                            $alert.show().delay(2500).fadeOut(400);
                                        },2000);
                                    }

                                    $.ajax({
                                        type: 'POST',
                                        url: sAction,
                                        data: $(this).serialize(),
                                        success: function(oData){
                                            
                                                // Track follow event
                                                __tnt.trackEvent({
                                                    'network': 'Site',
                                                    'socialAction': 'Follow',
                                                    'url': 'https://www.stltoday.com/users/profile/ed54fc6a-4657-11df-a84b-0017a4a78c22'
                                                });
                                            
                                            submit_finished( $('#followed-notification-modal-ed54fc6a-4657-11df-a84b-0017a4a78c22-user .alert-success') );
                                        },
                                        error: function(oData){
                                            submit_finished( $('#followed-notification-modal-ed54fc6a-4657-11df-a84b-0017a4a78c22-user .alert-danger') );
                                        }
                                    });
                                });
                            });
                        </script>
