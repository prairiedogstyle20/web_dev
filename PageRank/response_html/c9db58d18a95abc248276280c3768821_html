<script type="text/javascript">
	
	$(document).ready(function() {
		
		$('#btn-menu-toggle').click(function(e) {

			e.preventDefault();

			$('#menu-toggle').toggleClass('activo');
			$('#menu-toggle').closest('.nav-header').toggleClass('menu-activo');

			if ($('#menu-toggle').hasClass('activo')) {
				$('#menu-toggle').removeClass('desactivo');	
			} else {
				$('#menu-toggle').addClass('desactivo');
			}

			$('body').toggleClass('con-menu-activo');

		});

        $('#btn-abrir-usuario').click(function(e) {

        e.preventDefault();

        $(this).removeClass('desactivo').addClass('activo');
        $('#cnt-usuario').closest('.nav-header').removeClass('desactivo').addClass('activo');
        $('body').addClass('con-usuario-activo');

    });

    $('#btn-cerrar-usuario').click(function(e) {

        e.preventDefault();

        $('#btn-abrir-usuario').removeClass('activo').removeClass('desactivo');
        $('#cnt-usuario').closest('.nav-header').removeClass('activo').addClass('desactivo');
        $('body').removeClass('con-usuario-activo');

    });


    $('.btn-cerrar-alerta').click(function(e) {
		//$(this).parent().fadeOut(300);
        _class = $(this).parent().attr("class");
        $(this).parent().attr("class",_class+" ocultaAlerta");
            
	});

	})

	// CABECERA FIJA
    try {
    var el = $(window),lastY = 0;
	$(window).on('scroll', function() {
           
        // deteccion scrollup / scrolldown
        var currY = el.scrollTop()
        var orientacion = (currY > lastY) ? 'down' : ((currY === lastY) ? 'none' : 'up');
        lastY = currY;

		//console.log($('.bannerandheader').nextAll('main')[0].getBoundingClientRect().top);
				
                // se fija el class del bloqueo de la cabecera (fixed)
		if ($('#cabecera').nextAll('main')[0].getBoundingClientRect().top <= -1) {
			$('header.site-header').addClass('header-fixed');
			$('body').addClass('con-header-fixed');
		} else {
			$('header.site-header').removeClass('header-fixed');
			$('body').removeClass('con-header-fixed');
		}
		
		// asignacion de class asociados a la operacion scrollup/scrolldown
		var headerClass = $('header.site-header').attr('class');                
        if(headerClass.indexOf("header-fixed") != -1)
        {
            // scrolldown
            if(orientacion == "down")
            {
                if(headerClass.indexOf("scrollup") != -1)
                {
                    $('header.site-header').removeClass('scrollup');
                    $('header.site-header').addClass('scrolldown');
                }
            }
            else
            {
                //scrollup
                if(headerClass.indexOf("scrolldown") != -1)
                    $('header.site-header').removeClass('scrolldown');
                
                if(headerClass.indexOf("scrollup") == -1)
                    $('header.site-header').addClass('scrollup');
            }
        }
        else
        {
            // por seguridad borro los class
            if(headerClass.indexOf("scrollup") != -1)
                $('header.site-header').removeClass('scrollup');
            
            if(headerClass.indexOf("scrolldown") != -1)
                $('header.site-header').removeClass('scrolldown');
        }
		

	});
} catch (er) {
    
}
</script>
