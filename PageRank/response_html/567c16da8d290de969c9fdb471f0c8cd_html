<!-- JavaScript -->
<script src="http://i.haber7.net/assets/v3/common/js/libs/jquery-1.11.3.min.js"></script>
<script src="http://i.haber7.net/assets/v3/common/js/plugin.min.js?v3"></script>
<script src="http://i.haber7.net/assets/v3/common/js/main.min.js?v15"></script>

<script>
    // Read More
    // Scroll & Tracking Action
    if ($(".readmore").length > 0) {

        // Get Page Link
        var getPageLink  = $('link[rel="canonical"]').attr('href'),
            counter = 0;
        // Scroll
        $(window).on("scroll", function() {

            if ($(window).scrollTop() < $('.readmore').eq(0).offset().top && counter == 1)
                counter = -1;

            if (counter == -1) {
                history.replaceState('', '', getPageLink);
                nextPageTrackingAction(window.location.pathname);
                counter = 0;
            }

            if($(window).scrollTop() > $('.readmore').eq(0).offset().top && counter == 0) {
                if (counter == 0) {
                    history.replaceState('', '', getPageLink + '/?detay=1');
                    nextPageTrackingAction(window.location.pathname + "detay=1");
                    counter++;
                } else {
                    history.replaceState('', '', getPageLink);
                    nextPageTrackingAction(window.location.pathname);
                }
            }else if (typeof $('.readmore').eq(1).offset() !== "undefined" && $(window).scrollTop() > $('.readmore').eq(1).offset().top && counter == 1) {
                if (counter == 1) {
                    history.replaceState('', '', getPageLink + '/?detay=2');
                    nextPageTrackingAction(window.location.pathname + "detay=2");
                    counter++;
                }
            } else if (typeof $('.readmore').eq(1).offset() !== "undefined" && $(window).scrollTop() < $('.readmore').eq(1).offset().top && counter == 2){
                if(counter == 2) {
                    history.replaceState('', '', getPageLink);
                    nextPageTrackingAction(window.location.pathname);
                    counter=0;
                }
            }
        });

        // Readmore After
        // Empty Paragraph Fix
        $('.readmore').each(function(e) {
            if($(' + p', this).text().length < 2) {
                $(' + p', this).hide();
            }
        });
    }

    // Next Page
    // Tracking Action
    function nextPageTrackingAction(triggerAnalyticsUrl) {
        var getPageTitle = $('.news-detail h1').text();

        var mainAnalyticsUA = 'UA-78232540-22';

        gtag('config', mainAnalyticsUA, {'page_path': triggerAnalyticsUrl});

        gtag('event', 'page_view', { 'send_to': 'UA-46117935-5' }); // Ekonomi

        		gtag('event', 'page_view', { 'send_to': 'UA-107122519-2' });
        
        $(".readInfo").html("");

		pp_gemius_hit(pp_gemius_identifier.replace("USED_", ""));
        yaCounter14645857.hit(window.location.href, getPageTitle);
    }
</script>

