// auto executed when the article loads for the first time
function setInitialContentId(id, url, title, description) {
setMoods(id);
}
// reset mm to before user voted
function resetMM() {
$('#mood-meter-title').html("HOW DOES THIS STORY MAKE YOU FEEL?").attr('class','rappler-label bg rappler-orange');
$('#mobile-mood-meter-title').html("HOW DOES THIS STORY MAKE YOU FEEL?").attr('class','rappler-label bg rappler-orange');
$('#mood-clicked').html("");
}
// callback function after share is done
function gyShareCallback(e) {
}
//sets the mood values given the id
function setMoods(item_id, noreset) {
$('.mm-new').attr('data-article_id', item_id);
$.get('https://mm.rappler.com/moodmeter/ranking?rsort=1&content_id=' + item_id, {}, function (data) {
var mood_el = $('#dot-mm'),
mobile_mood_el = $('#dot-mobile-mm-' + item_id);
if (mood_el.length > 0) {
$('.mm-new').css('opacity', 0);
mood_el.find('strong.happy').html(data.moods.happy + '%');
mood_el.find('strong.sad').html(data.moods.sad + '%');
mood_el.find('strong.angry').html(data.moods.angry + '%');
mood_el.find('strong.dontcare').html(data.moods.dont_care + '%');
mood_el.find('strong.inspired').html(data.moods.inspired + '%');
mood_el.find('strong.afraid').html(data.moods.afraid + '%');
mood_el.find('strong.amused').html(data.moods.amused + '%');
mood_el.find('strong.annoyed').html(data.moods.annoyed + '%');
window.setTimeout(function () {
$('.mm-new').css('opacity', 1);
}, 100);
}
if (mobile_mood_el.length > 0) {
mobile_mood_el.find('strong.happy').html(data.moods.happy + '%');
mobile_mood_el.find('strong.sad').html(data.moods.sad + '%');
mobile_mood_el.find('strong.angry').html(data.moods.angry + '%');
mobile_mood_el.find('strong.dontcare').html(data.moods.dont_care + '%');
mobile_mood_el.find('strong.inspired').html(data.moods.inspired + '%');
mobile_mood_el.find('strong.afraid').html(data.moods.afraid + '%');
mobile_mood_el.find('strong.amused').html(data.moods.amused + '%');
mobile_mood_el.find('strong.annoyed').html(data.moods.annoyed + '%');
}
// console.log(data);
var topmood,
largest = 0,
cls = 'rappler-orange',
txt = 'People are divided on this issue';
for(var i in data.moods) {
if(data.moods[i] > largest) {
topmood = i;
largest = data.moods[i];
}
}
var storymakespeople = 'THIS STORY MAKES PEOPLE';
switch (topmood) {
case 'afraid':
txt = storymakespeople + ' Afraid';
break;
case 'amused':
txt = storymakespeople + ' Amused';
break;
case 'annoyed':
txt = storymakespeople + ' Annoyed';
break;
case 'angry':
txt = storymakespeople + ' Angry';
break;
case 'dont_care':
txt = "People do not care about this story";
break;
case 'happy':
txt = storymakespeople + ' Happy';
break;
case 'inspired':
txt = storymakespeople + ' Inspired';
break;
case 'sad':
txt = storymakespeople + ' Sad';
break;
}
if(topmood == 'dont_care') {
cls = 'dontcare'
} else {
if(typeof(topmood)!='undefined') {
cls = topmood;
}
}
resetMM();
$('#mood-meter-dominant').attr('class','');
$('#mood-meter-dominant').addClass('rappler-label-top bg '+cls);
$('#mood-meter-dominant').html(txt)
$('#mobile-mood-meter-dominant').attr('class','');
$('#mobile-mood-meter-dominant').addClass('rappler-label-top bg '+cls);
$('#mobile-mood-meter-dominant').html(txt)
});
}
//set moodmeter language
function setMoodMeterLanguage(url){
if((url.indexOf('/world/regions/asia-pacific/indonesia') > -1 || url.indexOf('/indonesia') > -1) && url.indexOf('/englishedition') == -1){
} else{
}
$(".happy .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Happy";
});
$(".sad .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Sad";
});
$(".angry .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Angry";
});
$(".dontcare .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Don't Care";
});
$(".inspired .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Inspired";
});
$(".afraid .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Afraid";
});
$(".amused .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Amused";
});
$(".annoyed .bg").each(function() {
$(this).nextAll("br").get(0).nextSibling.nodeValue = "Annoyed";
});
}
// execute this once a new story is added to the page
function initStoryItem(id, url, title, pubdate, authors, sections, tags, previousUrl) {
// reset moodmeter
$('.flip-container').removeClass('hover');
// do we have an x module ?
// initialize if we do
if($('#item-'+id).find('.x_stories_module').length>0) {
var m = $('#item-'+id).find('.x_stories_module')[0];
m_id = $(m).attr('data-module_id');
head.js("https://static.rappler.com/modules/mod_rappler3_x_stories/assets/js/mod_rappler3_x_stories.js", function () {
if(typeof(getXStories)!=='undefined') {
getXStories(m_id);
createXLink(m_id);
}
});
}
// do we have a scrolly slider (aka owl carousel) ?
if($('#item-'+id).find('.rappler-scrolly-slider').length>0) {
head.load("https://static.rappler.com/templates/rappler3_default/js/owl-carousel/owl.carousel.css",
"https://static.rappler.com/templates/rappler3_default/js/owl-carousel/owl.theme.css",
"https://static.rappler.com/templates/rappler3_default/js/owl-carousel/owl.transitions.css",
"https://static.rappler.com/templates/rappler3_default/css/scrollyslider.css",
"https://static.rappler.com/templates/rappler3_default/js/owl-carousel/owl.carousel.js",
"https://static.rappler.com/templates/rappler3_default/js/scrollyslider.js", function () {
if(typeof(initOwlSlider)!=='undefined') {
initOwlSlider($('#item-'+id).find('.rappler-scrolly-slider'));
}
});
}
// resetMM();
// fetch moods
setMoods(id);
// create sharebar for new story
rapplerCreateSharebar({
title: title,
url: url,
showpin: 1,
container: 'storypageShare-' + id,
orientation: 'vertical',
showcount: 'top',
via: '@rapplerdotcom'
}, true, true, id)
// - meta keywords
$('#rappler3-metakey-wrapper-' + id).empty().html($('#metakeyword_content-' + id).html());
// - show comments
showComments(url, id, title);
// - add the comment button with number of shares
getCommentCount(id);
// NOTE : updating of the url needs to be done in initScrolly inside storypage.php
//  because that's where we detect the direction of the scroll
//  detecting the direction is crucial so we know when
//  to update the url and moodmeter
// - load twitter again
twttr.widgets.load()
// HAM : content insights after we get comments
// var fn = function(r) {
// _ain.id = "1258";
// _ain.postid = id;
// _ain.url = url;
// _ain.title = title;
// _ain.pubdate = pubdate;
// _ain.authors = authors;
// _ain.sections = sections;
// _ain.tags = tags;
// _ain.maincontent = "#story-area-" + id;
// _ain.comments = r.streamInfo.commentCount.toString();
// _ain.track();
// }
// rapplerGetCommentcount(id,fn);
// TODO : only execute if this article is brandrap
}
// listen to mood votes
head.js("https://static.rappler.com/components/com_rappler/js/jquery.cookie.min.js",
"https://static.rappler.com/modules/mod_rappler3_moods/js/moodmeter.min.js",
"https://static.rappler.com/modules/mod_rappler3_moods/js/moodmeter_client.min.js", function () {
moodMeterClient = new MoodMeterClient({domain: "https://mm.rappler.com"});
moodMeterClient.initClient(function (isExists, hash) {
// listen to vote clicks
$(document).click(function (e) {
if ($(e.target).hasClass('dot_mmm_circle')) {
var mood_type_id = $(e.target).attr('rel'),
mood = '',
moodClicked = '',
moodText = '',
moodClass = $('#mood-clicked').attr('class'),
contentId = $(e.target).parents('div.mm-new').attr('data-article_id'),
mobile_parent = $(e.target).parents('#dot-mobile-mm-' + contentId);
// console.log(contentId);
// console.log(mood);
// console.log(mood_type_id);
if ($(e.target).hasClass('happy')) {
mood = "Happy";
moodText = 'Happy';
}
if ($(e.target).hasClass('sad')) {
mood = "Sad";
moodText = 'Sad';
}
if ($(e.target).hasClass('angry')) {
mood = "Angry";
moodText = 'Angry';
}
if ($(e.target).hasClass('inspired')) {
mood = "Inspired";
moodText = 'Inspired';
}
if ($(e.target).hasClass('afraid')) {
mood = "Afraid";
moodText = 'Afraid';
}
if ($(e.target).hasClass('amused')) {
mood = "Amused";
moodText = 'Amused';
}
if ($(e.target).hasClass('dontcare')) {
mood = "Don't Care";
moodText = "Don't Care";
}
if ($(e.target).hasClass('annoyed')) {
mood = "Annoyed";
moodText = 'Annoyed';
}
moodClicked = mood.toLowerCase()
if (moodClicked == "don't care") {
mood = 'dontcare';
moodText = "Don't Care";
}
var prefix = "I am";
if (mood_type_id == 4) {
prefix = "I";
}
//printMoodSponsorship(mood);
// record the vote
$.post('https://mm.rappler.com/moodmeter/vote', {
hash: hash,
mood_type_id: mood_type_id,
mood_type_name: mood,
content_id: contentId,
cookies_enabled: 1
}, function (data) {
var moodTypeId = 0;
if (!data.content_mood_id) {
if (mobile_parent.length == 1) {
// voting on mobile
$('#mmm_thankyou_title').html('You have already voted <span class="' + moodClicked + '">' + moodText + '</span>')
$('#mmm_popup_thanks').modal('show');
rapplerCreateSharebar({
title: prefix + ' ' + mood + ' - ' + document.title,
url: window.location.href,
showpin: 0,
container: 'mmm_thankyou_share',
orientation: 'horizontal',
showcount: 'none',
via: 'rapplerdotcom'
}, false, false, contentId)
} else {
// update share this vote
rapplerCreateSharebar({
title: prefix + ' ' + mood + ' - ' + document.title,
url: window.location.href,
showpin: 0,
container: 'voteshare',
orientation: 'horizontal',
showcount: 'none',
via: 'rapplerdotcom'
}, false, false, contentId)
// update the mood counts
setMoods(contentId);
// do the flip
$('.flip-container').toggleClass('hover');
// update the flip panel text
$('#mood-meter-title').html('You already voted ' + moodText).attr('class','rappler-label bg '+ moodClicked);
$('#mobile-mood-meter-title').html('You already voted ' + moodText).attr('class','rappler-label bg '+ moodClicked);
$("#mood-clicked").html(moodText).attr('class','').addClass(moodClicked);
$("#mood-clicked-after").html(moodText).attr('class','').addClass(moodClicked);
var oldMoodClicked = moodClicked;
$( "#mood-back" ).one( "click", function() {
switch(data.mood_type_slug) {
case 'happy':
moodClicked = 'Happy';
moodText = 'Happy';
break;
case 'sad':
moodClicked = 'Sad';
moodText = 'Sad';
break;
case 'angry':
moodClicked = 'Angry';
moodText = 'Angry';
break;
case 'dontcare':
moodClicked = "Don't Care";
moodText = "Don't Care";
break;
case 'amused':
moodClicked = 'Amused';
moodText = 'Amused';
break;
case 'afraid':
moodClicked = 'Afraid';
moodText = 'Afraid';
break;
case 'annoyed':
moodClicked = 'Annoyed';
moodText = 'Annoyed';
break;
}
$('#mood-meter-title').html('You already voted ' + moodText).attr('class','rappler-label bg '+ data.mood_type_slug);
$('#mobile-mood-meter-title').html('You already voted ' + moodText).attr('class','rappler-label bg '+ data.mood_type_slug);
$("#mood-clicked").html(moodText).attr('class','').addClass(data.mood_type_slug);
$("#mood-clicked-after").html(moodText).attr('class','').addClass(data.mood_type_slug);
//$("#mood-sponsorship").html('');
});
var relatedmood = "";
if (moodClicked == "Don't Care") {
relatedmood = 'Dont+care';
} else {
relatedmood = moodClicked;
}
$.getJSON('https://mm.rappler.com/api/votes/voted-top-in-moods', {type: relatedmood}, function (data) {
$('#latestmoodstories').html('');
$.each(data.results, function (index, entry) {
if (index < 5) {
var b = '1px solid #eaeaea';
if (index == (data.length - 1)) {
b = '';
}
$('#latestmoodstories').append(
$('<div>').append(
$('<h6>').append(
$('<a>').addClass('rappler-headline link').attr('href', '/?option=com_rappler&task=mobileapi.rdir&id=' + entry.contentId).html(entry.title)
)
)
).append('<hr>');
}
});
});
}
} else {
if (mobile_parent.length == 1) {
$('#mmm_thankyou_title').html('Thank you for voting <span class="' + moodClicked + '">' + moodText + '</span>')
$('#mmm_popup_thanks').modal('show');
rapplerCreateSharebar({
title: prefix + ' ' + mood + ' - ' + document.title,
url: window.location.href,
showpin: 0,
container: 'mmm_thankyou_share',
orientation: 'horizontal',
showcount: 'none',
via: 'rapplerdotcom'
}, false, false, contentId)
} else {
// update share this vote
rapplerCreateSharebar({
title: prefix + ' ' + mood + ' - ' + document.title,
url: window.location.href,
showpin: 0,
container: 'voteshare',
orientation: 'horizontal',
showcount: 'none',
via: 'rapplerdotcom'
}, false, false, contentId)
// update the mood counts
setMoods(contentId);
// do the flip
$('.flip-container').toggleClass('hover');
// update the flip panel text
window.setTimeout(function () {
$("#mood-clicked-after").html(moodText).attr('class','').addClass(moodClicked);
}, 100);
$('#mood-meter-title').html('<? echo $mood_name["youalreadyvoted"]; ?> ' + moodText).attr('class','rappler-label bg '+ moodClicked);
$('#mobile-mood-meter-title').html('<? echo $mood_name["youalreadyvoted"]; ?> ' + moodText).attr('class','rappler-label bg '+ moodClicked);
// fetch related news
var relatedmood = "";
if (moodClicked == "Don't Care") {
relatedmood = 'Dont+care';
} else {
relatedmood = moodClicked;
}
$.getJSON('https://mm.rappler.com/api/votes/voted-top-in-moods', {type: relatedmood}, function (data) {
$('#latestmoodstories').html('');
$.each(data.results, function (index, entry) {
if (index < 5) {
var b = '1px solid #eaeaea';
if (index == (data.length - 1)) {
b = '';
}
$('#latestmoodstories').append(
$('<div>').addClass('stats').append(
$('<h6>').addClass('top-in-moods-title').append(
$('<a>').addClass('rappler-headline link').attr('href', '/?option=com_rappler&task=mobileapi.rdir&id=' + entry.contentId).html(entry.title)
)
)
).append('<hr>');
}
});
});
}
recordGigAction('mood_vote',{
content_id:contentId,
mood_type_id:mood_type_id,
mood_type_name:mood
});
recordCommunityAction('mood_vote', {
content_id:contentId,
mood_type_id:mood_type_id,
mood_type_name:mood
});
}
})
}
})
})
})
$('#mood-back').click(function () {
$('.flip-container').removeClass('hover');
});
