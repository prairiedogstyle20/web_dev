                    jQuery(".search").submit(function() {
                        var label = jQuery("input:text[name=busca]").val();
                        if (label == "") {
                            alert("Ingrese el valor de la busqueda");
                            window.open("http://www.ntn24.com/", "_self");
                        } else {
                            var ruta = "http://www.ntn24.com/search/site/"+ label;
                            window.open(ruta);
