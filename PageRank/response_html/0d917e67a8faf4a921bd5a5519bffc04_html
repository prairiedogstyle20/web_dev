var CBSNEWS = CBSNEWS || {};
    CBSNEWS.features = {"video-preview-mobile-front_door":true,"vilynx-mobile-video":false,"vilynx-mobile-video_playlist":false,"vilynx-mobile-video_door":false,"vilynx-mobile-live_streaming_player":false,"vilynx-mobile-live_blog":false,"vilynx-mobile-media_post":false,"vilynx-mobile-news_item":false,"vilynx-amp":false,"vilynx":true,"live-cbsn-rundown":true,"live-cbs-sports-hq-rundown":true,"taboola":true,"election-2018":true,"adsSharethrough":true,"adsSonobi":true,"adsIndexExchange":true,"adsIMA":true,"adsGPT":true,"ads":true,"easeLive":false};

    
(function () {
	var oldFeatures = CBSNEWS.features || {};

	CBSNEWS.features = {
		featureStore: {},
		weightedFeatures: [],

		push: function (features) {
			for (var feature in features) {
				var featureValue = features[feature];

				// Fail fast - for inherited features from parent window
				if (typeof featureValue === 'function') continue;

				(function(feature){
					Object.defineProperty(CBSNEWS.features, feature, {
						get: function() {
							var featureValue = CBSNEWS.features.getFeatureValue(feature);
							window.console && console.log('features.get', feature, featureValue);
							return featureValue;
						},
						set: function(featureValue) {
							window.console && console.log('features.set', feature, featureValue);
							CBSNEWS.features.push({feature: featureValue});
						}
					});
				})(feature);

				CBSNEWS.features.featureStore[feature] = featureValue;
			}
		},

		getFeatureValue: function (feature) {
			var featureValue = CBSNEWS.features.featureStore[feature];

			if (/^(?:(?:\||^)\d+:[^\|]+)+$/.test(featureValue)) {
				try {
					var weights = [];
					featureValue.split('|').forEach(function(weight) {
						weight = weight.split(':');
						for (var i = 0; i < weight[0]; i++) {
							weights.push(weight[1]);
						}
					});

					featureValue = weights[Math.floor(Math.random() * weights.length)];

					window.console && console.log('features.getFeatureValue', feature, featureValue, CBSNEWS.features.featureStore[feature]);

					// now remember calculated value
					CBSNEWS.features.featureStore[feature] = featureValue;

					// log access to weighted features
					CBSNEWS.features.weightedFeatures.push(feature);
				} catch (e) {
					window.console && console.error('features.getFeatureValue', 'Error parsing feature weight string', featureValue);
				}
			}

			return featureValue;
		},

		isFeatureEnabled: function (feature) {
			var featureValue = CBSNEWS.features[feature];
			return featureValue !== false && parseInt(featureValue) !== 0;
		},

		isFeatureDisabled: function (feature) {
			var featureValue = CBSNEWS.features[feature];
			return featureValue === false || parseInt(featureValue) === 0;
		},

		executeIfEnabled: function (keys, cb, thisArg, argsArray) {
			for (var i = 0; i < keys.length; i++) {
				if (CBSNEWS.features.isFeatureDisabled(keys[i])) {
					return;
				}
			}

			cb.apply(thisArg || null, argsArray);
		},

		executeWithConsent: function (keys, cb, thisArg, argsArray) {
			if (CBSNEWS.gdprConsent) {
				window.console && console.log('gdprConsent.executeWithConsent', 'consent already received', window, keys, cb, thisArg, argsArray)
				CBSNEWS.features.executeIfEnabled(keys, cb, thisArg, argsArray);
			} else {
				var done = false;

				window.addEventListener('cbsnews.gdprConsent.set', function () {
					// Fail fast
					if (CBSNEWS.gdprConsent !== true) {
						window.console && console.log('gdprConsent.executeWithConsent', 'consent refused', window, keys, thisArg, argsArray)
						return
					}

					// Don't let run more than once
					if (done) return;
					done = true;

					window.console && console.log('gdprConsent.executeWithConsent', 'consent given', window, keys, cb, thisArg, argsArray)
					CBSNEWS.features.executeIfEnabled(keys, cb, thisArg, argsArray);
				}, false)
			}
		}
	}

	CBSNEWS.features.push(oldFeatures);

	var frameElement;
	var parentFeatures;
	try {
		frameElement = window.frameElement;
		parentFeatures = window.parent.CBSNEWS.features;
		if (frameElement) {
			CBSNEWS.features.featureStore = parentFeatures.featureStore;
			CBSNEWS.features.weightedFeatures = parentFeatures.weightedFeatures;
		}
	} catch (e) {
		frameElement = null;
	}
})();


