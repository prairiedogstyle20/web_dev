			for( let ad_unit in FFADS.units ) if( FFADS.units[ad_unit].ad_type == 'dfp_prebid' && FFADS.units[ad_unit].size_mappings ) for( let i = 0; i < FFADS.units[ad_unit].size_mappings.length; i++ ) if( window.innerWidth >= FFADS.units[ad_unit].size_mappings[i].screen_width ) {
