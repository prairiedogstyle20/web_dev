							}
							else params[param_name] = FFADS.units[ad_unit].bidders[i][param];
						}
						bids.push( { bidder: FFADS.units[ad_unit].bidders[i].name, params: params } );
					}

					if( FFADS.units[ad_unit].repeating ) for( let i = 0; i < FF_REP_COUNT; i++ ) FFADS.pb_units.push( { sizes: FFADS.units[ad_unit].sizes, bids: bids, code: 'REP_' + i } );
