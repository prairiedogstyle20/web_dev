			for( let i in FFADS.content_order ) {
				FFADS.debug( 'Checking if ' + FFADS.content_order[i] + ' is valid on this page' );
				if( FFADS.units[FFADS.content_order[i]].ad_type == 'custom' || FFADS.units[FFADS.content_order[i]].sizes ) new_unit_obj[FFADS.content_order[i]] = FFADS.units[FFADS.content_order[i]];
