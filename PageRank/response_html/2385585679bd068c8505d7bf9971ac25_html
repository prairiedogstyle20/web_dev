  <script>
    //sticky menu
    var sticky = $('#sticky-header').position().top;
    var windowTopGreater = true;
    var windowTopLess = true;

    function stickyHeader() {
      var windowTop = $(window).scrollTop();

      //console.log(sticky + ':' + windowTop);
      //console.log($("#menu-btn").is(':checked'));


      if (windowTop > sticky) {
        $('#sticky-header').addClass('sticky');
        $('#sticky-header .header').addClass('hamburgler');
        if (windowTopGreater){
          var searchPopup = parseInt($("#search-popup").css("left"))
          windowTopGreater = false;
          windowTopLess = true;
        }
        if (searchPopup > 50){ $('#search-popup').hide();}

      } else {
        $('#menu-btn').prop('checked', false);
        $('#sticky-header').removeClass('sticky');
        $('#sticky-header .header').removeClass('hamburgler');
        if(windowTopLess){
          var searchPopup = parseInt($("#search-popup").css("left"))
          windowTopLess = false;
          windowTopGreater = true;
        }
        if (searchPopup < 50){ $('#search-popup').hide();}
      }
    }

    $(function() {
      $(window).scroll(stickyHeader);
      stickyHeader();
    });
  </script>
</header>

