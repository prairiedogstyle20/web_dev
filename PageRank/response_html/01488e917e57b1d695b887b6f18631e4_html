//----- our customizations to the Browser-Update.org ------------
//--- makes the x button
function makeClosebtn() {
    let closeBtn = document.getElementById('buorg');
    closeBtn.insertAdjacentHTML('afterbegin', '<a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>');
}
function closeNav() {
    // removes the margin-top added to the html
    document.getElementsByTagName('html')[0].id = "HTML";
    document.getElementById('HTML').style.margin = "0px";
    // hides the modal
    document.getElementById('buorg').style.display = "none";
};

