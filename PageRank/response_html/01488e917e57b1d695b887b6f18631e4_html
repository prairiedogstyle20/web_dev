        <script src="https://rawgithub.com/darkskyapp/skycons/master/skycons.js"></script>

        <script>

      // Get weather info via ajax instead of cook (DEV-164)
      $(document).ready(function(){
        let weatherFeed = 'https://enrichmentservices.fccinteractive.com/weather.php?geocoordinates=46.854112,-96.9689393'
        $.ajax({
          type: "GET",
          url: weatherFeed,
          dataType: "json",
          success: (result) => {
             // console.log("weather ajax SUCCESS!:", result);

             // Display the current temperature in the page
             let realTimeTemp = result.currently.temperature;
             $('.temperature').html(realTimeTemp + '&deg;')

             // Display the current weather icon in the page
             let realTimeIcon = result.currently.icon;
             // console.log("realTimeIcon", realTimeIcon);
             // The name of the class must match the name of the icon
             $('#icon').removeClass(realTimeIcon);
             $('#icon').addClass(realTimeIcon);
             $('#icon').show();
             // set the skycon in the current context
             $(function () {
               var skycons = new Skycons({ "color": "#1e1f21" })
               /* Get class for each instance of skycon canvas */
               $('.skycon canvas').each(function (i, elem) {
                 skycons.add(elem, skyconType(elem.className))
               })
               skycons.play()
             });

             /* Activate Skycons */
             function skyconType(icon) {
               if (icon === 'rain')
                 return Skycons.RAIN
               else if (icon === 'snow')
                 return Skycons.SNOW
               else if (icon === 'sleet')
                 return Skycons.SLEET
               else if (icon === 'hail')
                 return Skycons.SLEET
               else if (icon === 'wind')
                 return Skycons.WIND
               else if (icon === 'fog')
                 return Skycons.FOG
               else if (icon === 'cloudy')
                 return Skycons.CLOUDY
               else if (icon === 'partly-cloudy-day')
                 return Skycons.PARTLY_CLOUDY_DAY
               else if (icon === 'partly-cloudy-night')
                 return Skycons.PARTLY_CLOUDY_NIGHT
               else if (icon === 'clear-day')
                 return Skycons.CLEAR_DAY
               else if (icon === 'clear-night')
                 return Skycons.CLEAR_NIGHT

               return Skycons.CLOUDY
             }


          },
          error: (error) => {
            console.log("weather ajax FAILED", url, error);
          }
        });
      });

      var once = true;
      $('.search-menu').on('click', function(){
        var search_position = $('.search-menu').position();
        $('#search-popup').css('left', search_position.left - 134);
        $('#search-popup').fadeToggle("fast");
        $('.search-body input').focus();
        if ($('#search-popup').is(":visible")) {
          clickAwayEvent();
        }
      });

      function clickAwayEvent() {
        if (once) {
          once = false;
          $('.container').on('click', function() {
            // event.stopPropagation();
            $('.container').unbind('click');
            if ($('#search-popup').is(":visible")) {
              console.log("test");
              $('#search-popup').fadeToggle("fast");
              once = true;
            }
          });
        }
      }

      function searchFormSubmit(search_input) {
        if (search_input.val().length > 1) {
          let sanitizedSearchTerm = search_input.val().replace(/[^a-zA-Z0-9 ]/g, "")
          window.location.href = "/search/?q=" + encodeURIComponent(sanitizedSearchTerm);
          // console.log("Search term raw:\n", search_input.val());
          // console.log("Search term cleaned:\n", sanitizedSearchTerm);
        } else {
          alert("Please type in something to search");
          search_input.focus();
        }
      }

      // Desktop form
      $( "#search-popup form" ).submit(function( event ) {
        var search_input = $('#search-popup input');
        searchFormSubmit(search_input);
        event.preventDefault();
      });
      // Desktop/mobile form submit when clicking icon
      $( "form > .input-search-icon" ).on("click", function(event) {
        var search_input = $('#search-popup input');
        if (search_input.val().length < 1) { search_input = $('#search-popup-mobile input'); }
        searchFormSubmit(search_input);
        event.preventDefault();
      });
      // Mobile Form
      $( "#search-popup-mobile form" ).submit(function( event ) {
        var search_input = $('#search-popup-mobile input');
        searchFormSubmit(search_input);
        event.preventDefault();
      });
          </script>

    
