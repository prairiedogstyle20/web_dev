		(function(){
			dd.hooks.addAction( 'dd_ads_slot_recommendation_engine', 'dd-ads', function( post_id ){
				var video_ads = document.getElementById( 'dd-video-' + post_id );

				if (video_ads) {
					dot.utils.events.throttleFn( dot.utils.video.render_video( video_ads ), 500 );
				}
			} );
		})();
	</script>
<script>
		(function($){
			dd.hooks.addAction( 'dd_continuous_article_loaded', 'dd-ads', function(articleBody) {
				if (!window.canRunAds || 1 === parseInt(window.tmntarget['tmn_disable_ads'], 10)) {
					return;
				}

				// Run complex ads
				var complex_leaderboard_unit = ('true' !== DD_OPT.is_mobile)? '3x3' : 'top';
				var complex_content_unit = ('true' !== DD_OPT.is_mobile)? '100x100' : 'content';
				CMNUNT.load(complex_leaderboard_unit, { containerId: 'cmn_ad_Y_L_1_1-'+DD_OPT.current_id });
				CMNUNT.load(complex_content_unit, { containerId: 'cmn_ad_Y_R_0_1-'+DD_OPT.current_id, lazyLoad: true });
				CMNUNT.load(complex_content_unit, { containerId: 'cmn_ad_Y_R_1_1-'+DD_OPT.current_id, lazyLoad: true });

				dot.utils.complex.loadInContentAds(articleBody, '.dd-content-body-wrapper > div > p:nth-of-type(4n+2)');
			} );

			$( document ).ready(function() {
				dot.events.EventTargetProvider.getEventTarget().addEventListener(
					dot.newsreader.content.InfiniteScroll.EventType.RENDERED, function(e){
						var page = e.detail.paged;
						var container = e.detail.container;
						var adsContainer = document.createElement( 'div' );
						adsContainer.className = 'fw_dailydot';

						var ads = document.createElement( 'div' );
						ads.className = 'adUnits';
						ads.id = 'cmn_ad_Y_R_0_2-'+page;

						adsContainer.appendChild(ads);
						container.appendChild(adsContainer);
						var pagination = document.getElementById( 'dd-pagination' );
						var footer = document.getElementById( 'dd-footer' );
						if (pagination) {
							pagination.remove();
							container.appendChild( pagination );
						}
						if (footer) {
							footer.remove();
							container.appendChild( footer );
						}

						// Run complex ads
						var complex_unit = ('true' !== DD_OPT.is_mobile)? '100x100' : 'content';
						CMNUNT.load(complex_unit, { containerId: ads.id, refreshable: true, lazyLoad: true });
					}
				);

				if ( "1" === DD_OPT.is_singular && window.canRunAds && 1 !== parseInt( window.tmntarget['tmn_disable_ads'], 10 ) ) {
					var articleBody = document.querySelector('.dd-article-body');
					dot.utils.complex.loadInContentAds(articleBody, '.dd-content-body-wrapper > div > p:nth-of-type(4n+2)');
				}
			});
		})(jQuery);
	</script>
<script>
	(function(){
		dd.hooks.addAction( 'dd_continuous_article_loaded', 'dd-ads', function( content ) {
			window._taboola = window._taboola || [];
			_taboola.push({
				mode:'thumbnails-a',
				container: 'dd-recommended-block-' + DD_OPT.current_id,
				placement: 'Below Article Thumbnails',
				target_type: 'mix'
			});
			_taboola.push({
				article: 'auto',
				url: DD_OPT.current_id
			});
		} );
	})();
</script>
