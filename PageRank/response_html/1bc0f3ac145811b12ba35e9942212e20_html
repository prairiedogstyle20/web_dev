	if (!url)
	url = location.search;
	name = name.replace(/\[/, "\\\[").replace(/\]/, "\\\]");
	var regex = new RegExp("[\\?&]" + name + "=([^&#]*)", 'i');
	var results = regex.exec(url);
	console.log("results: ",results);
	return results === null ? null : decodeURIComponent(results[1]);
	}
