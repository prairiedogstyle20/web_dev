                    <div class="hide-for-large">
                                                    <!-- Team info -->
<div class="show-for-large">
    <div class="created-date">Created February 11, 2019</div>
    <div class="dotted-line mt5 mb10 mr ml"></div>
</div>
<div class="co-info ml">
        <a
        class="avatar-contain js-profile-avatar flex-shrink"
        href="#"
                data-open-modal='team-members-modal'    >
            <img class="thumbnail media-round" src="https://images.gofundme.com/wb_eahcxG2u_e242ek6Iy2vFTmg=/120x120/https://d2g8igdw686xgo.cloudfront.net/36996780_1550338791913987_r.jpeg" alt="">
        </a>
    <div class="co-details co-details--teams">
        <a href="#" data-open-modal="team-members-modal">
            Randall Thom        </a>
        <i class="fa fa-envelope js-contact-organizer-open" data-gfm-analytics-element="btn_donate_contactorganizer" data-open-modal="contact-organizer-modal"></i>
                <div class="pills-contain">
                        <a href="/pet-animal-fundraising"
           class="icon-link category-link-name js-category-link"
           data-gfm-analytics-element="btn_donate_cocategorylink"
           data-gfm-analytics-meta="Animals">
            <i class="fa fa-tag fa--fixed-width"></i>&nbsp; <span class="text-small" style="color:#666;">Animals</span>
        </a>
                <a href="/search/us/garrison-mn-fundraising" class="icon-link location-name js-location-link">
            <i class="fa fa-map-marker fa--fixed-width"></i>&nbsp; GARRISON, MN        </a>
            </div>
                    <script>GFM.facepileUseFeedService = true;</script>
                <div class="teams-badge-facepile-contain js-facepile-contain">
