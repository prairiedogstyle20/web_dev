__tnt.metering.access.e.push(function(){
    var count = __tnt.recent.count.get();
    var limit = __tnt.metering.config.sub_limit;
    var remaining = (limit - count);

    if(count > limit){count = limit;}
    if(remaining < 0){remaining = 0;}

    var messageClass = 'access';
    if(__tnt.metering.config.referred){ messageClass = 'referred'; }
    if(__tnt.metering.config.read){ messageClass = 'read'; }

    // initialize by finding generic message for this count
    message = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+']').first();

    // find messages at this count specifically for users who are not logged in
    if(!__tnt.user.loggedIn){
        var messageCheck = {length:0};
            messageCheck = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+'][data-registered=false]').first();
        if(messageCheck.length && message.attr('data-registered') == "false"){ message = messageCheck; }
    }

    // find messages at this count specifically for users who are logged in
    if(__tnt.user.loggedIn){
        var messageCheck = {length:0};
            messageCheck = $('#template-metering').find('.template.meter.'+messageClass+'[data-count='+count+'][data-registered=true]').first();
        if(messageCheck.length && message.attr('data-registered') == "true"){ message = messageCheck; }
    }

    // find messages specifically for display after the presentation of the wall
    try{
        if((typeof(message)=="undefined" || !message.length) && window.sessionStorage){
            if(sessionStorage.getItem('__tnt.metering.wall.viewed')=="1"){
                message = $('#template-metering').find('.template.meter.'+messageClass+'[data-registered=true][data-after-wall=true]').first();
            }
        }
    }catch(e){}

    // find any generic message that doesn't require a count or an after wall state
    if(typeof(message)=="undefined" || !message.length){
        message = $('#template-metering').find('.template.meter.'+messageClass+':not([data-count]):not([data-after-wall])').first();
    }

    if(typeof(message)!="undefined" && message.length){
        if(!$('#notification-area').length){
            $('#asset-content').find('.asset-body').append('<div id="notification-area"></div>');
        }
        __tnt.template(
            message[0],
            $('#notification-area')[0],
            true,
            [{
                'element': '.count',
                'html': String(count)
            },{
                'element': '.total',
                'html': String(limit)
            },{
                'element': '.remaining',
                'html': String(remaining)
            }],
            function(el){
                if(__tnt.user.authToken){
                    $(el).find('.logged-in').show();
                }else{
                    $(el).find('.not-logged-in').show();
                }
                $(el).show();
            }
        );
    }

    try{
        if(window.sessionStorage){
            sessionStorage.removeItem('__tnt.metering.wall.viewed');
        }
    }catch(e){}

});
