   var searchChart_735720;
   searchChart_735720 = new FXautoComplete;
   searchChart_735720.Element.searchTextInputId = 'searchText_735720';
   searchChart_735720.Element.resultsInnerContainerId = 'searchTopResults_735720';
   searchChart_735720.Element.resultsOuterContainerId = 'subContainer735720';
   searchChart_735720.Element.formElementId = 'combineSearchForm_735720';
   searchChart_735720.Element.hasTabs = true;
   searchChart_735720.Element.tabsContainerId = 'search_tabs_735720';
   searchChart_735720.Element.tabsType = 'li';
   searchChart_735720.Element.portfolioPairId = 0;
   searchChart_735720.Element.searchType = 'portfolio';
   searchChart_735720.Element.tabsDivId = 'searchBoxTabsTopTabs_735720';
   searchChart_735720.Element.countriesType = 'td';
   searchChart_735720.Element.dynamicVar = '735720';
   searchChart_735720.Element.url = '/search/service/search?searchType=advinion';
   searchChart_735720.Element.mainResultRowIdPrefix = 'searchRowIdtop_';
   searchChart_735720.Element.tabsPrefix = 'search_box_top_'+ 735720;
   searchChart_735720.Element.countryRowPrefix = 'tdCountry735720_';
   searchChart_735720.Element.countriesWrapperId = 'countries_wrapper_735720';
   searchChart_735720.Element.countriesTableId = 'countries_table_735720';
   searchChart_735720.Element.countriesDefaultText  = 'Todos los países';
   searchChart_735720.Element.hiddenNoResultsDefine = 'noResultsDefinesearchTopResults_735720';
   searchChart_735720.Element.hiddenDocDirectionId = 'doc_directionsearchTopResults_735720';
   searchChart_735720.setTabsText('[{"id":"search_box_top_735720All","name":"Todo","select":true},{"id":"search_box_top_735720Forex","name":"Divisas","select":false},{"id":"search_box_top_735720Commodities","name":"M. primas","select":false},{"id":"search_box_top_735720Indices","name":"\u00cdndices","select":false},{"id":"search_box_top_735720Stocks","name":"Acciones","select":false},{"id":"search_box_top_735720ETFs","name":"ETFs","select":false},{"id":"search_box_top_735720Bonds","name":"Bonos","select":false},{"id":"search_box_top_735720Countries","name":"Todos los pa\u00edses","select":false}]');
   searchChart_735720.inputFieldBind( $('#searchText_735720') );

   searchChart_735720.getSymbol=function(selectedRowObj){
       return $('#' + selectedRowObj.attr('id') + ' .second').html();
   }

   $('#'+ searchChart_735720.Element.formElementId).submit(function() {
			if(searchChart_735720.Element.portfolioPairId != 0 && searchChart_735720.Element.portfolioPairId != undefined){
       			searchChart_735720.selectSymbol($('table[advinionSearch] tr[data-pair_id="' +searchChart_735720.Element.portfolioPairId+ '"]'));
       			$('#searchText_735720').val($('#searchText_735720').attr('default'));
			}
           return false;
   });

   searchChart_735720.selectSymbol=function(selectedRowObj){

		var pair_id = selectedRowObj.data('pair_id');
		var pair_type = selectedRowObj.data('pair_type');
		var futures_categories = selectedRowObj.data('futures_categories');
		var candles = selectedRowObj.data('candles');
		var decimal = selectedRowObj.data('decimal');
		var extraData, strSymbolName, strMarket;
		strSymbolName = strip(searchChart_735720.getSymbol(selectedRowObj));
		
		switch(pair_type)
		{
			case 'currency':
			  	if (futures_categories == 'FX'){
			  		extraData = 'indice';
			  		strMarket = futures_categories;
				}else{
					extraData = 'forex';
			  		strMarket = null;
			  		strSymbolName =  strip($('#' + selectedRowObj.attr('id') + ' .symbolName').html());
				}
			  	break;
			case 'Equities':
				extraData = 'cfds';
		  		strMarket = 2;
			  	break;
			default:
				extraData = pair_type;
	  			strMarket = futures_categories;
		}
		extraData += ',4';
		
		var objChart = investing_GetProChartObject('735720');
		objChart.SetExtraData(extraData);
		objChart.SetDecimalPlaces(decimal); 
		objChart.SetTimeScaleControls(candles, candles); 
		objChart.ChangeTimeFrame("5M");
		objChart.ChangeSymbolNoPush(strMarket, pair_id, strSymbolName);
		$('#chart_title_735720').html(strSymbolName);

		$('#chart_title_735720').data('extraData', extraData);
		$('#chart_title_735720').data('strMarket', strMarket);
		$('#chart_title_735720').data('decimal', decimal);
		$('#chart_title_735720').data('candles', candles);
		$('#chart_title_735720').data('pair_id', pair_id);
		$('#chart_title_735720').data('strSymbolName', strSymbolName);
		
		searchChart_735720.closeSearchResultBox();
   }
   
   searchChart_735720.bindResults=function(rowElement){
       var obj = this;
       $(rowElement)
           .mousemove(function(){
        	   obj.actionMouseMoveOnRow(rowElement,obj,searchChart_735720);
           })
           .click(function(){
               searchChart_735720.selectSymbol($(rowElement));
           });
   }

   searchChart_735720.buildSearchResults=function(data){
       var obj = this;
       var resultsHtml = '';
       var current_tab = obj.previousTab;
       obj.changeCurrentTabDisplay.call(obj,current_tab);
       obj.firstRowId = 0;
       var symbol = '', tr_data = '';
       var counter = 0;
       if(data['All'][0] !== 'No hay resultados para su búsqueda'){
         $.each(data['All'], function(index,item){

       	if(item !== undefined && (item['tab_ID'].toLowerCase() == current_tab.toLowerCase() || current_tab == 'All'))
           {
               symbol = searchChart_735720.boldSymbol(obj.searchText , item['symbol']);
               tr_data = ' data-pair_type="' +item['advinion']['pair_type']+ '" data-futures_categories="' +item['advinion']['futures_categories']+ '" data-candles="' +item['advinion']['str_candles']+ '" data-decimal="' +item['advinion']['pair_decimal_last']+ '" ';
               
               //check for exact match
               if((obj.searchText).toLowerCase() === (item['symbol']).toLowerCase() && counter == 0)
                   resultsHtml += '<table advinionSearch><tbody><tr ' +tr_data+ ' data-content-id="735720" data-pair_id="'+ item['pair_ID'] +'" class="row selectedRowExactMatch hoverSearch" id="'+obj.Element.mainResultRowIdPrefix+index+'">';
               else if(counter == 0)
                   resultsHtml += '<table advinionSearch><tbody><tr ' +tr_data+ ' data-content-id="735720" data-pair_id="'+ item['pair_ID'] +'" class="row hoverSearch" id="'+obj.Element.mainResultRowIdPrefix+index+'">';
               else
                   resultsHtml += '<tr ' +tr_data+ ' class="row "data-content-id="735720" data-pair_id="'+ item['pair_ID'] +'" id="'+obj.Element.mainResultRowIdPrefix+index+'">';

               counter++;
               resultsHtml += '<td class="first symbolName" pairid="'+item['pair_ID']+'" id="symbol_'+item['symbol']+'" link="'+item['link']+'">'+symbol+'</td><td class="second" class="dirLtr">'+item['name']+'</td>';
               resultsHtml += '<td class="third typeExchange arial_11 text_align_lang_base_2 dir_lang_base_1" pairid="'+item['pair_ID']+'" id="type_'+item['pair_ID']+'" link="'+item['link']+'">'+item['pair_type_label']+'';
               if(item['pair_type']=='Equities' || item['pair_type']=='indice' || item['pair_type']=='etf' || (item['exchange_name_short']))
               {
                   resultsHtml += ' - '+item['exchange_name_short'];
               }
               resultsHtml += '</td></tr>';
           }
         });
       }
       if(counter == 0){
       	resultsHtml = '<table><tbody><tr id="no_results"><td class="align_center noResults">No hay resultados para su búsqueda</td></tr></tbody></table>';
           obj.arrowfocus = "tabs";
           obj.firstRowId = 'no_results';
           $('#'+obj.Element.resultsInnerContainerId).html(resultsHtml);
           return;
       }
       resultsHtml += '</tbody></table>';
       $('#'+obj.Element.resultsInnerContainerId).html(resultsHtml);
   }

</script>
