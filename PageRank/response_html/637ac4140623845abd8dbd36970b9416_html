        },
        "headline": "Rep. John Lewis wants to see President&#39;s Trump&#39;s Tax Returns",

            
                "image" : [],
            

        "datePublished": "2/7/2019 11:24:51 PM",
        "dateModified": "2/8/2019 5:08:36 AM",
        "author": {
        "@type": "Person",
        "name": ""
        },
        "publisher": {
        "@type": "Organization",
        "name": "WXIA",
        "logo": {
