<script>
var flashImageDivId = "592275";
var commentContentId = '592275';
var commentImagesPath = 'https://d9-invdn-com.akamaized.net/comments_images/';

var socialCommentsApplicationsIds = {
	VK: 6015101
};


//Bind events
(function($, window, document, undefined) {

	var commentTotalComments = false;
	var commentPrependText = 'Commenti';
    var contentId = commentContentId;
	var FPcommentsObj = new FPcomments(contentId, commentTotalComments, commentImagesPath, commentPrependText);

	assignCommentEvents(document, contentId, FPcommentsObj);

}(jQuery, window, document));


$(document).ready(function() {
	var commentTextMax = 1000;
	var elapsedTimeFormatObj = new elapsedTimeFormat('{"minutes":{"X minuto fa":[1],"X minuti fa":["2-59"]},"hours":{"X ora fa":[1],"X ore fa":["2-24"]}}','Proprio Ora');
	setInterval( function(){ updateCommentsDates(elapsedTimeFormatObj); }, 30000 ); //30 seconds
	
    $('#commentBox_'+commentContentId).keyup(function() {
        calculateLimit('#commentBox_',commentContentId, commentTextMax);
	});
	
    avoidEmptyCommentSubmitError(commentContentId);

    setInterval(function () {
        if($("#pendingMessage"+commentContentId).is(':visible') && !$('.guidelinesBoxWrapper').is(':visible')){
            hideOverlay();
        }
    }, 400);
});


if ("" == "1"){
    $('.guidelinesBoxWrapper').addClass('firstPending');
    showOverlay();
    $('#comment-guidelines-container'+commentContentId).show();
    $('#agreementBox_'+commentContentId).removeClass( "displayNone" );
    $('#guidelinesButton'+commentContentId).unbind( "click" ).bind( "click", function(){
        if ($('#guidelinesCheckbox'+commentContentId).attr('checked') == 'checked'){

            $('#comment-guidelines-container'+commentContentId).hide();
            $('#agreementBox_'+commentContentId).addClass( "displayNone" );

            $.ajax({
                url: "/comments/Service/addFromTemp",
                type: "get",
                data: {
                    action					: "addFromTemp",
                    content					: commentContentId
                },
                success: function(status){
                    if(status == 'pending'){
						$('#pendingMessage'+commentContentId).removeClass('displayNone');
					} else {
						//TODO implement append just added comment
                        debugger;
						window.location = '#comments';
						window.location.reload();
					}

                    hideOverlay();
                    $('.guidelinesBoxWrapper, .guidelinesBoxWrapperAbove').css('margin-left','');
                }
            });
        }
    });
}

if ("" == "1"){
	$('#pendingMessage'+commentContentId).removeClass('displayNone');
}




//Top Search -------------------------------------------------------
   var searchChart_592275;
   searchChart_592275 = new FXautoComplete;
   searchChart_592275.Element.searchTextInputId = 'searchText_592275';
   searchChart_592275.Element.resultsInnerContainerId = 'searchTopResults_592275';
   searchChart_592275.Element.resultsOuterContainerId = 'subContainer592275';
   searchChart_592275.Element.formElementId = 'combineSearchForm_592275';
   searchChart_592275.Element.hasTabs = true;
   searchChart_592275.Element.tabsContainerId = 'search_tabs_592275';
   searchChart_592275.Element.tabsType = 'li';
   searchChart_592275.Element.portfolioPairId = 0;
   searchChart_592275.Element.searchType = 'portfolio';
   searchChart_592275.Element.tabsDivId = 'searchBoxTabsTopTabs_592275';
   searchChart_592275.Element.countriesType = 'td';
   searchChart_592275.Element.dynamicVar = '592275';
   searchChart_592275.Element.url = '/search/service/search?searchType=advinion';
   searchChart_592275.Element.mainResultRowIdPrefix = 'searchRowIdtop_';
   searchChart_592275.Element.tabsPrefix = 'search_box_top_'+ 592275;
   searchChart_592275.Element.countryRowPrefix = 'tdCountry592275_';
   searchChart_592275.Element.countriesWrapperId = 'countries_wrapper_592275';
   searchChart_592275.Element.countriesTableId = 'countries_table_592275';
   searchChart_592275.Element.countriesDefaultText  = 'Tutti gli Stati';
   searchChart_592275.Element.hiddenNoResultsDefine = 'noResultsDefinesearchTopResults_592275';
   searchChart_592275.Element.hiddenDocDirectionId = 'doc_directionsearchTopResults_592275';
   searchChart_592275.setTabsText('[{"id":"search_box_top_592275All","name":"Tutti","select":true},{"id":"search_box_top_592275Forex","name":"Valute","select":false},{"id":"search_box_top_592275Commodities","name":"Mat. prime","select":false},{"id":"search_box_top_592275Indices","name":"Indici","select":false},{"id":"search_box_top_592275Stocks","name":"Azioni","select":false},{"id":"search_box_top_592275ETFs","name":"ETF","select":false},{"id":"search_box_top_592275Bonds","name":"Obbl.","select":false},{"id":"search_box_top_592275Countries","name":"Tutti gli Stati","select":false}]');
   searchChart_592275.inputFieldBind( $('#searchText_592275') );

   searchChart_592275.getSymbol=function(selectedRowObj){
       return $('#' + selectedRowObj.attr('id') + ' .second').html();
   }

   $('#'+ searchChart_592275.Element.formElementId).submit(function() {
			if(searchChart_592275.Element.portfolioPairId != 0 && searchChart_592275.Element.portfolioPairId != undefined){
       			searchChart_592275.selectSymbol($('table[advinionSearch] tr[data-pair_id="' +searchChart_592275.Element.portfolioPairId+ '"]'));
       			$('#searchText_592275').val($('#searchText_592275').attr('default'));
			}
           return false;
   });

   searchChart_592275.selectSymbol=function(selectedRowObj){

		var pair_id = selectedRowObj.data('pair_id');
		var pair_type = selectedRowObj.data('pair_type');
		var futures_categories = selectedRowObj.data('futures_categories');
		var candles = selectedRowObj.data('candles');
		var decimal = selectedRowObj.data('decimal');
		var extraData, strSymbolName, strMarket;
		strSymbolName = strip(searchChart_592275.getSymbol(selectedRowObj));
		
		switch(pair_type)
		{
			case 'currency':
			  	if (futures_categories == 'FX'){
			  		extraData = 'indice';
			  		strMarket = futures_categories;
				}else{
					extraData = 'forex';
			  		strMarket = null;
			  		strSymbolName =  strip($('#' + selectedRowObj.attr('id') + ' .symbolName').html());
				}
			  	break;
			case 'Equities':
				extraData = 'cfds';
		  		strMarket = 2;
			  	break;
			default:
				extraData = pair_type;
	  			strMarket = futures_categories;
		}
		extraData += ',9';
		
		var objChart = investing_GetProChartObject('592275');
		objChart.SetExtraData(extraData);
		objChart.SetDecimalPlaces(decimal); 
		objChart.SetTimeScaleControls(candles, candles); 
		objChart.ChangeTimeFrame("5M");
		objChart.ChangeSymbolNoPush(strMarket, pair_id, strSymbolName);
		$('#chart_title_592275').html(strSymbolName);

		$('#chart_title_592275').data('extraData', extraData);
		$('#chart_title_592275').data('strMarket', strMarket);
		$('#chart_title_592275').data('decimal', decimal);
		$('#chart_title_592275').data('candles', candles);
		$('#chart_title_592275').data('pair_id', pair_id);
		$('#chart_title_592275').data('strSymbolName', strSymbolName);
		
		searchChart_592275.closeSearchResultBox();
   }
   
   searchChart_592275.bindResults=function(rowElement){
       var obj = this;
       $(rowElement)
           .mousemove(function(){
        	   obj.actionMouseMoveOnRow(rowElement,obj,searchChart_592275);
           })
           .click(function(){
               searchChart_592275.selectSymbol($(rowElement));
           });
   }

   searchChart_592275.buildSearchResults=function(data){
       var obj = this;
       var resultsHtml = '';
       var current_tab = obj.previousTab;
       obj.changeCurrentTabDisplay.call(obj,current_tab);
       obj.firstRowId = 0;
       var symbol = '', tr_data = '';
       var counter = 0;
       if(data['All'][0] !== 'La ricerca non ha prodotto risultati'){
         $.each(data['All'], function(index,item){

       	if(item !== undefined && (item['tab_ID'].toLowerCase() == current_tab.toLowerCase() || current_tab == 'All'))
           {
               symbol = searchChart_592275.boldSymbol(obj.searchText , item['symbol']);
               tr_data = ' data-pair_type="' +item['advinion']['pair_type']+ '" data-futures_categories="' +item['advinion']['futures_categories']+ '" data-candles="' +item['advinion']['str_candles']+ '" data-decimal="' +item['advinion']['pair_decimal_last']+ '" ';
               
               //check for exact match
               if((obj.searchText).toLowerCase() === (item['symbol']).toLowerCase() && counter == 0)
                   resultsHtml += '<table advinionSearch><tbody><tr ' +tr_data+ ' data-content-id="592275" data-pair_id="'+ item['pair_ID'] +'" class="row selectedRowExactMatch hoverSearch" id="'+obj.Element.mainResultRowIdPrefix+index+'">';
               else if(counter == 0)
                   resultsHtml += '<table advinionSearch><tbody><tr ' +tr_data+ ' data-content-id="592275" data-pair_id="'+ item['pair_ID'] +'" class="row hoverSearch" id="'+obj.Element.mainResultRowIdPrefix+index+'">';
               else
                   resultsHtml += '<tr ' +tr_data+ ' class="row "data-content-id="592275" data-pair_id="'+ item['pair_ID'] +'" id="'+obj.Element.mainResultRowIdPrefix+index+'">';

               counter++;
               resultsHtml += '<td class="first symbolName" pairid="'+item['pair_ID']+'" id="symbol_'+item['symbol']+'" link="'+item['link']+'">'+symbol+'</td><td class="second" class="dirLtr">'+item['name']+'</td>';
               resultsHtml += '<td class="third typeExchange arial_11 text_align_lang_base_2 dir_lang_base_1" pairid="'+item['pair_ID']+'" id="type_'+item['pair_ID']+'" link="'+item['link']+'">'+item['pair_type_label']+'';
               if(item['pair_type']=='Equities' || item['pair_type']=='indice' || item['pair_type']=='etf' || (item['exchange_name_short']))
               {
                   resultsHtml += ' - '+item['exchange_name_short'];
               }
               resultsHtml += '</td></tr>';
           }
         });
       }
       if(counter == 0){
       	resultsHtml = '<table><tbody><tr id="no_results"><td class="align_center noResults">La ricerca non ha prodotto risultati</td></tr></tbody></table>';
           obj.arrowfocus = "tabs";
           obj.firstRowId = 'no_results';
           $('#'+obj.Element.resultsInnerContainerId).html(resultsHtml);
           return;
       }
       resultsHtml += '</tbody></table>';
       $('#'+obj.Element.resultsInnerContainerId).html(resultsHtml);
   }

</script>
