				setCookie('adbBLk',9,1000,'/');
				ga('allSitesTracker.set', 'dimension21', '9_Blk_Yes');
				 
							}
    	}
		
		$.ajax({
		    url: "/jp.php?b="+b+"&k=YGw3YWZkMGk2aGxmMzIyYTEyZjo_amZlNWEwYDc1NTo0N2VlbmllZjBnamNjODliNWBjMjNnNTVhYG9nZmdgZGB_N35mdzA5NjJsYTNoMi4xNmZiP3pmMTV8MHM3cjVgNHFlLW47ZT8wOg==&d=1&i=1551205691&e=1199109546&lip=web18.forexpros.com&ti=1551205691",
