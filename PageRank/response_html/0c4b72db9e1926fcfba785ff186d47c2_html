        <label class="select-wrap"><select name="plan" class="amount" onchange="copyValue(this)"><option disabled>Choose an option</option><option value="5">5</option><option value="10" selected="selected">10</option><option value="15">15</option><option value="20">20</option><option value="25">25</option><option value="40">40</option><option value="50">50</option><option value="75">75</option><option value="100">100</option><option value="200">200</option><option value="250">250</option><option value="500">500</option><option value="666">666</option><option value="1000">1000</option><option value="1337">1337</option><option value="11412">11412</option></select></label><button class="donate-btn stripe_button_pay"><img src="https://assets.rbl.ms/18044033/2000x.jpg" alt="Donate with CC" /></button></form><form target="_blank" id="paypalTipForm" action="https://www.paypal.com/cgi-bin/webscr" method="post"><input type="hidden" name="business" value="rebecca@wonkette.com"><img src="https://assets.rbl.ms/18042881/2000x.png" alt="Donate with CC" id="paypal-button" class="paypal__button_pay" /><input type="hidden" name="cmd" value="_donations"><input type="hidden" name="item_name" value="Wonkette Donation"><input type="hidden" name="item_number" value="Tip Jar Donation"><input type="text" class="paypal-amount" name="amount" value="10."><input type="hidden" name="currency_code" value="USD"><img alt="" width="1" height="1" src="https://www.paypalobjects.com/en_US/i/scr/pixel.gif" style="display:inline;"></form></div><!-- End User Code --><!-- User Code --><script>
var event = new Event('load-more-event');
window.dispatchEvent(event);
window.addEventListener(event, showButtons());

function showButtons() {
    var existKeepReading = setInterval(function() {
        var keepReading = document.querySelectorAll('.js-keep-reading:not(.js-processed)');
        if (keepReading.length) {
            clearInterval(existKeepReading);
            Array.prototype.forEach.call(keepReading, function(el) {
                el.className += ' js-processed';
                var closestParent = getClosest(el,'.mb-2');
                closestParent.nextElementSibling.style.display='none';
                el.onclick = function(){
                   closestParent.nextElementSibling.style.display='block';
                };
            });
        }
    }, 500);
}
</script><!-- End User Code --></div>
 
