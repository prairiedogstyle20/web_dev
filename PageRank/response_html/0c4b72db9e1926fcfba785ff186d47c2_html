#disqus_thread {
    position: relative;
    max-width: 720px;
    margin: 0 auto;
}
.js--toggleDisqus {
background: #48A0FF;
}
</style><script type="text/javascript">
  window.disqus_config = function () {
      this.page.identifier = __BOOTSTRAP__.post.id,
      this.page.url = location.protocol + '//' + location.host + location.pathname;
