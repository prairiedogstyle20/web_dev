		<h2>Sign In</h2>		<script>
/* <![CDATA[ */
var dap_authenticate_url = "/dap/authenticate.php";

function validateW(form) {
	//alert("inside validate: " + form.name);
	
	for(i=0; i<form.elements.length; i++){
   		if( (form.elements[i].name == "email") || (form.elements[i].name == "password")) {
			//alert(form.elements[i].name + "=>" + form.elements[i].value);
			if(form.elements[i].value == "") {
				//alert(form.elements[i].name);
				alert("Please enter missing information...");
				form.elements[i].focus();
				return false;
			}
		}
   	}

	form.action = dap_authenticate_url;
    form.submit();
}


/* ]]> */
</script>
<form id="login-widget" name="loginFormW" method="post" action="" onSubmit="validateW(this.form); return false;">
      <div>Email:
        <input type="text" name="email" size="8" maxlength="60" />
      </div>
      <div>Password:
        <input type="password" name="password" size="8" maxlength="30" />
      </div>
      Remember Me: <input class="remember" name="rememberMe" type="checkbox" id="rememberMe" value="rememberMe" />
      <input class="form_submit" name="Login" type="button" value="Login" onClick="validateW(this.form); return false;" />

