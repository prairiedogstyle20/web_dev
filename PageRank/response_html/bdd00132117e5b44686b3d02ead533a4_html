                        <script type="text/javascript">                            //<![CDATA[
                            var ba = document.getElementById("ctl00_txtDate");
                            ba.readOnly = true;
                            
                            obj = "ctl00_txtDate";
                            Zapatec.Calendar.setup({ weekNumbers: false, electric: false, inputField: obj, button: "trigger1", ifFormat: "%d/%m/%Y", daFormat: "%d/%m/%Y", range: [2007, 2012], disableFunc: chkDate });
                            //]]>
                            function chkDate(date2check) {
                                var pastDate = new Date();
                                pastDate.setFullYear(2010);
                                pastDate.setMonth(4);
                                pastDate.setDate(4);

                                var futureDate = new Date();
                                futureDate = date2check;
                                futureDate.setDate(futureDate.getDate());
                                var today = new Date();

                                if (futureDate > today)
                                    return true;
                                else if (futureDate < pastDate)
                                    return true;
                                else
                                    return false;
                            }
