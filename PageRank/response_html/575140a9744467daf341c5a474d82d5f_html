            ga('set', {location: weburl, page: hash});
            ga('send', 'pageview', hash);
            self.COMSCORE && COMSCORE.beacon({c1: "2", c2: "6683813"});
            pageview_candidate_comescore();

           
           
           $('.vdswdgt-intro').html('<span>Updated: '+update+'</span>'+headline);
           if($('#lastdivcont').html()){
                var olddiv  = $('#lastdivcont').html(); 
                var newdata = $(this).closest('.data-list-cat').html();
                
                $(this).closest('.data-list-cat').html(olddiv);
            }
           $('#lastdivcont').html(newdata);
       }); 
    });
    //window scroll
    $(window).scroll(function() {
        //var isElementInView = Utils.isElementInView($('#alsowatchdiv'), true);
       // if (isElementInView && is_videos_playing===false) {
       
       $('#alsowatchdiv').on('inview', function(event, isInView) {
        if (isInView && is_videos_playing===false) {
    // element is now visible in the viewport
  
           console.log('in view');
           var mythis       = $('#firstvideo');
           var postid       = mythis.data('id');
           var headline     = mythis.data('title');
           var hls_stream_url= mythis.data('videohls');
           var videosrc      = mythis.data('videosrc');
           var postimage     = mythis.data('image');
           var postupdate     = mythis.data('update');
           var weburl       = mythis.data('weburl');
           //console.log('hls: '+hls_stream_url);
           is_videos_playing    =   true;
           get_hola_player(postid,weburl,'recommeded_player_'+postid,hls_stream_url,videosrc,postimage,thirdPartyJS);
           var hash =  weburl.replace('https://hindi.news18.com',''); 
            ga('set', {location: weburl, page: hash});
            ga('send', 'pageview', hash);
            self.COMSCORE && COMSCORE.beacon({c1: "2", c2: "6683813"});
            pageview_candidate_comescore();
            //console.log('PV fired!');
           var divdata ='<figure><a href="javascript:void(0)" data-videosrc="'+videosrc+'" data-videohls="'+hls_stream_url+'" data-title="'+headline+'" data-id="'+postid+'" data-image="'+postimage+'" data-update="'+postupdate+'" data-weburl="'+weburl+'"><img src="'+postimage+'"></a><span class="play"></span></figure><h6><a href="'+mythis.data('catslug')+'">'+mythis.data('cat')+'</a></h6><p><a href="javascript:void(0)" data-videosrc="'+videosrc+'" data-videohls="'+hls_stream_url.replace('http://','https://')+'" data-title="'+headline+'" data-id="'+postid+'" data-image="'+postimage+'" data-update="'+postupdate+'" data-weburl="'+weburl+'">'+headline+'</a></p>';
           
            $('#lastdivcont').html(divdata);
           
           } 
        });
       
    });
    
    
   // });
    
    
    
</script>
