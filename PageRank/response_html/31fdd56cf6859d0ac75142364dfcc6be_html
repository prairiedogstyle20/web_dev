<script>
function ShowMenudiv(menuid) {
if ($(menuid).find("#ArticleMenu").length > 0 && $(menuid).find(".post-list-wrapper").find(".post-item").length > 3) {
$(menuid).find("#ArticleMenu").show();
}
}
function HideMenudiv(menuid) {
if ($(menuid).find("#ArticleMenu")) {
$(menuid).find("#ArticleMenu").hide();
}
}
</script> <!-- End Secondary Nav -->
 
