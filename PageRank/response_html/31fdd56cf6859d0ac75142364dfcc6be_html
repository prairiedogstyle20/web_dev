// cricketinningopen = $(this).attr("data-inning");
//});
$('.tabsLstng').each(function(){
$(this).find('li:first-child').addClass('active');
});
for(var i=1;i<=2;i++)
{
$("#head"+i).removeClass("active");
$("#day"+i+"Cont").css("display","none");
}
$("#head"+2).addClass("active");
$("#day"+2+"Cont").css("display","block");
$('.contentTabs,.tabsContArea').each(function(){
$(this).find('.tabsCont').hide();
$(this).find('.tabsCont:first-child').show();
});
$('.tabsLstng li').click(function(){
$(this).siblings().removeClass('active');
$(this).addClass('active');
var activeTab = $(this).find('a').attr('href');
$(activeTab).fadeIn().siblings().hide();
return false;
});
});
</script>



