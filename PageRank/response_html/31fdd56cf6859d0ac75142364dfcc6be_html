function load_overcomparison(title,team1,team2,team1data,team2data,min,max,height_chart,team1wickets,team2wickets) {
Highcharts.chart('container_overcomparison', {
chart: {
height: height_chart,
type: 'bar'
},
title: {
text: ''
},
subtitle: {
text: title
},
credits: {
enabled: false
},
exporting: {
enabled: false
},
xAxis: {
reversed: true,
gridLineWidth: 0,
allowDecimals: false,
title: {
text: 'Overs'
},
scrollbar: {
enabled: false
},
crosshair: false
},
yAxis: {
min: min,
max: max,
tickInterval: 4,
labels: {
formatter: function () {
return Math.abs(this.value);
}
},
reversed: false,
opposite: true,
gridLineWidth: 0,
allowDecimals: false,
title: {
text: 'Runs Conceded'
}
},
plotOptions: {
series: {
stickyTracking: false,
stacking: 'normal',
pointWidth: 10
}
},
tooltip: {
backgroundColor: '#fff',
borderWidth: 1,
shadow: true,
borderRadius: 4,
borderColor: 'gray',
useHTML: true,
formatter: function () {
if (this.series.index <= 1) {
var html = "<b style=\"color:#2d4d87\">Over Details ( " + this.point.overno + " )</b></br>";
html += '<b>Runs Scored: </b><span style=\"color:green;font-weight:600\">' + (this.point.totalruns || '') + '</span>';
if (this.point.totalwickets>0) {
html +='</br><b>Total Wickets: </b><span style=\"color:red;font-weight:600\">' + (this.point.totalwickets || '')+'</span>';
}
return html;
} else {
return '<img src="https://drop.ndtv.com/common/images/wicketnew.png" title="" alt="" border="0" height="20" width="20">' + ' ' + '<b>' + (this.point.playername || '') + '</b>' + ' ' + (this.point.playerscore || '') + '<br>' + '( ' + (this.point.custom || '') + ' )';
}
}
},
series: [{
enableMouseTracking: true,
name: team1,
data: team1data
}
, {
enableMouseTracking: true,
name: team2,
data: team2data
},
{
showInLegend: false,
type: 'scatter',
name: ' ',
data: team1wickets,
marker: {
radius: 5,
enabled: true,
symbol: 'circle',
fillColor: '#45b4f0',
}
},
{
showInLegend: false,
type: 'scatter',
name: ' ',
data: team2wickets,
marker: {
radius: 5,
enabled: true,
symbol: 'circle',
fillColor: '#000',
}
}
]
});
}
function tabactivate(obj) {
var tab_id = obj.id;
$('#manhattan_ul li').removeClass('active');
$("#" + tab_id).addClass('active');
var id = parseInt(tab_id) + 1;
$('#manhattan_parent').children().hide();
$("#containers" + id).show();
}
function tabactivated(obj,count) {
var tab_id = obj.id;
$('#scatterscoringrate_ul li').removeClass('active');
$("#" + tab_id).addClass('active');
var id = parseInt(count) + 1;
$('#scatterscoringrate_parent').children().hide();
$("#scatterscoringrate_containers" + id).show();
}
function getEntityData() {
var d = new Date();
var matchurl = "";
if (lang == "") {
matchurl = 'https://sdata.ndtv.com/sportz/cricket/xml/' + match_gameid + '.json?t=' + d.getTime();
}
else {
matchurl = 'https://sdata.ndtv.com/sportz/cricket/json/' + lang + "/" + match_gameid + '_hi.json?t=' + d.getTime();
}
var newUrl = "/crossdomaindata?link=" + matchurl;
$.ajax({
url: newUrl,
cache: false,
async:false,
dataType: "json",
success: function (data) {
var playerids = '';
$.each(data.Teams, function (k, team) {
$.each(team.Players, function (key, player) {
playerids += key + ',';
})
})
SI_NS.getEntityMappings(playerids, 2, 1, function (dataEntity) {
entity_data=dataEntity;
})
},
error: function (data) {
return false;
}
});
}
function getPartnershipData(matchdata) {
var isTest = false;
var innings_trackerteam1 = 1;
var innings_trackerteam2 = 1;
var partnershipData_html = '';
var iningsNo_addon = "";
if (matchdata.Matchdetail.Match.Type == "Test" || matchdata.Matchdetail.Match.Type == "टेस्ट") {
isTest = true;
}
$.each(matchdata.Innings, function (k, item) {
var width = 50;
if (matchdata.Innings.length == 1) {
width = 100;
}
var current_score = item.Total + "/" + item.Wickets;
var d = new Date();
var status = "none";
if (k == 0) {
status = "block";
}
var _url = 'https://sdata.ndtv.com/sportz/cricket/xml/' + match_gameid + '_partnerships_' + (k + 1) + '.json?t=' + d.getTime();
var dataUrl = "/crossdomaindata?link=" + _url;
$.ajax({
url: dataUrl,
async: false,
cache: false,
dataType: "json",
}).done(function (partnershipdata) {
if (isTest) {
if (item.Battingteam == matchdata.Matchdetail.Team_Home) {
if (innings_trackerteam1 == 1) {
iningsNo_addon = "st";
}
else {
iningsNo_addon = "nd";
}
partnershipData_html += '<div class="table-wrap stat-container partnerships active tab-pane" style="width:' + width + '%;"><h3>' + matchdata.Teams[item.Battingteam].Name_Short + ' ' + innings_trackerteam1 + iningsNo_addon + ' Innings</h3><div>';
}
else {
if (innings_trackerteam2 == 1) {
iningsNo_addon = "st";
}
else {
iningsNo_addon = "nd";
}
partnershipData_html += '<div class="table-wrap stat-container partnerships active tab-pane" style="width:' + width + '%;"><h3>' + matchdata.Teams[item.Battingteam].Name_Short + ' ' + innings_trackerteam2 + iningsNo_addon + ' Innings</h3><div>';
}
}
else {
if (item.Battingteam == matchdata.Matchdetail.Team_Home) {
partnershipData_html += '<div class="table-wrap stat-container partnerships active tab-pane" style="width:' + width + '%;"><h3>' + matchdata.Teams[item.Battingteam].Name_Short + '</h3><div>';
}
else {
partnershipData_html += '<div class="table-wrap stat-container partnerships active tab-pane" style="width:' + width + '%;"><h3>' + matchdata.Teams[item.Battingteam].Name_Short + '</h3><div>';
}
}
$.each(partnershipdata.Partnerships, function (l, itm) {
partnershipData_html += '<div class="data-vis-box-container">\
<div class="stat-graph compareLineGraph compareCenter">\
<div class="bar-total">'+ itm.Runs + '</div>\
<div class="bar-col left">\
<div class="chartValue"><span class="name">'+ matchdata.Teams[matchdata.Innings[k].Battingteam].Players[parseInt(itm.Batsmen[0].Batsman)].Name_Full + '</span><span class="value">' + itm.Batsmen[0].Runs + ' (' + itm.Batsmen[0].Balls + ')</span></div>\
<span class="bar-percentage" style="max-width:68%;background:#145e7d;width:'+ itm.Batsmen[0].Runs + '%;"></span>\
</div>\
<div class="bar-col right">\
<span class="bar-percentage less" style="max-width:68%;background:#145e7d;width:'+ itm.Batsmen[1].Runs + '%;"></span>\
<div class="chartValue"><span class="name">'+ matchdata.Teams[matchdata.Innings[k].Battingteam].Players[parseInt(itm.Batsmen[1].Batsman)].Name_Full + '</span><span class="value">' + itm.Batsmen[1].Runs + ' (' + itm.Batsmen[1].Balls + ')</span></div>\
</div>\</div>\</div>';
})
partnershipData_html += '</div></div>';
});
if (item.Battingteam == matchdata.Matchdetail.Team_Home) {
innings_trackerteam1 = innings_trackerteam1 + 1;
}
else {
innings_trackerteam2 = innings_trackerteam2 + 1;
}
})
$("#partnership_box").show();
$("#partnership_box").html(partnershipData_html);
}
if (auto_refresh == 1) {
setInterval(loadcharts, 30000);
// setInterval(loadperformer_current, 30000);
}
</script>
 </div>
</div>
<div class="clr"></div>
</div>
</div>
</div>
</div>
