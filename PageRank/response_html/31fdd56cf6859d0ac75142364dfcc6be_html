<script>
var elem = document.querySelector('#h_search_menu');
var elem_child = document.querySelector('#searchdrop');
elem.addEventListener('click', function () {
if (elem_child.style.display == "") {
elem_child.style.display = "none";
} else {
elem_child.style.display = "";
}
})
function submit_search_form() {
var search_string = $('#s');
var search_string_value = $('#s').val();
var filtered_string = search_string_value.replace(/[^a-zA-Z0-9 ]/g, '').replace(/[^a-zA-Z0-9]/g, '-');
if (filtered_string == '') {
alert('Please enter search query');
search_string.focus();
return false;
} else {
window.location = '/search/q' + filtered_string;
}
}
function submit_topic_form() {
return submit_search_form();
}
</script>
