<script>
  new function(){
	var $modal = $("[data-modal-name='__static-movie-player-local']").eq(0);

	$(document).on('click', ".js-modal[data-target='__static-movie-player-local']", function(){
	  var href = $(this).attr("href");
	  $modal.find("iframe").attr("src",href + '&var=' + Math.floor(Math.random() * 1000000));
	  $modal.find("iframe").css("height","500");
	});

	$(document).on('click','.js-modal-wrap, .box__modal .btn__close', function(){
	  $modal.find("iframe").attr("src","");
	});
  };
