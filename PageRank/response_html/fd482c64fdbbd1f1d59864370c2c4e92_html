<script>
$(function(){
	var $trigger = $('.js__nav-breadCrumb');
	if($trigger.length) {
		if($('body').hasClass('__static')) {
			var $path = location.pathname.split('/'),
					$title = $('title').text().replace(/\s\|\s/, 'ï½').split('ï½').reverse(),
					$breadcrumb = '<div class="nav__breadcrumb"><div class="container"><ol itemscope="" itemtype="http://schema.org/BreadcrumbList">',
					href = '/',
					ajust = function(){
						if($path.length > $title.length) {
							$title.unshift(false);
							ajust();
						}
					};
			$path.pop();
			$path.shift();
			$title.shift();
			ajust();
			$breadcrumb += '<li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem"><a href="' + href + '" itemprop="item"><span itemprop="name">ãªã³ã©ã¤ã³TOP</span></a><meta itemprop="position" content="1"></li>';
			for (var $pathLengthIndex = 0; $pathLengthIndex < $path.length; $pathLengthIndex++) {
				href += $path[$pathLengthIndex] + '/';
				if($title[$pathLengthIndex]) {
					$breadcrumb += '<li itemprop="itemListElement" itemscope="" itemtype="http://schema.org/ListItem"><a href="'
					$breadcrumb += href;
					$breadcrumb += '" itemprop="item"><span itemprop="name">';
					$breadcrumb += $title[$pathLengthIndex];
					$breadcrumb += '</span></a><meta itemprop="position" content="'
					$breadcrumb += $pathLengthIndex + 2;
					$breadcrumb += '"></li>';
				};
			}
			$breadcrumb += '</ol></div></div>';
			$trigger.append($breadcrumb);
		}
	}
});
</script>


