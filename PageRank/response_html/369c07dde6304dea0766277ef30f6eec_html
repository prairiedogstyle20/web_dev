    /*<![CDATA[*/
    function initRegLinks (eleId) {
        var insitecookie=GetCookie("chron_user_auth");
        var hccookie=GetCookie("hc_customize");
        if (insitecookie && hccookie) {
            var name=insitecookie.replace(/\|.*/,"");
            var customarry=hccookie.split("|");
            var membername = customarry[10];
            var firstname = customarry[0];
            var lastname = customarry[1];
            var profileurl = "https://reg.timesunion.com/users/";
            document.getElementById(eleId).innerHTML='<a class="regSubscribe" href="/subscribe/" id="nav_subscribe">Subscribe</a><a href="/signin/" id="nav_login">Subscriber Services</a><a href="https://reg.timesunion.com/logout/" id="nav_logout">Sign Out</a>';
            var nonMemberArea = document.getElementById('reg_nonmember');
            if(nonMemberArea != null) {
            	nonMemberArea.style.display='none';
            }
        }
    }
    initRegLinks('reg_member');
    /*]]>*/
</script>

    </li>
</ul>
<!-- e hearst/home/social_links.tpl -->
                    </div>
                </div>
            </div>

