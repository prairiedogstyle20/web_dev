  <script src="http://www.baksoayam.com/assets/js/bootstrap.min.js"></script>
  <script src="http://www.baksoayam.com/assets/js/marquee.min.js"></script>
  <script src="http://www.baksoayam.com/assets/js/owl.carousel.min.js"></script>
  <script src="http://www.baksoayam.com/assets/js/main.js?v=13"></script>
  <script src="http://www.baksoayam.com/assets/js/accounting.min.js"></script>
  <script src="http://www.baksoayam.com/assets/js/bootstrap-progressbar.min.js"></script>
  <script src="http://www.baksoayam.com/assets/js/jquery.sticky.js"></script>
  <script>
    $(document).ready(function() {
      $('.progress .progress-bar').progressbar(
        {
          display_text: 'center',
          use_percentage: false,
          amount_format: function(p, max, min) {return p + ' / ' + max;}
        }
      );
      $('#progress-header .progressbar-back-text').css('display', 'none');
      $('#progress-header .progressbar-front-text').css('display', 'none');
          });
     
    
	// reload main balance
    $(function(){
      $('#refresh-balance').click(function() {
        $('#balance-holder').html('loading...');
        $('#main-balance-holder').html('loading...');
        $('.totalWallet').html('loading...');

        $.each($('.game-wallet'), function(i, val) {
          $(this).click();
        });

        setTimeout(
        function() 
        {
          $('#popover-balance').toggle();
          $.get('http://www.baksoayam.com/wallet/balance' , function(data) {
            var balance = accounting.formatNumber(data);
            $('#balance-holder').html(balance);
            $('.totalWallet').html(balance);
          }).fail(function() {
            $('#balance-holder').html('fail');
            $('.totalWallet').html('fail');
          });

          $.get('http://www.baksoayam.com/wallet/balance/main-wallet' , function(data) {
            var mainWallet = accounting.formatNumber(data);
            $('#main-balance-holder').html(mainWallet);
          }).fail(function() {
            $('#main-balance-holder').html('fail');
          }); 
          $('#popover-balance').data('bs.popover').options.content = $('#popover-balance-content').html();
        }, 6000);

        setTimeout(
        function() 
        {
          $('#popover-balance').data('bs.popover').options.content = $('#popover-balance-content').html();
          $('#popover-balance').toggle();
        },8000);
      });

            $('.span-balance').click(function() {
        $('#refresh-balance').click();
      });

      $('.login-modal, #captcha-login-popup, #re-captcha-login-popup').click(function() {
        var rand = Math.random();
        $('#captcha-login-popup').attr('src','/captcha/flat?'+rand);
        $('#captcha-login').attr('src','/captcha/flat?'+rand);
        $('#txtTargetCasino').val($(this).attr("data-target-casino"));
      });

      // Every time a modal is shown, if it has an autofocus element, focus on it.
      $('.modal').on('shown.bs.modal', function() {
        $(this).find('[autofocus]').focus();
      });
    });
  
        
    
    $(function(){
      $('.login-modal').on('click', function() {
        casinoNext = $(this).data('id');
      });
      $("#formLoginPopup").on("submit", function( event ) {
        event.preventDefault();
        var formSubmitLogin = $(this).serialize();
        $.ajaxSetup({
          headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
          }
        });

        $.ajax({
          type: "POST",
          url: "http://www.baksoayam.com/login",
          data: formSubmitLogin,
          success: function(data) {
              if (data.success){
                if (data.target_casino == "") {
                  window.location.reload();
                }else{
                  window.location = data.target_casino;
                };
              }else{
                $.each(data.message, function(index, element) {
                  $('#login-message-error').html(element);
                });
                $('#login-message-error').show();
                setTimeout(function(){ $('#login-message-error').hide(); }, 3000);
              }
          },
          error: function() {
              alert("error");
          }
        });

      });

      $("#formPopUpTransfer").on("submit", function( event ) {
        event.preventDefault();
        var formSubmitWalletCashIn = $(this).serialize();
        // $('#submitCashIn').val("Loading..");
        $('#submitCashIn').button('loading');
        $('#submitCashIn').attr('disabled', '');

        $.ajaxSetup({
          headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
          }
        });

        $.ajax({
          type: "POST",
          url: "http://www.baksoayam.com/wallet/cashin",
          data: formSubmitWalletCashIn,
          success: function(data) {
              if (data.success){
                  window.location.reload();
              }else{
                $.each(data.message, function(index, element) {
                  $('#popup-message-error').html(element);
                });
                $('#popup-message-error').show();
                setTimeout(function(){ $('#popup-message-error').hide(); }, 3000);
                $('#submitCashIn').button('reset');
              }
          },
          error: function() {
              alert("error");
          }
        });
      });
    });
    
  </script>


