                                    }
                                }
                                else if (classIndex >= 0)
                                {
                                    return classNameList.replace(className, "");
                                }
                                return classNameList;
                            }

                            window.dap = dapResult;
                        })();
                        //]]>

                            dap("&amp;AP=1089&amp;PG=FINUSEN13&amp;PVGUID=91519c52e434436a84b0e08178d48ffc&amp;PROVIDERID=7GZ465J", 300, 250, "rectangle2_article_markets_container_dc89851a-04f8-449e-96f7-3d0234df7e50");
                    </script>
            </div>
                    <a href="//go.microsoft.com/fwlink/?LinkID=286759" target="_blank" class="adchoices" data-piitxt="adchoices">
                        <span>AdChoices</span>
                    </a>
        </div>
    </div>


