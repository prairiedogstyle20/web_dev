if (isATIreporting) {


        RTE_AT_INTERNET.init();
        //Initializing ATInternet Tag Tracker

        var ati_config = {site: 592983},
          ati_context = {};


    //Getting the Device ID pasased as a query string
    var atiuserid = RTE_AT_INTERNET.getQueryString('atiuserid');

    if (atiuserid){

            ati_config.ClientSideUserId = {
                clientSideMode: 'required',
                userIdCookieDuration: 397,
                userIdExpirationMode: 'fixed'
            };

            ati_context = {
                userIdentifier: atiuserid
            };

    }

    var tag = new ATInternet.Tracker.Tag(ati_config,ati_context);





        tag.page.set({
            name: RTE_AT_INTERNET.atInternetObject.name, // (cleanUdmUrl())
            chapter1: RTE_AT_INTERNET.atInternetObject.chapter1,
            chapter2: RTE_AT_INTERNET.atInternetObject.chapter2,
            chapter3: '',
            level2: RTE_AT_INTERNET.atInternetObject.level2.capitalize(),
            customObject: RTE_AT_INTERNET.atInternetObject.customParams
        });

        //Sending Categories as Keywords
        var keywordsLength = RTE_AT_INTERNET.atInternetObject.keywords.length;
        if (keywordsLength){
            for (var i = 0; i < keywordsLength; i++) {
                tag.publisher.add({
                    impression: {
                        campaignId : '1',
                        creation : '['+RTE_AT_INTERNET.atInternetObject.keywords[i]+']',
                        url : '['+RTE_AT_INTERNET.atInternetObject.name+']'
                    }
                });
            }
        }


        tag.identifiedVisitor.set(
                {
                    id: RTE_AT_INTERNET.atInternetObject.rte_userid
                });

        tag.dispatch();

    //Clicks and Event Tracker (for rte.ie only)

    //Send event tags

        $( document ).ready(function() {
            var pageLinks = $("[data-ati-tracking]");
            if (pageLinks.length) {
                $.each(pageLinks, function () {

                        $(this).on("click", function (event) {
                           var  escapedTitle = $(this).data('atiTracking').url;
                                escapedTitle = escapedTitle.replace(/'/g, '');
                           var eventData={
                                elem: this,
                                click: {
                                campaignId: $(this).data('atiTracking').campaign,
                                generalPlacement: '[' + $(this).data('atiTracking').generalPlacement + ']',
                                creation: '[' + $(this).data('atiTracking').creation + ']',
                                variant: '[' + $(this).data('atiTracking').variant + ']',
                                format: '[' + $(this).data('atiTracking').format + ']',
                                url: '[' + escapedTitle + ']',
                                advertiserId: '[' + RTE_AT_INTERNET.atInternetObject.name + ']',
                              },
                            };

                            return tag.publisher.send(eventData);

                        });

                });
            }
        });


}


</script>

