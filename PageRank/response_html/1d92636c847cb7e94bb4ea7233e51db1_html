        ctrl_rep_ati = readCookie('ctrl_rep_ati');
if (ctrl_rep_ati === null){
  createCookie('ctrl_rep_ati', randomNumber, null,window.location.hostname);
  ctrl_rep_ati = readCookie('ctrl_rep_ati');
}

 if (ctrl_rep_ati==1){
    isATIreporting = true;
 }




if(window.location.host=="mobilebeta.rte.ie" || window.location.host=="beta.rte.ie" || window.location.host=='127.0.0.1:8000'){
    isATIreporting  =  true;
    console.log('ATInternet on Test Sites');
