            divBanner = document.getElementById("barronsOnMarketwatchBanner");
            if (divBanner) {
                btnClose = divBanner.querySelector('i.j-close');
                if (btnClose) {
                    btnClose.addEventListener('click', fnCloseClick);
                }
            }
        })();
