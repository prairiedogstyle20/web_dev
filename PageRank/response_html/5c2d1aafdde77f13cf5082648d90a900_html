        (function () {
            var divBanner, btnClose;

            function fnCloseClick(e) {
                e.preventDefault();
                document.cookie = 'hideBarronsOnMarketwatchBanner=true;domain=.marketwatch.com;path=/;';

                if (btnClose) {
                    btnClose.removeEventListener('click', fnCloseClick);
