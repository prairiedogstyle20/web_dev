					/*function shuffle(array) {
						var currentIndex = array.length
							, temporaryValue
							, randomIndex
							;
					
						// While there remain elements to shuffle...
						while (0 !== currentIndex) {
					
							// Pick a remaining element...
							randomIndex = Math.floor(Math.random() * currentIndex);
							currentIndex -= 1;
					
							// And swap it with the current element.
							temporaryValue = array[currentIndex];
							array[currentIndex] = array[randomIndex];
							array[randomIndex] = temporaryValue;
						}
					
						return array;
					}*/
          
          function updateHeaderTotalHours()
    			{
    				headerTotalHours += parseFloat($('div.totalHours span.hours').data('hourrate'));
    				var hourStr = Math.round(headerTotalHours).toString();
    				var commaRegex = /^(\d+)(\d{3})/;
    				while(commaRegex.test(hourStr))
    					hourStr = hourStr.replace(commaRegex,'$1' + ',' + '$2');
    				$("div.totalHours span.hours").text(hourStr);
    			}
          
          function updateHeaderViewCount()
    			{
    				headerTotalViews += (parseFloat($('div.totalViews span.views').data('viewrate')) * totalViewsUpdateInt);
    				var viewStr = Math.round(headerTotalViews).toString();
    				var commaRegex = /^(\d+)(\d{3})/;
    				while(commaRegex.test(viewStr))
    					viewStr = viewStr.replace(commaRegex,'$1' + ',' + '$2');
            var delay = Math.floor(Math.random() * ((totalViewsUpdateInt * 1000) / 2));
            setTimeout(function() {
              $("div.totalViews span.views").text(viewStr);
            }, delay);
    			}
				</script>
