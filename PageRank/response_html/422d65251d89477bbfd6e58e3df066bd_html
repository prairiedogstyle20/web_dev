  function footerBoomtrainSignup() {
    var email = document.footerSignupForm.Email.value;
    var newsletterBreaking = document.footerSignupForm.optInBreakingNews;
    var newsletterWeekly = document.footerSignupForm.optInWeekly;
    var newsletterHurt = document.footerSignupForm.optInHurt;
    var newsletterMorning = document.footerSignupForm.optInDaily;
    var newsletterPruden = document.footerSignupForm.optInPrudenNL;
    var _d = new Date();
    var now = _d.toISOString();

    _bt.signup(
        {email: email},
        {
          'breaking_news': newsletterBreaking.checked,
          'newsletter_sunday': newsletterWeekly.checked,
          'newsletter_hurt': newsletterHurt.checked,
          'newsletter_morning': newsletterMorning.checked,
          'newsletter_pruden': newsletterPruden.checked
        }
    );

    console.log('Boomtrain signup: Post save');

    window.setTimeout(function() {
      console.log('Boomtrain signup: Post save redirect');
      window.location.replace('/');
    }, 1000);

    return false;
  }

