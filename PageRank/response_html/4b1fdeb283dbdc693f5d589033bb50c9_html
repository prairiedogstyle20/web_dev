<script>
    var pathname        = window.location.pathname;
    var hostname        = window.location.hostname;

    var network         = 21686484267;
    var topLevelAdUnit  = 'LPCA';
    var adUnitPrefix    = 'LPCA_';
    var adUnits         = ["", "actualites", "international", "affaires", "sports", "auto", "arts", "debats", "cinema", "techno", "sciences", "environnement", "voyage", "vivre", "maison", "vins", "xtra", "suite", "detente", "meteo", "photos", "videos", "cpspecial", "rmon1", "noel"];
    var topLevelSection = '';
    var isHomepage      = false;
    var isArticle       = false;
    var adUnit          = 'LPCA';
    var section         = '';
    var sha1Encoder     = new SHA1Encoder();
    var userId          = getUserIdFromCookie();
    var userIdHash      = sha1Encoder.encode(userId);

    var articleId = extractArticleId(pathname);

    if (articleId) {
        pathname = cleanPathname(pathname);
    }

    var sectionCount = getSectionCount(pathname);

    if (sectionCount > 0) {
        if (isAdUnit(pathname.split('/')[1]) >= 0) {
            adUnit = adUnitPrefix + pathname.split('/')[1];
        }
        topLevelSection = pathname.split('/')[1];
        section = getSectionName(pathname, getSectionCount(pathname));

        if (topLevelSection == 'rmon1') {
            isHomepage = true;
            adUnit = adUnitPrefix + 'rmon1';
        }
    } else {
        if (pathname.length <= 1) {
            isHomepage = true;
            adUnit = adUnitPrefix + 'homepage';
        }
    }

    if (isAuto()) {
        adUnit = adUnitPrefix + 'auto';
        isHomepage = false;
    }

    function getNetwork() {
        return network;
    }

    function getTopLevelAdUnit() {
        return topLevelAdUnit;
    }

    function getTopLevelSection() {
        return topLevelSection;
    }

    function getAdUnit() {
        return adUnit;
    }

    function getArticleId() {
        return articleId;
    }

    function getSection() {
        return section;
    }

    function isAdUnit(section) {
        return adUnits.indexOf(section);
    }

    function getSectionCount(pathname) {
        var sections = pathname.split('/');

        return sections.length - 2;
    }

    function isAuto() {
        var patternHostname = 'auto';
        return hostname.match(patternHostname);
    }

    function extractArticleId(pathname) {
        var pattern = '\/[0-9]{2}-([0-9]{7})-';
        var matches = pathname.match(pattern);

        if (matches) {
            isArticle = true;
            return matches[1];
        }

        return false;
    }

    function cleanPathname(pathname) {
        var patternPathname = '(.*\/)[0-9]{6}';
        var cleanedPathname = pathname.match(patternPathname);

        if (cleanedPathname) {
            return cleanedPathname[1];
        }

        return pathname;
    }

    function getSectionName(pathname, index) {
        return pathname.split('/')[index];
    }

    if (isArticle) {
        typePage = 'article';
    } else if(!isArticle && adUnit != 'LPCA') {
        typePage = 'sectionfront';
    } else {
        typePage = 'page';
    }

    googletag.cmd.push(function () {
        googletag.pubads().setTargeting('TypePage', ['' + typePage + '']);
        googletag.pubads().setTargeting('SectionName', [getSection()]);
        googletag.pubads().setPublisherProvidedId(userIdHash);
        googletag.pubads().setTargeting('userId', userIdHash);

        if (isArticle) {
            googletag.pubads().setTargeting('ArticleID', [getArticleId()])
        }
        googletag.pubads().setCentering(true);
        googletag.pubads().collapseEmptyDivs();

        googletag.pubads().enableLazyLoad({
            fetchMarginPercent: 25,
            renderMarginPercent: 25,
            mobileScaling: 2.0
        });

        googletag.enableServices();
    });

    function getUserIdFromCookie() {
        var cookie = decodeURIComponent(document.cookie).split('; ');
        var cookieEntries = {};
        cookie.forEach(function(cookieEntry) { var splitEntry = cookieEntry.split('='); cookieEntries[splitEntry[0]] = splitEntry[1];});

        if (cookieEntries['LPUID']) {
            return cookieEntries['LPUID'];
        }
        return "";
    }

    var hasLeaderboard  = false;
    var hasWallpaper    = false;

    googletag.cmd.push(function () {
        googletag.pubads().addEventListener('slotRenderEnded', function(event) {
            var bodyTag = isHomepage ? $('body') : $j('body');
            if (event.slot.getSlotElementId() == 'dfp-outofpage-1' || event.slot.getSlotElementId() == 'dfp-leaderboard-1') {
                if (event.slot.getSlotElementId() == 'dfp-outofpage-1' && !event.isEmpty) {
                    hasWallpaper = true;
                }

                if (event.slot.getSlotElementId() == 'dfp-leaderboard-1' && !event.isEmpty) {
                    hasLeaderboard = true;
                }
            }

            if(!hasLeaderboard && !hasWallpaper ){bodyTag.addClass('lpNoPub')}
            else if(hasLeaderboard && !hasWallpaper ){bodyTag.addClass('lpLeaderboard').removeClass('lpNoPub')}
            else if(hasLeaderboard && hasWallpaper ){bodyTag.addClass('lpWallpaper').removeClass('lpNoPub').removeClass('lpLeaderboard')}
        });
    });

</script>
