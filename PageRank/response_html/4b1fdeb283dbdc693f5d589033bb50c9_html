    <script>
        googletag.cmd.push(function () {
            var slotName = "dfp-outofpage-1";

            googletag.defineOutOfPageSlot('/' + getNetwork() + '/' + getTopLevelAdUnit() + '/' + getAdUnit(), slotName)
                .setTargeting('position', 'pos1')
                .addService(googletag.pubads()
                );

            googletag.display(slotName);
        });
    </script>
