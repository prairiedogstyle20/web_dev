<script type="text/javascript">
	$j(".contenitore_nl").fadeIn();

	
	// popola cookie con sezione corrente
	// +
	// apre la gestione in overlay 
	$j("#apriEpopola").click(function(event) {
		var LStp_mcp_NSEL = getCookie("LStp_mcp_NSEL");
		var sezione_nl_id = "newsLST";

		
		if(LStp_mcp_NSEL.indexOf(sezione_nl_id)>=0){

		}else{
			LStp_mcp_NSEL = LStp_mcp_NSEL + "," + sezione_nl_id;
			createCookie("LStp_mcp_NSEL", LStp_mcp_NSEL, 1);
		}
		
		$j("#loading_modal").fadeIn(300);
		$j.ajax({
		  	type: "POST",
		  	url: "/modulo/registrazione/newsletter/jsp/gestione_newsletter.jsp",
		  	data: { 
		  		rdr: 1,
		  		logutente:'https://www.lastampa.it/2019/02/13/societa/il-presidente-trump-non-ho-tempo-per-un-cane-rompe-una-prassi-di-anni-alla-casa-bianca-cLYvmP5zKrVMadnWXHKUGK/pagina.html?lgut=1'
		  	}
		}).done(function( data ) {	
			$j("#loading_modal").fadeOut(300);
			
			$j(".conferma_reg_nl").fadeIn();	
			$j("#overlay_data").html(data);
		});
	});
	
	
	// nasconde il box newsletter
	// +
	// popola cookie con l'info 
	$j(".close_box_nl").click(function(e){
		e.preventDefault();
		$j(".contenitore_nl").fadeOut();
		createCookie("LStp_mcp_closeBox", 1, 1);
	});			
	
	
	// chiude overlay gestione newsletter
	$j(".close_conf").click(function(e){
		e.preventDefault();
		$j(".conferma_reg_nl").fadeOut();
		$j("#overlay_data").empty();
	});	
	


	//returns the value of a specified cookie
	function getCookie(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    for(var i=0; i < ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0)==' ') c = c.substring(1);
	        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
	    }
	    return "";
	} 
	
	//create cookie
	function createCookie(name,value,days) {
	    if (days) {
	        var date = new Date();
	        date.setTime(date.getTime()+(days*24*60*60*1000));
	        var expires = "; expires="+date.toGMTString();
	    }
	    else var expires = "";
	    document.cookie = name+"="+value+expires+"; path=/;domain=lastampa.it"; 
	}
</script>

