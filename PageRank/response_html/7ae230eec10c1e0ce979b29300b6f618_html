<script src="/modulo/gigya/js/viewportSensitive.js" type="text/javascript"></script>
<script>

	

	$j(document).ready(function(){
	
		if(!device.mobile()){

			var floatterSocialPromoHelper = $j(".floatterSocialPromoHelper");
			var floatterSocialPromoHelperClone = floatterSocialPromoHelper.clone();

			floatterSocialPromoHelperClone.empty();
			floatterSocialPromoHelperClone.attr("id", "");

			floatterSocialPromoHelperClone.appendTo($j(".bottom-floatter"));

			floatterSocialPromoHelper.addClass("top-helper");
			floatterSocialPromoHelperClone.addClass("bottom-helper");

			var barHelperBounding = "";
			var barHelperCloneBounding = "";
			var fixedBounding = "";
			
			var h = Math.max(document.documentElement.clientHeight, window.innerHeight || 0)

			window.addEventListener("scroll", function(){

				h = Math.max(document.documentElement.clientHeight, window.innerHeight || 0)

				if($j(".floatterSocialPromoHelper.top-helper").length > 0){
					barHelperBounding = $j(".floatterSocialPromoHelper.top-helper")[0].getBoundingClientRect();
				}
				
				if(typeof($j(".floatterSocialPromoHelper.bottom-helper")[0]) !== "undefined"){
					barHelperCloneBounding = $j(".floatterSocialPromoHelper.bottom-helper")[0].getBoundingClientRect();
				}
				
				if($j(".fixedTopHeader:not(.invisible-fixed-helper)").length > 0){
					fixedBounding = $j(".fixedTopHeader:not(.invisible-fixed-helper)")[0].getBoundingClientRect();
				}else{
					if($j(".invisible-fixed-helper").length > 0){
						fixedBounding = $j(".invisible-fixed-helper")[0].getBoundingClientRect();
					}else{
						fixedBounding = 0;
					}
				}
				
				if(fixedBounding != 0){
					if($j(".floatterSocialPromo").parent().hasClass("top-helper")){
						if(barHelperBounding.top < fixedBounding.bottom){
							//FIXED
							if(!$j(".floatterSocialPromo").hasClass("fixed")){
								$j(".floatterSocialPromo").addClass("fixed").addClass("shareBar-border");

								$j(".floatterSocialPromo").css({
									top: fixedBounding.bottom,
									left: fixedBounding.left
								});
							}
							
							$j(".floatterSocialPromo").width($j(".ls-colonna1").width());
						}

						if(barHelperBounding.top > fixedBounding.bottom){
							//NO FIXED
							if($j(".floatterSocialPromo").hasClass("fixed")){
								$j(".floatterSocialPromo").removeClass("fixed").removeClass("shareBar-border");
							}
							
							$j(".floatterSocialPromo").width("99%");
						}

						if(barHelperCloneBounding.top < h){
							//SPOSTA IN BASSO
							if($j(".floatterSocialPromoHelper.bottom-helper").find(".floatterSocialPromo").length == 0){
								$j(".floatterSocialPromo").removeClass("fixed").removeClass("shareBar-border");
								$j(".floatterSocialPromo").detach().appendTo($j(".floatterSocialPromoHelper.bottom-helper"));
							}
							
							$j(".floatterSocialPromo").width("99%");
						}
					}else if($j(".floatterSocialPromo").parent().hasClass("bottom-helper")){

						if(barHelperCloneBounding.top > h){
							//SPOSTA IN ALTO
							$j(".floatterSocialPromo").detach().appendTo($j(".floatterSocialPromoHelper.top-helper"));

							if(!$j(".floatterSocialPromo").hasClass("fixed")){
								$j(".floatterSocialPromo").addClass("fixed").addClass("shareBar-border");

								$j(".floatterSocialPromo").css({
									top: fixedBounding.bottom,
									left: fixedBounding.left
								});
							}
							
							$j(".floatterSocialPromo").width("100%");
						}
					}
				}
			});
		}
	});
</script><!-- dataPubblicazione : 2019/02/13 -->

