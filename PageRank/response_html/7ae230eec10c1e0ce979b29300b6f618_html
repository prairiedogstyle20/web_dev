						}
					}
				} else {
					$j("#containerArtCorr").css('top','0px');
				}
			} else {
				if (caso != 4) {
					caso = 4;

					$j('#afterTopBanner').css('display','none');
					$j('#afterUltimiArticoli').css('display','none');

