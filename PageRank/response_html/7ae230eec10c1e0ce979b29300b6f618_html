<script>
	function promozioneModal(message, close){
		$j(".ls-iniziativa-modal-girata-testo-modal", $j(".ls-iniziativa-modal-girata-modal")).html(message);
		$j(".ls-iniziativa-modal-girata-close-modal").on("click", $j(".ls-iniziativa-modal-girata-modal"), function(){
			hideModal();
		});
		
		$j(".ls-iniziativa-modal-girata-modal").removeClass("hidden");
		$j(".ls-iniziativa-modal-girata-modal-overlay").removeClass("hidden");
		
		$j(".ls-iniziativa-modal-girata-modal").removeClass("nonvisible");
		$j(".ls-iniziativa-modal-girata-modal-overlay").removeClass("nonvisible");
		
		if(typeof(close) !== "undefined"){
			if(!close){
				$j(".ls-iniziativa-modal-girata-close-modal").addClass("hidden");
			}
		}
	}
	
	function hideModal(){
		$j(".ls-iniziativa-modal-girata-modal").addClass("hidden");
		$j(".ls-iniziativa-modal-girata-modal-overlay").addClass("hidden");
		
		$j(".ls-iniziativa-modal-girata-modal").addClass("nonvisible");
		$j(".ls-iniziativa-modal-girata-modal-overlay").addClass("nonvisible");
		
		$j(".ls-iniziativa-modal-girata-close-modal").removeClass("hidden");
	}
</script><script>
