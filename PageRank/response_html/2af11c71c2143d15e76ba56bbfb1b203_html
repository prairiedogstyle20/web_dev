            key: "fb_adid",
            val: function(bidResponse) {
              return bidResponse.fbBidId;
            }
          }, {
            key: "hb_bidder",
            val: function(bidResponse) {
              return bidResponse.bidderCode;
            }
          }, {
            key: "hb_pb",
            val: function(bidResponse) {
              return bidResponse.pbMg;
            }
          }]
        }
      };
      pbjs.addAdUnits(adUnits);
      pbjs.setConfig({
        priceGranularity: customConfigObject
      });
      pbjs.requestBids({
        bidsBackHandler: initAdserver,
        timeout: PREBID_TIMEOUT
      });
    });

    function initAdserver() {
      if (pbjs.initAdserverSet) return;
      pbjs.initAdserverSet = true;
      googletag.cmd.push(function() {
        pbjs.que.push(function() {
          pbjs.setTargetingForGPTAsync();
          googletag.pubads().refresh();
        });
      });
    }

    // in case PBJS doesn't load
    setTimeout(function() {
      initAdserver();
    }, FAILSAFE_TIMEOUT);

    googletag.cmd.push(function() {

      var sizeMappings = {
        header: googletag.sizeMapping()
          .addSize([864, 0], [970, 250])
          .build(),
        image: googletag.sizeMapping()
          .addSize([864, 0], [728, 90])
          .build(),
        body: googletag.sizeMapping()
          .addSize([864, 0], [728, 90])
          .build(),
        sidebar: googletag.sizeMapping()
          .addSize([1180, 0], [300, 600])
          .build(),
      }

      googletag.defineSlot(
        '/61914415/DW_Header', [300, 300],
        'div-gpt-ad-DW_Header')
        .defineSizeMapping(sizeMappings.header)
        .addService(googletag.pubads());

      
      googletag.defineSlot(
        '/61914415/DW_Image', [],
        'div-gpt-ad-DW_Image')
        .defineSizeMapping(sizeMappings.image)
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Body1', [300, 250],
        'div-gpt-ad-DW_Body1')
        .defineSizeMapping(sizeMappings.body)
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Body2', [300, 250],
        'div-gpt-ad-DW_Body2')
        .defineSizeMapping(sizeMappings.body)
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Body3', [300, 250],
        'div-gpt-ad-DW_Body3')
        .defineSizeMapping(sizeMappings.body)
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Sidebar1', [],
        'div-gpt-ad-DW_Sidebar1')
        .defineSizeMapping(sizeMappings.sidebar)
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Sidebar2', [],
        'div-gpt-ad-DW_Sidebar2')
        .defineSizeMapping(sizeMappings.sidebar)
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Footer', [480, 360],
        'div-gpt-ad-DW_Footer')
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Pixel1', [1, 1],
        'div-gpt-ad-DW_Pixel1')
        .addService(googletag.pubads());

      googletag.defineSlot(
        '/61914415/DW_Pixel2', [1, 1],
        'div-gpt-ad-DW_Pixel2')
        .addService(googletag.pubads());

      
      googletag.pubads().enableSingleRequest();
      googletag.enableServices();
    });
  </script>

