<script id="hotwire-template" type="text/x-handlebars-template">
  <section class="article-hot-wires mb-4-ns">
    <h3 class="block-title">Hot Wire</h3>
    {{#each articles}}
      <article class="article-hot-wire fx pos-r pb-3 pb-2-l">
        <div class="media as-fs beaufort-heavy f-min pos-r fx-s-0 mr-2 mr-3-m">
          <a href="{{ url }}">
            <img src="{{ metadata.image_style_urls.169medium }}" alt="{{ title }}" />
          </a>
        </div>
        <div class="as-c">
          <h3 class="f-5 f-sm-l mb-1"><a href="{{ url }}">{{ title }}</a></h3>
          {{#each authors}}
            <address class="author">
              <span class="label">By</span> {{ this }}
            </address>
          {{/each}}
        </div>
      </article>
    {{/each}}
  </section>
</script>
<script>
  (function($, Drupal) {
    "use strict";

    var source   = $("script#hotwire-template").html();
    var template = Handlebars.compile(source);

    var cleanUpArticles = function(articles){
      var cleaned = [];
      for (var i in articles){
        articles[i].metadata = JSON.parse(articles[i].metadata);
        cleaned.push(articles[i]);
      }
      return cleaned;
    };

    $.ajax({
      url: 'https://api.parsely.com/v2/analytics/posts',
      jsonp: 'callback',
      dataType: 'jsonp',
      data: {
        'apikey': 'dailywire.com',
        'pub_date_start': '1w', // 90 days
        'limit': 5,
        'page': 1,
        'section': 'News',
        'sort': 'views',
        'period_start': '30m'
      },
      success: function(response) {
        var articles = cleanUpArticles(response.data);
        console.log(articles);
        $('.hotwire').append(template({ articles: articles }));
      }
    });

  })(jQuery, Drupal);
</script>
