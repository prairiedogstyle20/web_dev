<script>
  (function($) {
    "use strict";

    function mkFormatter(num) {
      if (num > 999999) {
        return parseFloat((num/1000000).toFixed(1)) + 'm'
      } else if (num > 999) {
        return parseFloat((num/1000).toFixed(1)) + 'k'
      } else {
        return num;
      }
    }

    var getArticleShares = function() {
      $.ajax({
        url: 'https://api.parsely.com/v2/shares/post/detail',
        jsonp: 'callback',
        dataType: 'jsonp',
        data: {
          'apikey': 'dailywire.com',
          'url': location.href,
        },
        success: function(response) {
          var shares = response.data[0];
          var fb = !shares || shares.fb < 10 ? 0 : mkFormatter(shares.fb);
          var tw = !shares || shares.tw < 10 ? 0 : mkFormatter(shares.tw);
          $('.share .share-facebook a').addClass(!fb ? 'in no-stats' : 'in stats').find('small').text(fb);
          $('.share .share-twitter a').addClass(!tw ? 'in no-stats' : 'in stats').find('small').text(tw);
        }
      });
    };

    var getArticleViews = function() {
      $.ajax({
        url: 'https://api.parsely.com/v2/analytics/post/detail',
        jsonp: 'callback',
        dataType: 'jsonp',
        data: {
          'apikey': 'dailywire.com',
          'url': location.href,
          'period_start': '90d',
        },
        success: function(response) {
          var postDetail = response.data[0];
          if (postDetail && postDetail.metrics.views) {
            var views = mkFormatter(postDetail.metrics.views); // test/tmp
            $('.view-count').removeClass('hide').find('span').text(views);
          }
        }
      });
    };

    $(window).on('parsely_ready', function(){
      getArticleShares();
      if ($('.article-full .view-count').length) {
        getArticleViews();
      }
    });

  })(jQuery);
</script>
