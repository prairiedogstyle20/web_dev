			$(document).ready(function(){
				var re = /\?id=(\d+)/g;
				$('iframe[src*="admin.mangomolo.com"]').each(function(){
					var player = new playerjs.Player(this);
					match = re.exec($(this).attr('src'));
					if (match != null) {
						player.videoId = match[1];
					}
					player.on('ready', function(){
						player.on('play', function(){
							ga('send', 'event', 'Videos', 'play', this.videoId);
							ga('dmi.send', 'event', 'Videos', 'play', this.videoId);
						});
						player.on('pause', function(){
							ga('send', 'event', 'Videos', 'pause', this.videoId);
							ga('dmi.send', 'event', 'Videos', 'pause', this.videoId);
						});
						player.on('ended', function(){
							ga('send', 'event', 'Videos', 'ended', this.videoId);
							ga('dmi.send', 'event', 'Videos', 'ended', this.videoId);
						});
					});
				});
			});
		</script>

