function UpdateOffsetTop(){ 
        Slot_sm_pc_foot_728x90_topOffset =  $("#div-gpt-ad-1352514192773-0_box").offset().top;
        Slot_sm_pc_foot_728x90_adAlmostVisibleScrollValue = Slot_sm_pc_foot_728x90_topOffset - $(window).height();
        
        Slot_sm_pc_foot_left_336x280_topOffset =  $("#div-gpt-ad-1352514192773-2_box").offset().top;
        Slot_sm_pc_foot_left_336x280_adAlmostVisibleScrollValue = Slot_sm_pc_foot_left_336x280_topOffset - $(window).height();
        
        Slot_sm_pc_foot_right_336x280_topOffset =  $("#div-gpt-ad-1352514192773-4_box").offset().top;
        Slot_sm_pc_foot_right_336x280_adAlmostVisibleScrollValue = Slot_sm_pc_foot_right_336x280_topOffset - $(window).height();
        
}

      
          Slot_sm_pc_foot_728x90_refreshed = false;
          Slot_sm_pc_foot_728x90_topOffset =  $("#div-gpt-ad-1352514192773-0_box").offset().top;
          Slot_sm_pc_foot_728x90_listener = function() {
            if (window.scrollY >= (Slot_sm_pc_foot_728x90_topOffset - $(window).height() - 10) && !Slot_sm_pc_foot_728x90_refreshed) {
              googletag.cmd.push(function() {
              googletag.pubads().refresh([Slot_sm_pc_foot_728x90]);
              });
              Slot_sm_pc_foot_728x90_refreshed = true;window.removeEventListener('scroll', Slot_sm_pc_foot_728x90_listener);
  						UpdateOffsetTop();
            }

          }
          window.addEventListener('scroll', Slot_sm_pc_foot_728x90_listener);

          Slot_sm_pc_foot_left_336x280_refreshed = false;
          Slot_sm_pc_foot_left_336x280_topOffset =  $("#div-gpt-ad-1352514192773-2_box").offset().top;
          Slot_sm_pc_foot_left_336x280_listener = function() {
            if (window.scrollY >= (Slot_sm_pc_foot_left_336x280_topOffset - $(window).height() - 10) && !Slot_sm_pc_foot_left_336x280_refreshed) {
              googletag.cmd.push(function() {
              googletag.pubads().refresh([Slot_sm_pc_foot_left_336x280]);
              });
              Slot_sm_pc_foot_left_336x280_refreshed = true;window.removeEventListener('scroll', Slot_sm_pc_foot_left_336x280_listener);
  						UpdateOffsetTop();
            }

          }
          window.addEventListener('scroll', Slot_sm_pc_foot_left_336x280_listener);

          Slot_sm_pc_foot_right_336x280_refreshed = false;
          Slot_sm_pc_foot_right_336x280_topOffset =  $("#div-gpt-ad-1352514192773-4_box").offset().top;
          Slot_sm_pc_foot_right_336x280_listener = function() {
            if (window.scrollY >= (Slot_sm_pc_foot_right_336x280_topOffset - $(window).height() - 10) && !Slot_sm_pc_foot_right_336x280_refreshed) {
              googletag.cmd.push(function() {
              googletag.pubads().refresh([Slot_sm_pc_foot_right_336x280]);
              });
              Slot_sm_pc_foot_right_336x280_refreshed = true;window.removeEventListener('scroll', Slot_sm_pc_foot_right_336x280_listener);
  						UpdateOffsetTop();
            }

          }
          window.addEventListener('scroll', Slot_sm_pc_foot_right_336x280_listener);

window.onload = function() {
  UpdateOffsetTop();
}

