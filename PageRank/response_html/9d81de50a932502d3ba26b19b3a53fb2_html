<script type="text/javascript">

$('#favoriteModal').on('click', '.favaction_mytag', function(){
    tag = $(this).text();
    tag_id = $(this).attr('id');  
    SetFavoriteTag(tag,tag_id);
});

$(function () {	
	tm = [];
  $('#favoriteModal').on('keyup keydown','#form-favorite-tag',function(){
		tm.push(1);
		setTimeout(function(){

      tm.pop();
      if(tm.length == 0){
        CheckFavoriteMytags();
      }

		},1000);
  });
});

function FavoriteDelete(shindan_id){
  $("#alert_box").hide();
  bootbox.confirm("お気に入りから削除してよろしいですか？", function(result) {
    if (result) {
      FavoriteAction(3,shindan_id);
    }
  });
}

function FavoriteAction(mode, shindan_id){

  switch (mode){
  case 1:
    m = "check";
    break;
  case 2:
    m = "save";
    break;
  case 3:
    m = "delete";
    break;
  }
  
  
  if(m == "check"){
    $("#alert_box").hide();
    $('#favorite_modal-body').empty().html('<div class="loading"><div style="text-align: center; margin-bottom: 20px; color: #CCC;"><i class="fas fa-spinner fa-spin fa-2x"></i></div></div>');
    $('#favoriteModal').modal('show');
  }
  
  
  if(m == "save" || m == "delete"){
    $('#favoriteModal').modal('hide');
  }
  
  var tag = $("#form-favorite-tag").val();
  var private = $("[name=form-favorite-private]").prop("checked");
	$.ajax({
    type: 'POST',
    url: '/c/shindan_favorite',
    dataType: 'json',
       data: {
       shindan_id: shindan_id,
       m: m,
       tag: tag,
       private: private,
    },
    success: function(data) {
      if(m == "check"){
        $('#favorite_modal-title').html(data.title);
        $('#favorite_modal-body').html(data.text);
        $('#favorite_modal-footer').html(data.footer);
        mytags = data.mytags;
      }
      if(m == "save" || m == "delete" ){
        $('.favactionbutton').html(data.favactionbutton);
        $('#favtest').html(data.text);
        $('#favlabel_container_'+shindan_id).html(data.favlabel);
        
        var html = '<div class="alert alert-'+data.msg_alert+' alert_text">' + data.msg+ '<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button></div>';

                $("#alert_box").html(html).hide().fadeIn();
        setTimeout(function() {
            $('#alert_box').fadeOut(5000);
        }, 4000);
        
      }

    },
	});
}

function SetFavoriteTag(tag,tag_id){
  form_tag_val = $("#form-favorite-tag").val().replace(/　/g, " ");
  form_tag = form_tag_val.split(" ");
  tag_flg = jQuery.inArray(tag,form_tag);
  
  if(tag_flg >= 0) {
    var form_tag = form_tag.filter(function(a) {
      return a !== tag;
    });

  } else {
    form_tag.push(tag);
  }

  form_tag = form_tag.join(" ");
  $("#form-favorite-tag").val(form_tag+" ");
  $("#" + tag_id).toggleClass("favaction_mytag_selected");
}

function CheckFavoriteMytags(){
  form_tag_val = $("#form-favorite-tag").val().replace(/　/g, " ");
  form_tag = form_tag_val.split(" ");
  console.log(form_tag);
  var mytag_node = $(".favaction_mytag_selected");
  mytag_node.each(function(){
      $(this).removeClass("favaction_mytag_selected");
  });
  $.each(form_tag, function(i, val) {
    mytag_place = jQuery.inArray(val,mytags);
    if( mytag_place >= 0 ){
      $("#mytag_"+mytag_place).addClass("favaction_mytag_selected");
    }

  });
}
