function SendEventGa(eventCategory,eventAction,eventLabel){
  
  if(eventAction === undefined){
    eventAction = "";
  }

  if(eventLabel === undefined){
    eventLabel = "";
  }

  if(eventCategory !== undefined) {
    ga('send', 'event', eventCategory, eventAction, eventLabel);
  }
}


