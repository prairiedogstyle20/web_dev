                        <script>
                            jQuery(document).ready(function($){
                                $('#followed-notification-modal-0aaf1134-3728-11e6-9620-2f8eb2a4525e-form').submit(function(e) {
                                    e.preventDefault();
                                    var $followBtn = $(this).find('.follow-btn');
                                    var sAction = $(this).attr('action') + '&title=' + $(this).find('.input-notification-name').val();
                                    $followBtn.find('.tnt-svg').show();
                                    $followBtn.prop('disabled', true);

                                    function submit_finished($alert){
                                        setTimeout(function(){
                                            $followBtn.find('.tnt-svg').hide();
                                            $alert.show().delay(2500).fadeOut(400);
                                        },2000);
                                    }

                                    $.ajax({
                                        type: 'POST',
                                        url: sAction,
                                        data: $(this).serialize(),
                                        success: function(oData){
                                            
                                                // Track follow event
                                                __tnt.trackEvent({
                                                    'network': 'Site',
                                                    'socialAction': 'Follow',
                                                    'url': 'https://www.theadvocate.com/users/profile/0aaf1134-3728-11e6-9620-2f8eb2a4525e'
                                                });
                                            
                                            submit_finished( $('#followed-notification-modal-0aaf1134-3728-11e6-9620-2f8eb2a4525e-user .alert-success') );
                                        },
                                        error: function(oData){
                                            submit_finished( $('#followed-notification-modal-0aaf1134-3728-11e6-9620-2f8eb2a4525e-user .alert-danger') );
                                        }
                                    });
                                });
                            });
