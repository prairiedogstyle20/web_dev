    <script>
      document.addEventListener('DOMContentLoaded', remove1159);
      function remove1159() {
        const remove1159Ver = '0.2.1';
        const dateNodes = document.querySelectorAll('time');
        dateNodes.forEach((node) => {
          if (node.innerText === '11:59 pm') {
            if (node.previousSibling) {
              const prevSibling = node.previousSibling;
              if (prevSibling.nodeValue.indexOf('–') !== -1) {
                prevSibling.remove();
              }
            }
            node.remove();
          }
        });
      }
    </script>



            
