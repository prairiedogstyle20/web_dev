<script type="text/javascript">
	(function ($) {

		$(document).ready(function () {
			$('.stansberry-form form input[type="submit"]').click(function (event) {
				event.preventDefault();
				event.stopPropagation();

				const jwtToken = "eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.eyJpc3MiOiJodHRwczpcL1wvd3d3LmluZm93YXJzLmNvbSIsImlhdCI6MTU1MDg4NjY1MSwibmJmIjoxNTUwODg2NjUxLCJleHAiOjE1NTE0OTE0NTEsImRhdGEiOnsidXNlciI6eyJpZCI6IjE4NzQ0OSJ9fX0.EY3VthCKanhjpq2Wz1kgEajSA5N8eG_n-0PxJyKiZHU";

				var $form = $('.stansberry-form form');
				var fname = $form.find('input[name="FNAME"]').val();
				var lname = $form.find('input[name="LNAME"]').val();
				var email = $form.find('input[name="EMAIL"]').val();

				var your_name = fname;

				if (lname.length > 0) {
					your_name += ' ' + lname;
				}

				fetch('/wp-json/ifw/v1/newsletter', { 
					method: 'POST',
					headers: {
						'Accept': 'application/json',
						'Content-Type': 'application/json',
						'Authorization': `Bearer ${jwtToken}`
					},
					body: JSON.stringify({
						'your-name': your_name,
						'your-email-signup': email
					})
				})
				.then(response => response.json())
				.then(data => {
					if (data.success) {
						var $resp = $('.wpcf7-response');
						$resp.html('Thanks for signing up for the Infowars Newsletter! Your signup request has been sent. Please check your email to confirm.');
						$resp.animate({opacity: 1}, 350);
					}
				})
				.catch(e => console.error(e));
			});
		});

	})(jQuery);
</script>
