    <section class="full-article">
      <article style="margin-bottom: 0;">
        
        
        
        
        
        
        
        
        
          <div class="ndn_embed" id="Inform-Washington-Times-Player-1" data-config-playlist-id="18050" data-config-distributor-id="91568" data-config-width="100%" data-config-height="9/16w">
            <div class="main-photo article-story-photo">
              <section class="block more-from-story" style="padding-bottom: 10px; margin-bottom: 0; border-bottom: 1px solid #ccc;">
                <div class="block-content">
                  <figure class="photo-zoom">
                    <div class="photo"><a href="/multimedia/image/1_9_2019_b1-crow-trump-jobs-8201jpg/" alt="Trump Jobs Illustration by Greg Groesch/The Washington Times" title="Trump Jobs Illustration by Greg Groesch/The Washington Times" target="_blank"><img src="https://twt-thumbs.washtimes.com/media/image/2019/01/09/1_9_2019_b1-crow-trump-jobs-8201_c0-0-2200-1282_s885x516.jpg?6267bf3b7f2085cc42a67f38dcbeb8acbe2b6668" width="885" height="516" alt="Trump Jobs Illustration by Greg Groesch/The Washington Times" /><i class="zoom"></i></a></div>
                    
                    <figcaption class="source">Trump Jobs Illustration by Greg Groesch/The Washington Times <a href="/multimedia/image/1_9_2019_b1-crow-trump-jobs-8201jpg/" target="_blank">more &gt;</a></figcaption>
                    
                  </figure>
                </div>
              </section>
            </div>
          </div>
        

