<script type="text/javascript" title="Validar formulario landing">
			$(document).ready(function(){
				// Llamamos al validador del formulario
				$("#form_473").validate({
					onblur: false,
					onkeyup: false,
				//	rules: {
				//		telefono: { phoneES : '' }
				//	},
					errorPlacement: function(error, element) {
						error.appendTo(element.parent());
					},
					submitHandler: function(form) {
						form.submit();
					}
				});

				
				// Cargamos las reglas propias de la landing si existieran
				if ($.isFunction(window.addValidateRules)) {
					addValidateRules();
				}

				// Evento CLICK del boton para enviar el formulario
				$("#form_473 input[name='ENVIAR']").click(function(){
					$("#form_473").submit();
				});
			});
		</script>
<script type="text/javascript">
                $(document).ready(function () {
                    //open box by clicking email field
                    $('#newsletterEmail').click(function () {
                        $('#next_step').slideDown('slow');
                    });
                    //stop closing box by clicking inside module
                    $('#newsleterNewUser').click(function (e) {
                        e.stopPropagation();
                    });
                    //close box by clicking outside module
                    $(document).click(function() {
                        $('#next_step').slideUp('slow');
                    });

                    var validator = $("#frmNewsletter").validate({
                        showErrors: function (errorMap, errorList) {
                            this.defaultShowErrors();
                            if ($(this.currentForm).find('label.error').css('display') === 'inline') {
                                $(this.currentForm).find('label.error').css('display', 'block');
                            }
                        }
                    });

                    $("#btnNewsletter").click(function (e) {
                        e.preventDefault();
                        if (validator.form()) {
                            $("#frmNewsletter").submit();
                        }
                    });

                    $("#frmNewsletter").submit(function (e) {
                        e.preventDefault(); //STOP default action

                        var postData = $(this).serialize();
                        var formURL = "https://www.bolsamania.com/?section=ajax&module=newsletter/bolsamania/v1/newUserNewsletter&divFlag=false&";

                        $.ajax({
                            url: formURL,
                            type: "POST",
                            data: postData,
                            success: function (data, textStatus, jqXHR) {
                                $("#newsleterNewUser").parent().empty().append(data);
                            },
                            error: function (jqXHR, textStatus, errorThrown) {
                                $("#newsleterNewUser").parent().empty().html("<label class='error'>"+errorThrown+"</label>");
                            }
                        });
                        //e.unbind(); //unbind. to stop multiple form submit.
                    });

                });
            </script>
