    <script>
        $(document).ready(function() {
            // De la cookie sacamos el user hash ID
            var __cookieValue = $.cookie('us_media_users_bolsamania_cn_uid') ,
                __url = '/user/' + __cookieValue + '/data',
                __content =
                    '<li class="user-out">' +
                    '<a href="/login.html" class="fast-login">' +
                    '<svg width="19" height="19" viewBox="0 0 24 24"><path d="M2.984 21h13c.553 0 1-.447 1-1 0-2.99-1.763-5.569-4.3-6.773.925-.861 1.514-2.079 1.514-3.44 0-2.6-2.114-4.715-4.714-4.715S4.771 7.187 4.771 9.786c0 1.361.589 2.579 1.514 3.44-2.537 1.204-4.3 3.783-4.3 6.773C1.984 20.553 2.432 21 2.984 21zM6.771 9.786c0-1.497 1.218-2.715 2.714-2.715s2.714 1.218 2.714 2.715c0 1.496-1.218 2.714-2.714 2.714S6.771 11.282 6.771 9.786zM9.484 14.5c2.691 0 4.938 1.943 5.409 4.5H4.075C4.547 16.443 6.793 14.5 9.484 14.5zM15.5 12.497c-.001.267.104.521.293.71C17.966 15.38 19 17.101 19 21h2c0-3.74-.817-6.008-3.179-8.568.5-.805.768-1.803.754-2.856-.022-1.63-.712-3.199-1.892-4.305l-1.367 1.459c.785.736 1.244 1.783 1.259 2.873.012.888-.271 1.686-.778 2.187C15.607 11.977 15.501 12.231 15.5 12.497z"/></svg>' +
                    '</a>' +
                    '</li>';

            if (typeof __cookieValue !== 'undefined') {

                $.ajax(__url, {
                    success: function (response) {
                        if (typeof response !== 'object') {
                            response = JSON.parse(response);
                        }

                        //si tenemos datos
                        if (typeof response.data_crypt !== 'undefined') {

                            var _menuHtml = '',
                                _data_crypt = JSON.parse(__functionalities.__base64_decode(response.data_crypt)),
                                _username = _data_crypt.username,
                                _letter = _username.charAt(0).toUpperCase();

                            // creamos el menu
                            if (typeof response.menu !== 'undefined') {
                                $.each(response.menu, function (k, v) {
                                    if (v.text.indexOf('separator') === -1) {
                                        _menuHtml +=
                                            '<li><a href="' + v.link + '">' +
                                                '<i class="' + v.icon + '" aria-hidden="true"></i>' + v.text +
                                            '</a></li>';
                                    } else {
                                        _menuHtml += '<li role="separator" class="divider"></li>';
                                    }
                                })
                            }

                            __content =
                                '<li class="user-in dropdown user-dropdown">' +
                                    '<button class="dropdown-toggle" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">' +
                                        '<svg width="19" height="19" viewBox="0 0 24 24"><path d="M2.984 21h13c.553 0 1-.447 1-1 0-2.99-1.763-5.569-4.3-6.773.925-.861 1.514-2.079 1.514-3.44 0-2.6-2.114-4.715-4.714-4.715S4.771 7.187 4.771 9.786c0 1.361.589 2.579 1.514 3.44-2.537 1.204-4.3 3.783-4.3 6.773C1.984 20.553 2.432 21 2.984 21zM15.797 11.789c-.39.386-.396 1.013-.015 1.406l.534.549C17.947 15.206 19 17.661 19 20v1h2v-1c0-2.863-1.229-5.768-3.171-7.58.494-.803.76-1.796.746-2.845-.022-1.63-.712-3.199-1.892-4.305l-1.367 1.459c.785.736 1.244 1.783 1.259 2.873C16.587 10.49 16.304 11.288 15.797 11.789z"/></svg>' +
                                    '</button>' +
                                    '<ul class="dropdown-menu" aria-labelledby="dropdownMenu1">' +
                                        '<div class="user-whois">' +
                                            '<i class="user-profile-pic user-' + _letter + '"></i>' +
                                            '<h3>' + _username + '</h3>' +
                                        '</div>' + _menuHtml +
                                    '</ul>' +
                                '</li>';
                        }
                    },
                    complete: function () {
                        // colocamos el boton en su sitio
                        $('ul.search li:eq(0)').before(__content);
                    }
                });
