<script type="text/javascript">
	function chgFntSz(id, size) {
		var currentSize = "";
		var elem = document.getElementById(id);
		if (elem.style.fontSize == "") {
			currentSize = 15;
		} else {
			currentSize = elem.style.fontSize.replace("px", "");
			currentSize = Number(currentSize);
		}
		elem.style.fontSize = currentSize + size + "px";
	}
	</script>
