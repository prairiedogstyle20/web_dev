	var s = d.createElement("script"); s.async = true;
	s.src = !("IntersectionObserver" in w) ? "https://www.texasobserver.org/wp-content/plugins/wp-rocket/inc/front/js/lazyload-8.15.2.min.js" : "https://www.texasobserver.org/wp-content/plugins/wp-rocket/inc/front/js/lazyload-10.17.min.js";
