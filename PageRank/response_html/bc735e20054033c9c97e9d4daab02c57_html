	        	jQuery(document).ready(function(e) {
			    jQuery('#_qf_Edit_next').click(function() {
			        var sEmail = jQuery('#email-Primary').val();
			        if (jQuery.trim(sEmail).length == 0) {
			            alert('Please enter valid email address');
			            return false;
			        }
			        if (!validateEmail(sEmail)) {
			            alert('Invalid Email Address');
			            return false;
			        }
			    });
				jQuery(".fancybox").fancybox();

				jQuery(".mapbox").fancybox({
					type: "iframe"
				});

				jQuery('div.page-left > div.upcomming-events').last().addClass('last');

				var numbers = jQuery('.covered-category > div.latest-single').length;
				var round = Math.ceil(numbers/2);
				
				for(var i = 0; i < round; i+=1) {
				  jQuery('.covered-category > div.latest-single').slice(0, i+(round)).wrapAll("<div class='all-issues-sec'></div>");
				}

				setTimeout(function() {
       				jQuery('div.issue-post-sec').first().addClass('left');
				}, 1000);
				setTimeout(function() {
       				jQuery('div.issue-post-sec').last().addClass('right');
				}, 1000);
				
