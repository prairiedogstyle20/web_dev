        <script>
	
	var adSeccion = $('#currentSection').val();
	var numP = 0;
	var numAds = 0;

	var ejecutarPauta = Config.variables.adblocker;

	function labelEnPublicidad(numArticle){
		setTimeout(function () {
			$('#principal-' + numArticle +  ' .publicidad').each(function(){
				$(this).prepend( "Publicidad" );
			});	
		}, 1000);
	}

	function createAdsSpot(numArticle){
		//Pauta cada 3 parrafos...
		var position = 2;

		//Limpiar parrafos...
		$('#principal-'+ numArticle +' p').each(function(){
		    if ($(this).html() == ""){
		        $(this).remove();
		    }
		});	

		if ($('#principal-'+ numArticle +' > p').length >= 4) {
			//Creamos el div del intext en el 4 parrafo...
			slotDiv = document.createElement('div');
			slotDiv.id = 'divIntext-'+numArticle; 
			//InsertAfter
			var nodo = $('#principal-'+ numArticle +' > p')[1].parentNode.insertBefore(slotDiv, $('#principal-'+ numArticle +' > p')[3].nextSibling);
		}

		
		//Create div container for ad
		for(var i= 1;i < numAds + 1; i++){
			
			if($('#principal-'+ numArticle +' > p')[position] != undefined){
				slotDiv = document.createElement('div');
				slotDiv.id = 'div-Ad-Body-'+numArticle+i;
				slotDiv.className = 'publicidad';
				//slotDiv.appendChild( document.createTextNode("Publicidad") );
				//InsertAfter
				nodo = $('#principal-'+ numArticle +' > p')[position].parentNode.insertBefore(slotDiv, $('#principal-'+ numArticle +' > p')[position].nextSibling);

			}
			
			position = position + 6;
		}
		
	}

	function mostrarAdInread(){

		if ( $('#principal-1 > p').length >= 4) {
			setTimeout(function () {
				//Se llamo la pauta correctamente ?? evita el recuadro negro si no hay pauta por DFP
				if ($('#Inread_Inhouse_1_1_1').css('display')  != 'none' ) {

					$('.INREAD_CONTAINER').show();
					var _window = $(window);
					_window.scroll(function(){
				        if( $('#Inread_Inhouse_1_1_1').length > 0){
							var scrolled = _window.scrollTop()- $(".INREAD_CONTAINER").offset().top + 60;
							$("#Inread_Inhouse_1_1_1").css({"top": scrolled+"px"});
						}
					});
				}
			}, 2000);
			
		}
	}

	function adInitDesktop(){

		var nid = $('#articleShareData1').data('article-id');

		googletag.cmd.push(function(){
			
			if(Config.variables.device == 'desktop'){
				gptadslots = googletag.defineSlot('/11420885/Article_Desktop_310_60_1', [310, 60], 'divIntext-0').setTargeting('seccion', adSeccion ).addService(googletag.pubads());
				var displayName = 'Intext_Desktop_1_1_1';
			}else{
				gptadslots = googletag.defineSlot('/11420885/Article_Mobile_310_60_1', [310, 60], 'divIntext-0').setTargeting('seccion', adSeccion ).addService(googletag.pubads());
				var displayName = 'Intext_Mobile_1_1_1';
			}

			googletag.pubads().collapseEmptyDivs();
			googletag.pubads().enableSingleRequest();
			googletag.pubads().disableInitialLoad();
			googletag.enableServices();
			googletag.pubads().refresh(gptadslots);
			googletag.display( displayName );
		});

	}

	function DFPAds(numArticle){

		var native = $('#articleShareData'+numArticle).data('article-native');
		var color = $('#articleShareData'+numArticle).data('article-color');
		var nid = $('#articleShareData'+numArticle).data('article-id');
		

		numP = $('#principal-'+numArticle).children('p').length;
		numAds = (numP - 2) / 6;
		
		//new divs
		createAdsSpot(numArticle);
		showAdsDevice(native, color, nid, numArticle);
		

	} //End function...


	
		function showAdsDevice(native, color, nid, numArticle){
			var gptadslots = [];

			googletag.cmd.push(function(){

				var firstAd = 'Article_Desktop_310_60_1';

									//Native ?
					if(native == 1){
						gptadslots[0] = googletag.defineSlot('/11420885/Native_Desktop_1280_370_1', [[1280, 370], [1280, 470]], 'divNative-'+numArticle).setTargeting('seccion', adSeccion ).setTargeting('nid', nid ).addService(googletag.pubads());
						$('#divNative-'+numArticle).css('background-color', color);
						richmedia = true;
						firstAd = 'Native_Desktop_1280_470_1';
					}else{
						gptadslots[0] = googletag.defineSlot('/11420885/Article_Desktop_970_250_1', [[728, 90], [970, 250]], 'divNative-'+numArticle).setTargeting('seccion', adSeccion ).setTargeting('nid', nid ).addService(googletag.pubads());
						firstAd = 'Article_Desktop_970_250_1';
					}

				

				
				/* Primera carga de pauta tradicional */
				gptadslots[1] = googletag.defineSlot('/11420885/Article_Desktop_310_60_1', [310, 60], 'Article_Desktop_310_60_'+numArticle).setTargeting('seccion', adSeccion ).setTargeting('nid', nid ).addService(googletag.pubads());
				

				//Inread
				if($('#inread').val() == 1 && index == 1){
					gptadslots[2] = googletag.defineSlot('/11420885/Inread_Inhouse_1_1_1', [1, 1], 'Inread_Inhouse_1_1_1').setTargeting('seccion', adSeccion ).setTargeting('nid', nid ).addService(googletag.pubads());
					$('.INREAD_CONTAINER').insertAfter($('#principal-1 > p')[3]);
				}
					
				googletag.pubads().collapseEmptyDivs();
				googletag.pubads().enableSingleRequest();
				googletag.pubads().disableInitialLoad();
				googletag.enableServices();
				googletag.pubads().refresh(gptadslots);
				googletag.display( firstAd );

				}); //End googletag.cmd.push
		}

		function loadSecondAds(numArticle){

			var gptadslots = [];
			var nid = $('#articleShareData'+numArticle).data('article-id');

			googletag.cmd.push(function(){
				gptadslots[2] = googletag.defineSlot('/11420885/Intext_Desktop_1_1_1', [1, 1], 'divIntext-'+numArticle).setTargeting('seccion', adSeccion ).setTargeting('nid', nid ).addService(googletag.pubads());

				/* Carga dinamica en el body */
				//Una pauta cada 3 parrafos...
				//Create div container for ad
				for(var i= 1;i < numAds+1;i++){
					gptadslots.push(googletag.defineSlot('/11420885/Article_Desktop_728_90_1', [728, 90], 'div-Ad-Body-'+numArticle+i).setTargeting('nid', nid ).setTargeting('seccion', adSeccion ).addService(googletag.pubads()));
				}
					
				googletag.pubads().collapseEmptyDivs();
				googletag.pubads().enableSingleRequest();
				googletag.pubads().disableInitialLoad();
				googletag.enableServices();
				googletag.pubads().refresh(gptadslots);
				googletag.display( 'Intext_Desktop_1_1_1' );

			});
				

		}


	
	
	//Ejecutamos para el artículo 1
	function mostrarAdsArticulo(numArticle){
		//Solo se muestra la pauta si el artículo no esta censurado
		if ($('#articleShareData'+numArticle).data('article-censorship') == 0 && $('#active_adblocker').val() == 0){

			DFPAds(numArticle);
			labelEnPublicidad(numArticle);

			if($('#inread').val() == 1 && numArticle == 1){
				mostrarAdInread();
			}
		}
	}

	//Inicializamos con un llamado de pauta falso...
	adInitDesktop();

</script>
