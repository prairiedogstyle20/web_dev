  <script type='text/javascript'>
    var url = 'https://www.redbubble.com/people/ozzyme/works/36982080-trump-happens/carousel';

    var loadCarousel = function () {
      RB.trackEvent('Work Page', 'on-demand-load', 'more-work');
      $(window).off('scroll', loadCarouselIfVisible);
      $('#load_carousel_link').off('click', loadCarousel);

      $.get(url, function(data, status) {
        $('#carousel-placeholder').replaceWith(data);
      });
    }

    var loadCarouselIfVisible = function () {
      if (isCarouselVisible($('#carousel-placeholder'))) {
        loadCarousel();
      }
    }

    if (isCarouselVisible($('#carousel-placeholder'))) {
      loadCarousel();
    } else {
      $(window).on('scroll', loadCarouselIfVisible);
      $('#load_carousel_link').on('click', loadCarousel);
    }
  </script>
