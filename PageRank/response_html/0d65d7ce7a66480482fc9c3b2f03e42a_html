<script>
    (function() {
        var base;
		var rulerDiv = $(".footer-ruler");
        var expandedDiv = rulerDiv.find(".expanded");
        var collapsedDiv = rulerDiv.find(".collapsed");

         var Ruler = {            
			settings: {
                "timeout": "10000",
				"interval": "3,7,10,12,15",
                "expandedImg": "https://www.themarker.com/st/media/Media18/HDC/ruler/22/970x150.jpg",
                "collapsedImg": "https://www.themarker.com/st/media/Media18/HDC/ruler/22/970x60.jpg",
				"link": "http://pubads.g.doubleclick.net/gampad/clk?id=4901524075&iu=/9401/Haaretz.com.ClickTracker/Haaretz.com.ClickTracker.Poster_HomePage "
            },
            init: function() {
                base = this;
				 $(".close--ruler").click(function() {
					base.collapsed();
          			sendGAevent('Ruler_Subscription','Close','');	
				});
				$(".close--ruler").hover(function() {
					$(".close--ruler").css('transform', 'rotate(180deg)').css('transition', 'transform 1s');
				});
				
        		$(".footer-ruler").on('click','a',function () {
					sendGAevent('Ruler_Subscription','Click','');											
				});		
				
				rulerDiv.show();
                base.expanded();
              	setTimeout(base.collapsed, parseInt(base.settings.timeout));
            },
            expanded: function() {
                (rulerDiv[0]).style.height = "150px";
                (expandedDiv[0]).style.height = "150px";
                expandedDiv.html('<a href="'+base.settings.link +'"><img alt = "promotional content" title = "רכוש מנוי הארץ" src="' + base.settings.expandedImg + '"/></a>');
            },
            collapsed: function() {
                (expandedDiv[0]).style.height = "0px";
                (collapsedDiv[0]).style.height = "60px";
                (rulerDiv[0]).style.height = "60px";
                $(".close--ruler").hide();
                collapsedDiv.html('<a href="'+base.settings.link +'"><img alt = "promotional content" title = "רכוש מנוי הארץ" src="' + base.settings.collapsedImg + '"/></a>');
        
        		var rullerEvent = new Event('rullerCollapsed');
				(rulerDiv[0]).dispatchEvent(rullerEvent);
            },
			shouldbeShownToday: function() {							
				var today = (new Date());
				today.setHours(0);
                today.setMinutes(0);
                today.setSeconds(0);
				if (!localStorage.dayInterval){
					var dateTimeInterval = [];
					var dayInterval = Ruler.settings.interval.split(",");	
					$.each(dayInterval, function( index, value ) {
						var nextIntervalDay = new Date();
						nextIntervalDay.setHours(0);
						nextIntervalDay.setMinutes(0);
						nextIntervalDay.setSeconds(0);
						var nextDay = (nextIntervalDay).setDate(today.getDate() + parseInt(value));
						dateTimeInterval.push(nextDay);
					});
					localStorage.dayInterval = JSON.stringify(dateTimeInterval); 
					return true;
				} else {
					var dateTimeInterval =  JSON.parse(localStorage.dayInterval)
					if (dateTimeInterval.length > 0){
						var nextDateTimeInterval = dateTimeInterval[0];
						if (nextDateTimeInterval <= today){
							dateTimeInterval.shift();
							localStorage.dayInterval = JSON.stringify(dateTimeInterval);
							return true;
						}
					} else{
						var nextIntervalDay = new Date();
						nextIntervalDay.setHours(0);
						nextIntervalDay.setMinutes(0);
						nextIntervalDay.setSeconds(0);
						dateTimeInterval.push((nextIntervalDay).setDate(today.getDate() + 1));
						localStorage.dayInterval = JSON.stringify(dateTimeInterval);
						return false;
					}
					return false;
				}
			}
		}
		
        $(document).ready(function() {
			if (UserUtil.getLoginInfo().userType != 'paying' && Ruler.shouldbeShownToday()) {
				Ruler.init();
			}else{
      								var event = new Event('noRulerDetected');
									window.dispatchEvent(event);
                }

        });
    })();
</script>

					      
                	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		    	    

          
        	    		                
