<script>
/*!
 * Open external URLs in a new tab
 */
(function($, w, undefined){
	$('.post .entry-content a').on('click', function(e){
		if (
			this.hostname == w.location.hostname ||
			this.target == '_blank'
		) {
			return;
		}
		e.preventDefault();
		e.stopPropagation();
		window.open(this.href, '_blank');
	});
}(jQuery, window));
</script>

