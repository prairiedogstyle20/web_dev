      <li class="dm-tab bdrgr5 ow-igr2-bx cnr-bdr xogr2 link-gr5ox">
        <span class="xogr5 tl"></span>
        <span class="xogr5 tr"></span>
        <a href="#">Best rated</a>
      </li>
      <li class="dm-tab bdrgr5 ow-igr2-bx cnr-bdr xogr2 link-gr5ox last">
        <span class="xogr5 tl"></span>
        <span class="xogr5 tr"></span>
        <a href="#">Worst rated</a>
      </li>
    </ul>

    <script type="text/javascript">
     DM.later('bundle', function (){
       Ext.each(Ext.get('rc-tabs').select('li.dm-tab').elements,
		function(el){
           Ext.get(el).on('click', function(e){ window.RCdefferedTab = e;
						e.preventDefault(); });
         });
     });
    </script>

