       var mapping = AdBridg.sizeMapping().
           // Mobile phone runs 300x250
           addSize([320, 400], [[300, 250]]).
           // Horizontal Phone
            addSize([667, 375], [[300, 250]]).
           // Vertical tablet
            addSize([600, 900], [[300, 250]]).
           // Landscape tablet
            addSize([768, 200], [[300, 250]]).
           // Desktop
           addSize([1000, 200], [[300, 250]]).build();
       var slot = AdBridg.defineSlot('/25722649/Politifact/Politifact_MiddleRectangle', [320, 50], 'ad-middlerect')
                         .defineSizeMapping(mapping);
       window.gptAdSlots.push(slot);
       AdBridg.useSizeMapping(slot, mapping);
       AdBridg.display('ad-middlerect');
       AdBridg.serve();
     });
   </script>
</div>
</div>
</div>
