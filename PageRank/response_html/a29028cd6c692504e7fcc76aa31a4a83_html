<script>
    var gdpr_banner = $('.cookie-notice-wrapper');

    var gdpr_cookie = false;
    
    var gdpr_cookie_expiration_date = new Date();
    gdpr_cookie_expiration_date.setMonth(gdpr_cookie_expiration_date.getMonth() + 120);

    var initialScrollEvent = true;

    document.cookie.split(';').filter(function(item){
        item.trim();
        if(item.indexOf("gdpr=1") >= 0){
            gdpr_banner.hide();
            gdpr_cookie = true;
        }
    });

    $(window).on('load', function(e){
        if(!gdpr_cookie){
            $(this).on('scroll', function(e){
                if(initialScrollEvent){
                    initialScrollEvent = false;
                } else {
                    document.cookie = "gdpr=1;path=/;expires=" + gdpr_cookie_expiration_date.toGMTString();
                    gdpr_banner.slideUp(500);
                }
            });
        }
    });

    $('#cookie-notice-close-button').on('click', function(e){
        document.cookie = "gdpr=1;path=/;expires=" + gdpr_cookie_expiration_date.toGMTString();
        gdpr_banner.slideUp(500);
    });
    $('.cookie-notice-content a').on('click', function(e){
        e.preventDefault();
        document.cookie = "gdpr=1;path=/;expires=" + gdpr_cookie_expiration_date.toGMTString();
        window.location = this.href;
    });
</script>
