     AdBridg.cmd.push(function() {
       var mapping = AdBridg.sizeMapping().
           // Accepts common mobile banner formats
            addSize([320, 400], [[320, 50]]).
           // Horizontal Phone
            addSize([667, 375], [[320,50]]).
           // Vertical tablet
