            dq.push(d);
            if (es) {
              dq.process();
            }
          }
          AdBridg.refresh = function(s, o) {
            googletag.pubads().refresh(s, o);
          }
          AdBridg.render = function() {}
          AdBridg.serve = function() {
            googletag.enableServices();
            es = 1;
            dq.process();
          }
          AdBridg.sizeMapping = function() {
            return googletag.sizeMapping();
          }
          AdBridg.useSizeMapping = function() {}
          while (AdBridg.cmd.length) {
            googletag.cmd.push(AdBridg.cmd.shift());
          }
          AdBridg.cmd = googletag.cmd;
