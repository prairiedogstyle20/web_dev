<script type="text/javascript">

	if (($('.sponsor-byline')[0]) && (window.innerWidth > 1166)) {
	
		console.log('sponsor byline exists');
		$stick = $('.sponsor-byline');
		var height = $stick.height();
		console.log('height: ', height);
		var top = $stick.offset().top
		var offset = height + top;
	
		jQuery(function($) {
		  function fixDiv() {
		   //console.log('width: ', window.innerWidth);
			var $cache = $('.sticky-ad-wrap-SP');
			if ($(window).scrollTop() > $stick.offset().top + height + 20)
			  $cache.css({
				'position': 'fixed',
				'top': '10px'
			  });
			else
			  $cache.css({
				'position': 'relative',
				'top': 'auto'
			  });
		  }
		  $(window).scroll(fixDiv);
		  fixDiv();
		});
	}
	
	
</script>





