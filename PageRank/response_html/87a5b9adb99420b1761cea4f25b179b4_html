		var slot01 = googletag.defineSlot('/38749147/BR-TopLeaderboard', sizetopleaderboard, 'div-gpt-ad-' + gadsgenerateId + '-01').addService(googletag.pubads());	
		var slot02 = googletag.defineSlot('/38749147/BR-BottomLeaderboard', sizebottomleaderboard, 'div-gpt-ad-' + gadsgenerateId + '-02').addService(googletag.pubads());
