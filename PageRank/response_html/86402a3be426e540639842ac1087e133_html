<script>
    $(document).foundation();

    $(document).ready(function () {
        $('#search-input').attr('placeholder', 'Introduce el texto a buscar');
        $('#newsletter-input').attr('placeholder', 'Introduce tu email');
        $('#start_date').attr('placeholder', 'Desde');
        $('#date_end').attr('placeholder', 'Hasta');
        $('.search-keys').attr('placeholder', 'Introduce el texto a buscar');
    });

    var origin = window.location.origin;
    toastr.options.closeButton = true;
    //toastr.options.escapeHtml = true;
    if ($('#alert-toastr').length && origin != "http://metalocus.test") {
        toastr.warning($('#alert-toastr').html());
    }

    if (origin == "http://metalocus.test") {
        $('#alert-toastr').removeClass('is-hidden');
    }

</script>

