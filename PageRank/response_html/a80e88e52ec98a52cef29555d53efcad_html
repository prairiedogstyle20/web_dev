            window.bbcpage = {
                loadModule: function(deps) {
                    return new Promise(function (resolve, reject) {
                        window.require(deps, function () {
                            resolve.apply(this, arguments);
                        },
                        function (error) {
                            reject(error);
                        });
                    })
