                $('#block-1493796 .owl-carousel').owlCarousel({
                    loop: $('#block-1493796 .owl-carousel .item').length > 1 ? true:false,
                    margin:15,
                    nav:false,
                    dots:false,
                    slideBy: 'page',
                    autoHeight:false,
                    
                        autoplay: false,
                    responsive:{
                        0:{
                            items:1,
                            stagePadding: 40,
                            nav:false
                        },
                        600:{
                            items:2
                        },
                        1000:{
                            items:1
                        }
                    }
                });

                // load instance data
                window.card_carousel_1493796 = $('#block-1493796 .owl-carousel').data('owlCarousel');
                
                // bind previous click
                $('#block-1493796 .owl-carousel-control.prev').on(__tnt.client.clickEvent, function(e){
                    e.stopPropagation();
                    card_carousel_1493796.prev();
                });
                // bind next click
                $('#block-1493796 .owl-carousel-control.next').on(__tnt.client.clickEvent, function(e){
                    e.stopPropagation();
                    card_carousel_1493796.next();
                });
                $('#block-1493796 .owl-controls').remove();
                
                

            });
