    <link itemprop="mainEntityOfPage" href="https://www.wacotrib.com/sports/high_schools/central_texas_girls_basketball/high-school-basketball-playoff-pairings/article_4e7b3762-9141-5a5e-962d-3f0fde29fe9c.html" />
    <style>
                .asset-paging .direction {
                    color: #86002b;
                }
            </style><div class="asset-paging-container hidden-print">
            <div id="asset-paging-4e7b3762-9141-5a5e-962d-3f0fde29fe9c" class="asset-paging">
                <div id="paging-prev-4e7b3762-9141-5a5e-962d-3f0fde29fe9c" class="paging-link prev offset"></div>
                <div id="paging-next-4e7b3762-9141-5a5e-962d-3f0fde29fe9c" class="paging-link next offset"></div>
