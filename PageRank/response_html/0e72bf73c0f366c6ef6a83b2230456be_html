                    function addAdobeTestAndTarget() {
                        var d = document, sn = 'script', f = d.getElementsByTagName(sn)[0]; if (!f) f = d.head;
                        var s = d.createElement(sn);
                        s.async = true;
                        f.parentNode.insertBefore(s, f);
