function send2SNS ( media )
{
    var url = "" ;
    switch(media)
    {
        case "facebook":
            url = "http://www.facebook.com/sharer.php?u=http%3A%2F%2Fsports.donga.com%2F3%2Fall%2F20190123%2F93829781%2F1" ;
            break;
        case "twitter":
            url = "http://twitter.com/share?text=%EC%BC%80%EC%9D%B4%EB%B8%94%C2%B7%EC%A2%85%ED%8E%B8%20%EB%88%88%EC%B9%98%20%EB%B3%B4%EB%8A%94%20%EC%A7%80%EC%83%81%ED%8C%8C%20%3A%20%EC%8A%A4%ED%8F%AC%EC%B8%A0%EB%8F%99%EC%95%84&url=http%3A%2F%2Fsports.donga.com%2F3%2Fall%2F20190123%2F93829781%2F1" ;
            break;
        case "naver":
            url = "http://bookmark.naver.com/post?ns=1&title=%EC%BC%80%EC%9D%B4%EB%B8%94%C2%B7%EC%A2%85%ED%8E%B8%20%EB%88%88%EC%B9%98%20%EB%B3%B4%EB%8A%94%20%EC%A7%80%EC%83%81%ED%8C%8C%20%3A%20%EC%8A%A4%ED%8F%AC%EC%B8%A0%EB%8F%99%EC%95%84&url=http%3A%2F%2Fsports.donga.com%2F3%2Fall%2F20190123%2F93829781%2F1" ;
            break;
        case "cyworld":
            url = "http://csp.cyworld.com/bi/bi_recommend_pop.php?url=http%3A%2F%2Fsports.donga.com%2F3%2Fall%2F20190123%2F93829781%2F1&title_nobase64=%EC%BC%80%EC%9D%B4%EB%B8%94%C2%B7%EC%A2%85%ED%8E%B8%20%EB%88%88%EC%B9%98%20%EB%B3%B4%EB%8A%94%20%EC%A7%80%EC%83%81%ED%8C%8C%20%3A%20%EC%8A%A4%ED%8F%AC%EC%B8%A0%EB%8F%99%EC%95%84&thumbnail=&writer=&corpid=65816299";
            break;
        case "google":
            url = "http://www.google.com/bookmarks/mark?op=add&title=%EC%BC%80%EC%9D%B4%EB%B8%94%C2%B7%EC%A2%85%ED%8E%B8%20%EB%88%88%EC%B9%98%20%EB%B3%B4%EB%8A%94%20%EC%A7%80%EC%83%81%ED%8C%8C%20%3A%20%EC%8A%A4%ED%8F%AC%EC%B8%A0%EB%8F%99%EC%95%84&bkmk=http%3A%2F%2Fsports.donga.com%2F3%2Fall%2F20190123%2F93829781%2F1" ;
            break;
    }
    if ( url != "" )
    {
        openWindow ( url, "_SNS", 860, 500 ) ;
    }
    return false;
}

var fontSize = 2 ;
function callCSset(key, val)
{
    var targetUrl = "/HUB/common/cs.php?s="+key+"&u="+val ;
    $.ajax( {
        type: "GET",
        url: targetUrl,
        dataType: "html",
        success: function (result) {
        }
    });
}
function changeFontSize ()
{
    if ( fontSize == 0 )
    {
        alert ('최소 폰트 사이즈입니다.') ;
        fontSize = 1 ;
        return false;
    }
    else if ( fontSize == 5 )
    {
        alert ('최대 폰트 사이즈입니다.') ;
        fontSize = 4 ;
        return false;
    }

    if ( fontSize == 1 )
    {
        $('#article_body').css({'font-size':'14px'});
    }
    else if ( fontSize == 3 )
    {
        $('#article_body').css({'font-size':'20px'});
    }
    else if ( fontSize == 4 )
    {
        $('#article_body').css({'font-size':'24px'});
    }
    else
    {
        $('#article_body').css({'font-size':'16px'});
    }

    callCSset('pfs', fontSize) ;
    return false;
}
function getNewsLikeCount()
{
    $.getJSON ( "http://spintop.donga.com/comment?jsoncallback=?", {
        m: "newsinfo" ,
        p3: "sports.donga.com" ,
        p4: "93829781"
    })
    .done ( function( result ) {
        var data = $.parseJSON(result);
        if (data.DATA) { $('#likeCnt').html( data.DATA.ref0 ) ; }
        else { $('#likeCnt').html( 0 ) ; }
    })
    .error ( function( result ) {
    });
}

function rCnt()
{
    $("#replyCnt").html();

    return false;
}

function copyUrl(id)
{
    var tag = $('#'+id).val() ;
    $('#'+id).val(tag).select() ;
    var succeed;
    try {
        succeed = document.execCommand("copy");
    } catch(e) {
        succeed = false;
    }

    if (succeed) {
        $('#'+id+'Txt').show();
        $('#'+id+'Txt').delay(1000).fadeOut('fast');
    }

    return false;
}

$(document).ready( function() {
    getNewsLikeCount() ;
} ) ;


</script>        <script>
            function replyMove(){
                var offset = $("#spinTopLayer").offset();
                $('html, body').animate({scrollTop : offset.top - 240}, 400);
            }

            $(function(){
                var munuOffset = $( '.article_tit' ).offset();
                $( window ).scroll( function() {
                    if ( $( document ).scrollTop() > munuOffset.top ) {
                        $( '#sub_header' ).addClass( 'fix' );
                    }
                    else {
                        $( '#sub_header' ).removeClass( 'fix' );
                    }
                });
            });

            function getNewsCount()
            {
                $.getJSON ( "http://spintop.donga.com/comment?jsoncallback=?", {
                    m: "count" ,
                    p3: "sports.donga.com" ,
                    p4: "93829781"
                })
                .done ( function( result ) {
                    if ( result == 0 )
                    {
                        $('#replyCnt1').addClass('no');
                    }
                    else
                    {
                        $('#replyCnt1').removeClass('no');
                        $('#replyCnt1').html( result ) ;
                    }
                })
                .error ( function( result ) {
                });
            }

            $(document).ready( function() {
                $("#header").after("<div id='sub_header'><div class='sub_headerbox'><span class='icon_comm btn_allmenu' onClick='javascript:return allmenuPopupObj.show();'>전체메뉴보기</span><div class='next_con'><span class='next'>NEXT</span><a href='http://sports.donga.com/Feed/3/all/20190226/94290876/1' class='next_tit'>이장희, 대마초 파동으로…경악할 행보</a></div><div class='sns_con'><a href='#' class='num' id='replyCnt1' onclick='replyMove();return false;' ></a><a href='#' class='icon_comm btn_facebook' onclick=\"javascript:return send2SNS('facebook');return false;\"><span>페이스북</span></a><a href='#' class='icon_comm btn_twitter' onclick=\"javascript:return send2SNS('twitter');return false;\"><span>트위터</span></a></div></div></div>") ;
                getNewsCount() ;
            } ) ;

