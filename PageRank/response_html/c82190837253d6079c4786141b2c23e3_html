        <script>
            $(document).ready(function () {
                var blockCount = $('#list_da li').length;
                var blockWidth = $('#list_da li').width();
                var blockHeight = $('#list_da li').height();
                var blockUlWidth = blockCount * blockWidth;
                
                $('#report_da').css({ height: blockHeight });
                $('#report_da .element').css({ width: blockUlWidth, marginLeft: - blockWidth });
                $('#list_da li:last-child').prependTo('#list_da');

                function movePrev() {
                    $('#report_da .element').animate({
                        left: + blockWidth
                    }, 300, function () {
                        $('#list_da li:last-child').prependTo('#list_da');
                        $('#report_da .element').css('left', '');
                    });
                };
                function moveNext() {
                    $('#report_da .element').animate({
                        left: - blockWidth
                    }, 300, function () {
                        $('#list_da li:first-child').appendTo('#list_da');
                        $('#report_da .element').css('left', '');
                    });
                };

                $('.report_spc .btn_prev').click(function () {
                    movePrev();
                });
                $('.report_spc .btn_next').click(function () {
                    moveNext();
                });
            });    
        </script>        <div class='news_stand'>
