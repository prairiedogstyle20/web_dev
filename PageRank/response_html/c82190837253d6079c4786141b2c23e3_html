            <script>
                $(document).ready(function () {
                    var slideCount = $('#photo_slide .element .pannel').length;
                    var slideWidth = $('#photo_slide .element .pannel').width();
                    var slideHeight = $('#photo_slide .element .pannel').height();
                    var sliderUlWidth = slideCount * slideWidth;
                    
                    $('#photo_slide').css({ width: slideWidth, height: slideHeight });
                    $('#photo_slide .element').css({ width: sliderUlWidth, marginLeft: - slideWidth });
                    $('#photo_slide .element .pannel:last-child').prependTo('#photo_slide .element');

                    function moveLeft() {
                        $('#photo_slide .element').animate({
                            left: + slideWidth
                        }, 300, function () {
                            $('#photo_slide .element .pannel:last-child').prependTo('#photo_slide .element');
                            $('#photo_slide .element').css('left', '');
                        });
                    };
                    function moveRight() {
                        $('#photo_slide .element').animate({
                            left: - slideWidth
                        }, 300, function () {
                            $('#photo_slide .element .pannel:first-child').appendTo('#photo_slide .element');
                            $('#photo_slide .element').css('left', '');
                        });
                    };

                    $('.photo_cont .btn_prev').click(function () {
                        moveLeft();
                    });
                    $('.photo_cont .btn_next').click(function () {
                        moveRight();
                    });
                });    
            </script><!-- 타불라 - 스포츠 -->
